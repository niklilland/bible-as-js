[{"chapter":10,"text":"The proverbs of Solomon. A wise son maketh a glad father: but a foolish son [is] the heaviness of his mother.","verse":1,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H04912","text":"proverbs"},{"index":2,"strongs":0,"text":"of"},{"index":3,"strongs":"H08010","text":"Solomon."},{"index":4,"strongs":0,"text":"A"},{"index":5,"strongs":"H02450","text":"wise"},{"index":6,"strongs":"H01121","text":"son"},{"index":7,"strongs":0,"text":"maketh a"},{"index":8,"strongs":"H08055","text":"glad"},{"index":9,"strongs":"H0001","text":"father:"},{"index":10,"strongs":0,"text":"but a"},{"index":11,"strongs":"H03684","text":"foolish"},{"index":12,"strongs":"H01121","text":"son"},{"index":13,"strongs":0,"text":"is the"},{"index":14,"strongs":"H08424","text":"heaviness"},{"index":15,"strongs":0,"text":"of his"},{"index":16,"strongs":"H0517","text":"mother."}]},{"chapter":10,"text":"Treasures of wickedness profit nothing: but righteousness delivereth from death.","verse":2,"book":"Proverbs","reference":[{"index":0,"strongs":"H0214","text":"Treasures"},{"index":1,"strongs":0,"text":"of"},{"index":2,"strongs":"H07562","text":"wickedness"},{"index":3,"strongs":"H03276","text":"profit"},{"index":4,"strongs":0,"text":"nothing: but"},{"index":5,"strongs":"H06666","text":"righteousness"},{"index":6,"strongs":"H05337","text":"delivereth"},{"index":7,"strongs":0,"text":"from"},{"index":8,"strongs":"H04194","text":"death."}]},{"chapter":10,"text":"The LORD will not suffer the soul of the righteous to famish: but he casteth away the substance of the wicked.","verse":3,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H03068","text":"LORD"},{"index":2,"strongs":0,"text":"will not"},{"index":3,"strongs":"H07456","text":"suffer"},{"index":4,"strongs":0,"text":"the"},{"index":5,"strongs":"H05315","text":"soul"},{"index":6,"strongs":0,"text":"of the"},{"index":7,"strongs":"H06662","text":"righteous"},{"index":8,"strongs":0,"text":"to"},{"index":9,"strongs":"H07456","text":"famish:"},{"index":10,"strongs":0,"text":"but he casteth"},{"index":11,"strongs":"H01920","text":"away"},{"index":12,"strongs":0,"text":"the"},{"index":13,"strongs":"H01942","text":"substance"},{"index":14,"strongs":0,"text":"of the"},{"index":15,"strongs":"H07563","text":"wicked."}]},{"chapter":10,"text":"He becometh poor that dealeth [with] a slack hand: but the hand of the diligent maketh rich.","verse":4,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"He becometh "},{"index":1,"strongs":"H07326","text":"poor"},{"index":2,"strongs":0,"text":"that"},{"index":3,"strongs":"H06213","text":"dealeth"},{"index":4,"strongs":0,"text":"with a"},{"index":5,"strongs":"H07423","text":"slack"},{"index":6,"strongs":"H03709","text":"hand:"},{"index":7,"strongs":0,"text":"but the"},{"index":8,"strongs":"H03027","text":"hand"},{"index":9,"strongs":0,"text":"of the"},{"index":10,"strongs":"H02742","text":"diligent"},{"index":11,"strongs":0,"text":"maketh"},{"index":12,"strongs":"H06238","text":"rich."}]},{"chapter":10,"text":"He that gathereth in summer [is] a wise son: [but] he that sleepeth in harvest [is] a son that causeth shame.","verse":5,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"He that "},{"index":1,"strongs":"H0103","text":"gathereth"},{"index":2,"strongs":0,"text":"in"},{"index":3,"strongs":"H07019","text":"summer"},{"index":4,"strongs":0,"text":"is a"},{"index":5,"strongs":"H07919","text":"wise"},{"index":6,"strongs":"H01121","text":"son:"},{"index":7,"strongs":0,"text":"but he that"},{"index":8,"strongs":"H07290","text":"sleepeth"},{"index":9,"strongs":0,"text":"in"},{"index":10,"strongs":"H07105","text":"harvest"},{"index":11,"strongs":0,"text":"is a"},{"index":12,"strongs":"H01121","text":"son"},{"index":13,"strongs":0,"text":"that causeth"},{"index":14,"strongs":"H0954","text":"shame."}]},{"chapter":10,"text":"Blessings [are] upon the head of the just: but violence covereth the mouth of the wicked.","verse":6,"book":"Proverbs","reference":[{"index":0,"strongs":"H01293","text":"Blessings"},{"index":1,"strongs":0,"text":"are upon the"},{"index":2,"strongs":"H07218","text":"head"},{"index":3,"strongs":0,"text":"of the"},{"index":4,"strongs":"H06662","text":"just:"},{"index":5,"strongs":0,"text":"but"},{"index":6,"strongs":"H02555","text":"violence"},{"index":7,"strongs":"H03680","text":"covereth"},{"index":8,"strongs":0,"text":"the"},{"index":9,"strongs":"H06310","text":"mouth"},{"index":10,"strongs":0,"text":"of the"},{"index":11,"strongs":"H07563","text":"wicked."}]},{"chapter":10,"text":"The memory of the just is blessed: but the name of the wicked shall rot.","verse":7,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H02143","text":"memory"},{"index":2,"strongs":0,"text":"of the"},{"index":3,"strongs":"H06662","text":"just"},{"index":4,"strongs":0,"text":"is"},{"index":5,"strongs":"H01293","text":"blessed:"},{"index":6,"strongs":0,"text":"but the"},{"index":7,"strongs":"H08034","text":"name"},{"index":8,"strongs":0,"text":"of the"},{"index":9,"strongs":"H07563","text":"wicked"},{"index":10,"strongs":0,"text":"shall"},{"index":11,"strongs":"H07537","text":"rot."}]},{"chapter":10,"text":"The wise in heart will receive commandments: but a prating fool shall fall.","verse":8,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H02450","text":"wise"},{"index":2,"strongs":0,"text":"in"},{"index":3,"strongs":"H03820","text":"heart"},{"index":4,"strongs":0,"text":"will"},{"index":5,"strongs":"H03947","text":"receive"},{"index":6,"strongs":"H04687","text":"commandments:"},{"index":7,"strongs":0,"text":"but a"},{"index":8,"strongs":"H08193","text":"prating"},{"index":9,"strongs":"H0191","text":"fool"},{"index":10,"strongs":0,"text":"shall"},{"index":11,"strongs":"H03832","text":"fall."}]},{"chapter":10,"text":"He that walketh uprightly walketh surely: but he that perverteth his ways shall be known.","verse":9,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"He that "},{"index":1,"strongs":"H03212","text":"walketh"},{"index":2,"strongs":"H08537","text":"uprightly"},{"index":3,"strongs":"H01980","text":"walketh"},{"index":4,"strongs":"H0983","text":"surely:"},{"index":5,"strongs":0,"text":"but he that"},{"index":6,"strongs":"H06140","text":"perverteth"},{"index":7,"strongs":0,"text":"his"},{"index":8,"strongs":"H01870","text":"ways"},{"index":9,"strongs":0,"text":"shall be"},{"index":10,"strongs":"H03045","text":"known."}]},{"chapter":10,"text":"He that winketh with the eye causeth sorrow: but a prating fool shall fall.","verse":10,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"He that "},{"index":1,"strongs":"H07169","text":"winketh"},{"index":2,"strongs":0,"text":"with the"},{"index":3,"strongs":"H05869","text":"eye"},{"index":4,"strongs":"H05414","text":"causeth"},{"index":5,"strongs":"H06094","text":"sorrow:"},{"index":6,"strongs":0,"text":"but a"},{"index":7,"strongs":"H08193","text":"prating"},{"index":8,"strongs":"H0191","text":"fool"},{"index":9,"strongs":0,"text":"shall"},{"index":10,"strongs":"H03832","text":"fall."}]},{"chapter":10,"text":"The mouth of a righteous [man is] a well of life: but violence covereth the mouth of the wicked.","verse":11,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H06310","text":"mouth"},{"index":2,"strongs":0,"text":"of a"},{"index":3,"strongs":"H06662","text":"righteous"},{"index":4,"strongs":0,"text":"man is a"},{"index":5,"strongs":"H04726","text":"well"},{"index":6,"strongs":0,"text":"of"},{"index":7,"strongs":"H02416","text":"life:"},{"index":8,"strongs":0,"text":"but"},{"index":9,"strongs":"H02555","text":"violence"},{"index":10,"strongs":"H03680","text":"covereth"},{"index":11,"strongs":0,"text":"the"},{"index":12,"strongs":"H06310","text":"mouth"},{"index":13,"strongs":0,"text":"of the"},{"index":14,"strongs":"H07563","text":"wicked."}]},{"chapter":10,"text":"Hatred stirreth up strifes: but love covereth all sins.","verse":12,"book":"Proverbs","reference":[{"index":0,"strongs":"H08135","text":"Hatred"},{"index":1,"strongs":0,"text":"stirreth"},{"index":2,"strongs":"H05782","text":"up"},{"index":3,"strongs":"H04090","text":"strifes:"},{"index":4,"strongs":0,"text":"but"},{"index":5,"strongs":"H0160","text":"love"},{"index":6,"strongs":"H03680","text":"covereth"},{"index":7,"strongs":0,"text":"all"},{"index":8,"strongs":"H06588","text":"sins."}]},{"chapter":10,"text":"In the lips of him that hath understanding wisdom is found: but a rod [is] for the back of him that is void of understanding.","verse":13,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"In the "},{"index":1,"strongs":"H08193","text":"lips"},{"index":2,"strongs":0,"text":"of him that hath"},{"index":3,"strongs":"H0995","text":"understanding"},{"index":4,"strongs":"H02451","text":"wisdom"},{"index":5,"strongs":0,"text":"is"},{"index":6,"strongs":"H04672","text":"found:"},{"index":7,"strongs":0,"text":"but a"},{"index":8,"strongs":"H07626","text":"rod"},{"index":9,"strongs":0,"text":"is for the"},{"index":10,"strongs":"H01460","text":"back"},{"index":11,"strongs":0,"text":"of him that is"},{"index":12,"strongs":"H02638","text":"void"},{"index":13,"strongs":0,"text":"of"},{"index":14,"strongs":"H03820","text":"understanding."}]},{"chapter":10,"text":"Wise [men] lay up knowledge: but the mouth of the foolish [is] near destruction.","verse":14,"book":"Proverbs","reference":[{"index":0,"strongs":"H02450","text":"Wise"},{"index":1,"strongs":0,"text":"men lay"},{"index":2,"strongs":"H06845","text":"up"},{"index":3,"strongs":"H01847","text":"knowledge:"},{"index":4,"strongs":0,"text":"but the"},{"index":5,"strongs":"H06310","text":"mouth"},{"index":6,"strongs":0,"text":"of the"},{"index":7,"strongs":"H0191","text":"foolish"},{"index":8,"strongs":0,"text":"is"},{"index":9,"strongs":"H07138","text":"near"},{"index":10,"strongs":"H04288","text":"destruction."}]},{"chapter":10,"text":"The rich man's wealth [is] his strong city: the destruction of the poor [is] their poverty.","verse":15,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"The rich "},{"index":1,"strongs":"H06223","text":"man's"},{"index":2,"strongs":"H01952","text":"wealth"},{"index":3,"strongs":0,"text":"is his"},{"index":4,"strongs":"H05797","text":"strong"},{"index":5,"strongs":"H07151","text":"city:"},{"index":6,"strongs":0,"text":"the"},{"index":7,"strongs":"H04288","text":"destruction"},{"index":8,"strongs":0,"text":"of the"},{"index":9,"strongs":"H01800","text":"poor"},{"index":10,"strongs":0,"text":"is their"},{"index":11,"strongs":"H07389","text":"poverty."}]},{"chapter":10,"text":"The labour of the righteous [tendeth] to life: the fruit of the wicked to sin.","verse":16,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H06468","text":"labour"},{"index":2,"strongs":0,"text":"of the"},{"index":3,"strongs":"H06662","text":"righteous"},{"index":4,"strongs":0,"text":"tendeth to"},{"index":5,"strongs":"H02416","text":"life:"},{"index":6,"strongs":0,"text":"the"},{"index":7,"strongs":"H08393","text":"fruit"},{"index":8,"strongs":0,"text":"of the"},{"index":9,"strongs":"H07563","text":"wicked"},{"index":10,"strongs":0,"text":"to"},{"index":11,"strongs":"H02403","text":"sin."}]},{"chapter":10,"text":"He [is in] the way of life that keepeth instruction: but he that refuseth reproof erreth.","verse":17,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"He is in the "},{"index":1,"strongs":"H0734","text":"way"},{"index":2,"strongs":0,"text":"of"},{"index":3,"strongs":"H02416","text":"life"},{"index":4,"strongs":0,"text":"that"},{"index":5,"strongs":"H08104","text":"keepeth"},{"index":6,"strongs":"H04148","text":"instruction:"},{"index":7,"strongs":0,"text":"but he that"},{"index":8,"strongs":"H05800","text":"refuseth"},{"index":9,"strongs":"H08433","text":"reproof"},{"index":10,"strongs":"H08582","text":"erreth."}]},{"chapter":10,"text":"He that hideth hatred [with] lying lips, and he that uttereth a slander, [is] a fool.","verse":18,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"He that "},{"index":1,"strongs":"H03680","text":"hideth"},{"index":2,"strongs":"H08135","text":"hatred"},{"index":3,"strongs":0,"text":"with"},{"index":4,"strongs":"H08267","text":"lying"},{"index":5,"strongs":"H08193","text":"lips,"},{"index":6,"strongs":0,"text":"and he that"},{"index":7,"strongs":"H03318","text":"uttereth"},{"index":8,"strongs":0,"text":"a"},{"index":9,"strongs":"H01681","text":"slander,"},{"index":10,"strongs":0,"text":"is a"},{"index":11,"strongs":"H03684","text":"fool."}]},{"chapter":10,"text":"In the multitude of words there wanteth not sin: but he that refraineth his lips [is] wise.","verse":19,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"In the "},{"index":1,"strongs":"H07230","text":"multitude"},{"index":2,"strongs":0,"text":"of"},{"index":3,"strongs":"H01697","text":"words"},{"index":4,"strongs":0,"text":"there"},{"index":5,"strongs":"H02308","text":"wanteth"},{"index":6,"strongs":0,"text":"not"},{"index":7,"strongs":"H06588","text":"sin:"},{"index":8,"strongs":0,"text":"but he that"},{"index":9,"strongs":"H02820","text":"refraineth"},{"index":10,"strongs":0,"text":"his"},{"index":11,"strongs":"H08193","text":"lips"},{"index":12,"strongs":0,"text":"is"},{"index":13,"strongs":"H07919","text":"wise."}]},{"chapter":10,"text":"The tongue of the just [is as] choice silver: the heart of the wicked [is] little worth.","verse":20,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H03956","text":"tongue"},{"index":2,"strongs":0,"text":"of the"},{"index":3,"strongs":"H06662","text":"just"},{"index":4,"strongs":0,"text":"is as"},{"index":5,"strongs":"H0977","text":"choice"},{"index":6,"strongs":"H03701","text":"silver:"},{"index":7,"strongs":0,"text":"the"},{"index":8,"strongs":"H03820","text":"heart"},{"index":9,"strongs":0,"text":"of the"},{"index":10,"strongs":"H07563","text":"wicked"},{"index":11,"strongs":0,"text":"is little"},{"index":12,"strongs":"H04592","text":"worth."}]},{"chapter":10,"text":"The lips of the righteous feed many: but fools die for want of wisdom.","verse":21,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H08193","text":"lips"},{"index":2,"strongs":0,"text":"of the"},{"index":3,"strongs":"H06662","text":"righteous"},{"index":4,"strongs":"H07462","text":"feed"},{"index":5,"strongs":"H07227","text":"many:"},{"index":6,"strongs":0,"text":"but"},{"index":7,"strongs":"H0191","text":"fools"},{"index":8,"strongs":"H04191","text":"die"},{"index":9,"strongs":0,"text":"for"},{"index":10,"strongs":"H02638","text":"want"},{"index":11,"strongs":0,"text":"of"},{"index":12,"strongs":"H03820","text":"wisdom."}]},{"chapter":10,"text":"The blessing of the LORD, it maketh rich, and he addeth no sorrow with it.","verse":22,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H01293","text":"blessing"},{"index":2,"strongs":0,"text":"of the"},{"index":3,"strongs":"H03068","text":"LORD,"},{"index":4,"strongs":0,"text":"it maketh"},{"index":5,"strongs":"H06238","text":"rich,"},{"index":6,"strongs":0,"text":"and he"},{"index":7,"strongs":"H03254","text":"addeth"},{"index":8,"strongs":0,"text":"no"},{"index":9,"strongs":"H06089","text":"sorrow"},{"index":10,"strongs":0,"text":"with it."}]},{"chapter":10,"text":"[It is] as sport to a fool to do mischief: but a man of understanding hath wisdom.","verse":23,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"It is as "},{"index":1,"strongs":"H07814","text":"sport"},{"index":2,"strongs":0,"text":"to a"},{"index":3,"strongs":"H03684","text":"fool"},{"index":4,"strongs":0,"text":"to"},{"index":5,"strongs":"H06213","text":"do"},{"index":6,"strongs":"H02154","text":"mischief:"},{"index":7,"strongs":0,"text":"but a"},{"index":8,"strongs":"H0376","text":"man"},{"index":9,"strongs":0,"text":"of"},{"index":10,"strongs":"H08394","text":"understanding"},{"index":11,"strongs":0,"text":"hath"},{"index":12,"strongs":"H02451","text":"wisdom."}]},{"chapter":10,"text":"The fear of the wicked, it shall come upon him: but the desire of the righteous shall be granted.","verse":24,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H04034","text":"fear"},{"index":2,"strongs":0,"text":"of the"},{"index":3,"strongs":"H07563","text":"wicked,"},{"index":4,"strongs":0,"text":"it shall"},{"index":5,"strongs":"H0935","text":"come"},{"index":6,"strongs":0,"text":"upon him: but the"},{"index":7,"strongs":"H08378","text":"desire"},{"index":8,"strongs":0,"text":"of the"},{"index":9,"strongs":"H06662","text":"righteous"},{"index":10,"strongs":0,"text":"shall be"},{"index":11,"strongs":"H05414","text":"granted."}]},{"chapter":10,"text":"As the whirlwind passeth, so [is] the wicked no [more:] but the righteous [is] an everlasting foundation.","verse":25,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"As the "},{"index":1,"strongs":"H05492","text":"whirlwind"},{"index":2,"strongs":"H05674","text":"passeth,"},{"index":3,"strongs":0,"text":"so is the"},{"index":4,"strongs":"H07563","text":"wicked"},{"index":5,"strongs":0,"text":"no more: but the"},{"index":6,"strongs":"H06662","text":"righteous"},{"index":7,"strongs":0,"text":"is an"},{"index":8,"strongs":"H05769","text":"everlasting"},{"index":9,"strongs":"H03247","text":"foundation."}]},{"chapter":10,"text":"As vinegar to the teeth, and as smoke to the eyes, so [is] the sluggard to them that send him.","verse":26,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"As "},{"index":1,"strongs":"H02558","text":"vinegar"},{"index":2,"strongs":0,"text":"to the"},{"index":3,"strongs":"H08127","text":"teeth,"},{"index":4,"strongs":0,"text":"and as"},{"index":5,"strongs":"H06227","text":"smoke"},{"index":6,"strongs":0,"text":"to the"},{"index":7,"strongs":"H05869","text":"eyes,"},{"index":8,"strongs":0,"text":"so is the"},{"index":9,"strongs":"H06102","text":"sluggard"},{"index":10,"strongs":0,"text":"to them that"},{"index":11,"strongs":"H07971","text":"send"},{"index":12,"strongs":0,"text":"him."}]},{"chapter":10,"text":"The fear of the LORD prolongeth days: but the years of the wicked shall be shortened.","verse":27,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H03374","text":"fear"},{"index":2,"strongs":0,"text":"of the"},{"index":3,"strongs":"H03068","text":"LORD"},{"index":4,"strongs":"H03254","text":"prolongeth"},{"index":5,"strongs":"H03117","text":"days:"},{"index":6,"strongs":0,"text":"but the"},{"index":7,"strongs":"H08141","text":"years"},{"index":8,"strongs":0,"text":"of the"},{"index":9,"strongs":"H07563","text":"wicked"},{"index":10,"strongs":0,"text":"shall be"},{"index":11,"strongs":"H07114","text":"shortened."}]},{"chapter":10,"text":"The hope of the righteous [shall be] gladness: but the expectation of the wicked shall perish.","verse":28,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H08431","text":"hope"},{"index":2,"strongs":0,"text":"of the"},{"index":3,"strongs":"H06662","text":"righteous"},{"index":4,"strongs":0,"text":"shall be"},{"index":5,"strongs":"H08057","text":"gladness:"},{"index":6,"strongs":0,"text":"but the"},{"index":7,"strongs":"H08615","text":"expectation"},{"index":8,"strongs":0,"text":"of the"},{"index":9,"strongs":"H07563","text":"wicked"},{"index":10,"strongs":0,"text":"shall"},{"index":11,"strongs":"H0006","text":"perish."}]},{"chapter":10,"text":"The way of the LORD [is] strength to the upright: but destruction [shall be] to the workers of iniquity.","verse":29,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H01870","text":"way"},{"index":2,"strongs":0,"text":"of the"},{"index":3,"strongs":"H03068","text":"LORD"},{"index":4,"strongs":0,"text":"is"},{"index":5,"strongs":"H04581","text":"strength"},{"index":6,"strongs":0,"text":"to the"},{"index":7,"strongs":"H08537","text":"upright:"},{"index":8,"strongs":0,"text":"but"},{"index":9,"strongs":"H04288","text":"destruction"},{"index":10,"strongs":0,"text":"shall be to the"},{"index":11,"strongs":"H06466","text":"workers"},{"index":12,"strongs":0,"text":"of"},{"index":13,"strongs":"H0205","text":"iniquity."}]},{"chapter":10,"text":"The righteous shall never be removed: but the wicked shall not inhabit the earth.","verse":30,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H06662","text":"righteous"},{"index":2,"strongs":0,"text":"shall"},{"index":3,"strongs":"H05769","text":"never"},{"index":4,"strongs":0,"text":"be"},{"index":5,"strongs":"H04131","text":"removed:"},{"index":6,"strongs":0,"text":"but the"},{"index":7,"strongs":"H07563","text":"wicked"},{"index":8,"strongs":0,"text":"shall not"},{"index":9,"strongs":"H07931","text":"inhabit"},{"index":10,"strongs":0,"text":"the"},{"index":11,"strongs":"H0776","text":"earth."}]},{"chapter":10,"text":"The mouth of the just bringeth forth wisdom: but the froward tongue shall be cut out.","verse":31,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H06310","text":"mouth"},{"index":2,"strongs":0,"text":"of the"},{"index":3,"strongs":"H06662","text":"just"},{"index":4,"strongs":0,"text":"bringeth"},{"index":5,"strongs":"H05107","text":"forth"},{"index":6,"strongs":"H02451","text":"wisdom:"},{"index":7,"strongs":0,"text":"but the"},{"index":8,"strongs":"H08419","text":"froward"},{"index":9,"strongs":"H03956","text":"tongue"},{"index":10,"strongs":0,"text":"shall be cut"},{"index":11,"strongs":"H03772","text":"out."}]},{"chapter":10,"text":"The lips of the righteous know what is acceptable: but the mouth of the wicked [speaketh] frowardness.","verse":32,"book":"Proverbs","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H08193","text":"lips"},{"index":2,"strongs":0,"text":"of the"},{"index":3,"strongs":"H06662","text":"righteous"},{"index":4,"strongs":"H03045","text":"know"},{"index":5,"strongs":0,"text":"what is"},{"index":6,"strongs":"H07522","text":"acceptable:"},{"index":7,"strongs":0,"text":"but the"},{"index":8,"strongs":"H06310","text":"mouth"},{"index":9,"strongs":0,"text":"of the"},{"index":10,"strongs":"H07563","text":"wicked"},{"index":11,"strongs":0,"text":"speaketh"},{"index":12,"strongs":"H08419","text":"frowardness."}]}]