[{"chapter":49,"text":"Hear this, all [ye] people; give ear, all [ye] inhabitants of       the world:","verse":1,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Hear this, all "},{"index":1,"strongs":"H05329","text":"ye"},{"index":2,"strongs":0,"text":"people;"},{"index":3,"strongs":"H04210","text":"give"},{"index":4,"strongs":0,"text":"ear, all"},{"index":5,"strongs":"H01121","text":"ye"},{"index":6,"strongs":0,"text":"inhabitants"},{"index":7,"strongs":"H07141","text":"of"},{"index":8,"strongs":"H08085","text":"the"},{"index":9,"strongs":0,"text":"world:"}]},{"chapter":49,"text":"Both low and high, rich and poor, together.","verse":2,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Both "},{"index":1,"strongs":"H0120","text":"low"},{"index":2,"strongs":"H01121","text":"and"},{"index":3,"strongs":"H0376","text":"high,"},{"index":4,"strongs":"H01121","text":"rich"},{"index":5,"strongs":"H06223","text":"and"},{"index":6,"strongs":0,"text":"poor,"},{"index":7,"strongs":"H0034","text":"together."}]},{"chapter":49,"text":"My mouth shall speak of wisdom; and the meditation of my heart       [shall be] of understanding.","verse":3,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"My "},{"index":1,"strongs":"H06310","text":"mouth"},{"index":2,"strongs":0,"text":"shall"},{"index":3,"strongs":"H01696","text":"speak"},{"index":4,"strongs":0,"text":"of"},{"index":5,"strongs":"H02454","text":"wisdom;"},{"index":6,"strongs":0,"text":"and the"},{"index":7,"strongs":"H01900","text":"meditation"},{"index":8,"strongs":0,"text":"of my"},{"index":9,"strongs":"H03820","text":"heart"},{"index":10,"strongs":0,"text":"shall be of"},{"index":11,"strongs":"H08394","text":"understanding."}]},{"chapter":49,"text":"I will incline mine ear to a parable: I will open my dark saying       upon the harp.","verse":4,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"I will "},{"index":1,"strongs":"H05186","text":"incline"},{"index":2,"strongs":0,"text":"mine"},{"index":3,"strongs":"H0241","text":"ear"},{"index":4,"strongs":0,"text":"to a"},{"index":5,"strongs":"H04912","text":"parable:"},{"index":6,"strongs":0,"text":"I will"},{"index":7,"strongs":"H06605","text":"open"},{"index":8,"strongs":0,"text":"my dark"},{"index":9,"strongs":"H02420","text":"saying"},{"index":10,"strongs":0,"text":"upon the"},{"index":11,"strongs":"H03658","text":"harp."}]},{"chapter":49,"text":"Wherefore should I fear in the days of evil, [when] the iniquity       of my heels shall compass me about?","verse":5,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Wherefore should I "},{"index":1,"strongs":"H03372","text":"fear"},{"index":2,"strongs":0,"text":"in the"},{"index":3,"strongs":"H03117","text":"days"},{"index":4,"strongs":0,"text":"of"},{"index":5,"strongs":"H07451","text":"evil,"},{"index":6,"strongs":0,"text":"when the"},{"index":7,"strongs":"H05771","text":"iniquity"},{"index":8,"strongs":0,"text":"of my"},{"index":9,"strongs":"H06120","text":"heels"},{"index":10,"strongs":0,"text":"shall"},{"index":11,"strongs":"H05437","text":"compass"},{"index":12,"strongs":0,"text":"me about?"}]},{"chapter":49,"text":"They that trust in their wealth, and boast themselves in the       multitude of their riches;","verse":6,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"They that "},{"index":1,"strongs":"H0982","text":"trust"},{"index":2,"strongs":0,"text":"in their"},{"index":3,"strongs":"H02428","text":"wealth,"},{"index":4,"strongs":0,"text":"and"},{"index":5,"strongs":"H01984","text":"boast"},{"index":6,"strongs":0,"text":"themselves in the"},{"index":7,"strongs":"H07230","text":"multitude"},{"index":8,"strongs":0,"text":"of their"},{"index":9,"strongs":"H06239","text":"riches;"}]},{"chapter":49,"text":"None [of them] can by any means redeem his brother, nor give to       God a ransom for him:","verse":7,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"None of them "},{"index":1,"strongs":"H0376","text":"can"},{"index":2,"strongs":0,"text":"by any"},{"index":3,"strongs":"H06299","text":"means"},{"index":4,"strongs":"H06299","text":"redeem"},{"index":5,"strongs":0,"text":"his"},{"index":6,"strongs":"H0251","text":"brother,"},{"index":7,"strongs":0,"text":"nor"},{"index":8,"strongs":"H05414","text":"give"},{"index":9,"strongs":0,"text":"to"},{"index":10,"strongs":"H0430","text":"God"},{"index":11,"strongs":0,"text":"a"},{"index":12,"strongs":"H03724","text":"ransom"},{"index":13,"strongs":0,"text":"for him:"}]},{"chapter":49,"text":"(For the redemption of their soul [is] precious, and it ceaseth       for ever:)","verse":8,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"(For the "},{"index":1,"strongs":"H06306","text":"redemption"},{"index":2,"strongs":0,"text":"of their"},{"index":3,"strongs":"H05315","text":"soul"},{"index":4,"strongs":0,"text":"is"},{"index":5,"strongs":"H03365","text":"precious,"},{"index":6,"strongs":0,"text":"and it"},{"index":7,"strongs":"H02308","text":"ceaseth"},{"index":8,"strongs":0,"text":"for"},{"index":9,"strongs":"H05769","text":"ever:)"}]},{"chapter":49,"text":"That he should still live for ever, [and] not see corruption.","verse":9,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"That he should still "},{"index":1,"strongs":"H02421","text":"live"},{"index":2,"strongs":0,"text":"for"},{"index":3,"strongs":"H05331","text":"ever,"},{"index":4,"strongs":0,"text":"and not"},{"index":5,"strongs":"H07200","text":"see"},{"index":6,"strongs":"H07845","text":"corruption."}]},{"chapter":49,"text":"For he seeth [that] wise men die, likewise the fool and the       brutish person perish, and leave their wealth to others.","verse":10,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"For he "},{"index":1,"strongs":"H07200","text":"seeth"},{"index":2,"strongs":0,"text":"that wise"},{"index":3,"strongs":"H02450","text":"men"},{"index":4,"strongs":"H04191","text":"die,"},{"index":5,"strongs":"H03162","text":"likewise"},{"index":6,"strongs":0,"text":"the"},{"index":7,"strongs":"H03684","text":"fool"},{"index":8,"strongs":0,"text":"and the brutish"},{"index":9,"strongs":"H01198","text":"person"},{"index":10,"strongs":"H0006","text":"perish,"},{"index":11,"strongs":0,"text":"and"},{"index":12,"strongs":"H05800","text":"leave"},{"index":13,"strongs":0,"text":"their"},{"index":14,"strongs":"H02428","text":"wealth"},{"index":15,"strongs":0,"text":"to"},{"index":16,"strongs":"H0312","text":"others."}]},{"chapter":49,"text":"Their inward thought [is, that] their houses [shall continue]       for ever, [and] their dwelling places to all generations; they       call [their] lands after their own names.","verse":11,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Their "},{"index":1,"strongs":"H07130","text":"inward"},{"index":2,"strongs":0,"text":"thought is, that their"},{"index":3,"strongs":"H01004","text":"houses"},{"index":4,"strongs":0,"text":"shall continue for"},{"index":5,"strongs":"H05769","text":"ever,"},{"index":6,"strongs":0,"text":"and their"},{"index":7,"strongs":"H04908","text":"dwelling"},{"index":8,"strongs":0,"text":"places to"},{"index":9,"strongs":"H01755","text":"all"},{"index":10,"strongs":"H01755","text":"generations;"},{"index":11,"strongs":0,"text":"they"},{"index":12,"strongs":"H07121","text":"call"},{"index":13,"strongs":0,"text":"their"},{"index":14,"strongs":"H0127","text":"lands"},{"index":15,"strongs":0,"text":"after their own"},{"index":16,"strongs":"H08034","text":"names."}]},{"chapter":49,"text":"Nevertheless man [being] in honour abideth not: he is like the       beasts [that] perish.","verse":12,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Nevertheless "},{"index":1,"strongs":"H0120","text":"man"},{"index":2,"strongs":0,"text":"being in"},{"index":3,"strongs":"H03366","text":"honour"},{"index":4,"strongs":"H03885","text":"abideth"},{"index":5,"strongs":0,"text":"not: he is"},{"index":6,"strongs":"H04911","text":"like"},{"index":7,"strongs":0,"text":"the"},{"index":8,"strongs":"H0929","text":"beasts"},{"index":9,"strongs":0,"text":"that"},{"index":10,"strongs":"H01820","text":"perish."}]},{"chapter":49,"text":"This their way [is] their folly: yet their posterity approve       their sayings. Selah.","verse":13,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"This their "},{"index":1,"strongs":"H01870","text":"way"},{"index":2,"strongs":0,"text":"is their"},{"index":3,"strongs":"H03689","text":"folly:"},{"index":4,"strongs":0,"text":"yet their"},{"index":5,"strongs":"H0310","text":"posterity"},{"index":6,"strongs":"H07521","text":"approve"},{"index":7,"strongs":0,"text":"their"},{"index":8,"strongs":"H06310","text":"sayings."},{"index":9,"strongs":"H05542","text":"Selah."}]},{"chapter":49,"text":"Like sheep they are laid in the grave; death shall feed on       them; and the upright shall have dominion over them in the       morning; and their beauty shall consume in the grave from their       dwelling.","verse":14,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Like "},{"index":1,"strongs":"H06629","text":"sheep"},{"index":2,"strongs":0,"text":"they are"},{"index":3,"strongs":"H08371","text":"laid"},{"index":4,"strongs":0,"text":"in the"},{"index":5,"strongs":"H07585","text":"grave;"},{"index":6,"strongs":"H04194","text":"death"},{"index":7,"strongs":0,"text":"shall"},{"index":8,"strongs":"H07462","text":"feed"},{"index":9,"strongs":0,"text":"on them; and the"},{"index":10,"strongs":"H03477","text":"upright"},{"index":11,"strongs":0,"text":"shall have"},{"index":12,"strongs":"H07287","text":"dominion"},{"index":13,"strongs":0,"text":"over them in the"},{"index":14,"strongs":"H01242","text":"morning;"},{"index":15,"strongs":0,"text":"and their"},{"index":16,"strongs":"H06697","text":"beauty"},{"index":17,"strongs":"H06736","text":"shall"},{"index":18,"strongs":0,"text":"consume"},{"index":19,"strongs":"H01086","text":"in"},{"index":20,"strongs":0,"text":"the grave"},{"index":21,"strongs":"H07585","text":"from"},{"index":22,"strongs":0,"text":"their dwelling."}]},{"chapter":49,"text":"But God will redeem my soul from the power of the grave: for he       shall receive me. Selah.","verse":15,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"But "},{"index":1,"strongs":"H0430","text":"God"},{"index":2,"strongs":0,"text":"will"},{"index":3,"strongs":"H06299","text":"redeem"},{"index":4,"strongs":0,"text":"my"},{"index":5,"strongs":"H05315","text":"soul"},{"index":6,"strongs":0,"text":"from the"},{"index":7,"strongs":"H03027","text":"power"},{"index":8,"strongs":0,"text":"of the"},{"index":9,"strongs":"H07585","text":"grave:"},{"index":10,"strongs":0,"text":"for he shall"},{"index":11,"strongs":"H03947","text":"receive"},{"index":12,"strongs":0,"text":"me."},{"index":13,"strongs":"H05542","text":"Selah."}]},{"chapter":49,"text":"Be not thou afraid when one is made rich, when the glory of his       house is increased;","verse":16,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Be not thou "},{"index":1,"strongs":"H03372","text":"afraid"},{"index":2,"strongs":0,"text":"when"},{"index":3,"strongs":"H0376","text":"one"},{"index":4,"strongs":0,"text":"is made"},{"index":5,"strongs":"H06238","text":"rich,"},{"index":6,"strongs":0,"text":"when the"},{"index":7,"strongs":"H03519","text":"glory"},{"index":8,"strongs":0,"text":"of his"},{"index":9,"strongs":"H01004","text":"house"},{"index":10,"strongs":0,"text":"is"},{"index":11,"strongs":"H07235","text":"increased;"}]},{"chapter":49,"text":"For when he dieth he shall carry nothing away: his glory shall       not descend after him.","verse":17,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"For when he "},{"index":1,"strongs":"H04194","text":"dieth"},{"index":2,"strongs":0,"text":"he shall carry nothing"},{"index":3,"strongs":"H03947","text":"away:"},{"index":4,"strongs":0,"text":"his"},{"index":5,"strongs":"H03519","text":"glory"},{"index":6,"strongs":0,"text":"shall not"},{"index":7,"strongs":"H03381","text":"descend"},{"index":8,"strongs":"H0310","text":"after"},{"index":9,"strongs":0,"text":"him."}]},{"chapter":49,"text":"Though while he lived he blessed his soul: and [men] will       praise thee, when thou doest well to thyself.","verse":18,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Though while he "},{"index":1,"strongs":"H02416","text":"lived"},{"index":2,"strongs":0,"text":"he"},{"index":3,"strongs":"H01288","text":"blessed"},{"index":4,"strongs":0,"text":"his"},{"index":5,"strongs":"H05315","text":"soul:"},{"index":6,"strongs":0,"text":"and men will"},{"index":7,"strongs":"H03034","text":"praise"},{"index":8,"strongs":0,"text":"thee, when thou doest"},{"index":9,"strongs":"H03190","text":"well"},{"index":10,"strongs":0,"text":"to thyself."}]},{"chapter":49,"text":"He shall go to the generation of his fathers; they shall never       see light.","verse":19,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He shall "},{"index":1,"strongs":"H0935","text":"go"},{"index":2,"strongs":0,"text":"to the"},{"index":3,"strongs":"H01755","text":"generation"},{"index":4,"strongs":0,"text":"of his"},{"index":5,"strongs":"H0001","text":"fathers;"},{"index":6,"strongs":0,"text":"they shall"},{"index":7,"strongs":"H05331","text":"never"},{"index":8,"strongs":"H03808","text":"see"},{"index":9,"strongs":"H07200","text":"light."}]},{"chapter":49,"text":"Man [that is] in honour, and understandeth not, is like the       beasts [that] perish.   Psalm 50  A Psalm of Asaph.","verse":20,"book":"Psalms","reference":[{"index":0,"strongs":"H0120","text":"Man"},{"index":1,"strongs":0,"text":"that is in"},{"index":2,"strongs":"H03366","text":"honour,"},{"index":3,"strongs":0,"text":"and"},{"index":4,"strongs":"H0995","text":"understandeth"},{"index":5,"strongs":0,"text":"not, is"},{"index":6,"strongs":"H04911","text":"like"},{"index":7,"strongs":0,"text":"the"},{"index":8,"strongs":"H0929","text":"beasts"},{"index":9,"strongs":0,"text":"that"},{"index":10,"strongs":"H01820","text":"perish."}]}]