[{"chapter":72,"text":"Give the king thy judgments, O God, and thy righteousness       unto the king's son.","verse":1,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Give the king "},{"index":1,"strongs":"H08010","text":"thy"},{"index":2,"strongs":"H05414","text":"judgments,"},{"index":3,"strongs":0,"text":"O"},{"index":4,"strongs":"H04428","text":"God,"},{"index":5,"strongs":0,"text":"and"},{"index":6,"strongs":"H04941","text":"thy"},{"index":7,"strongs":0,"text":"righteousness"},{"index":8,"strongs":"H0430","text":"unto"},{"index":9,"strongs":0,"text":"the king's"},{"index":10,"strongs":"H06666","text":"son."}]},{"chapter":72,"text":"He shall judge thy people with righteousness, and thy poor with       judgment.","verse":2,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He shall "},{"index":1,"strongs":"H01777","text":"judge"},{"index":2,"strongs":0,"text":"thy"},{"index":3,"strongs":"H05971","text":"people"},{"index":4,"strongs":0,"text":"with"},{"index":5,"strongs":"H06664","text":"righteousness,"},{"index":6,"strongs":0,"text":"and thy"},{"index":7,"strongs":"H06041","text":"poor"},{"index":8,"strongs":0,"text":"with"},{"index":9,"strongs":"H04941","text":"judgment."}]},{"chapter":72,"text":"The mountains shall bring peace to the people, and the little       hills, by righteousness.","verse":3,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H02022","text":"mountains"},{"index":2,"strongs":0,"text":"shall"},{"index":3,"strongs":"H05375","text":"bring"},{"index":4,"strongs":"H07965","text":"peace"},{"index":5,"strongs":0,"text":"to the"},{"index":6,"strongs":"H05971","text":"people,"},{"index":7,"strongs":0,"text":"and the little"},{"index":8,"strongs":"H01389","text":"hills,"},{"index":9,"strongs":0,"text":"by"},{"index":10,"strongs":"H06666","text":"righteousness."}]},{"chapter":72,"text":"He shall judge the poor of the people, he shall save the       children of the needy, and shall break in pieces the oppressor.","verse":4,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He shall "},{"index":1,"strongs":"H08199","text":"judge"},{"index":2,"strongs":0,"text":"the"},{"index":3,"strongs":"H06041","text":"poor"},{"index":4,"strongs":0,"text":"of the"},{"index":5,"strongs":"H05971","text":"people,"},{"index":6,"strongs":0,"text":"he shall"},{"index":7,"strongs":"H03467","text":"save"},{"index":8,"strongs":0,"text":"the"},{"index":9,"strongs":"H01121","text":"children"},{"index":10,"strongs":0,"text":"of the"},{"index":11,"strongs":"H0034","text":"needy,"},{"index":12,"strongs":0,"text":"and shall break in"},{"index":13,"strongs":"H01792","text":"pieces"},{"index":14,"strongs":0,"text":"the"},{"index":15,"strongs":"H06231","text":"oppressor."}]},{"chapter":72,"text":"They shall fear thee as long as the sun and moon endure,       throughout all generations.","verse":5,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"They shall "},{"index":1,"strongs":"H03372","text":"fear"},{"index":2,"strongs":0,"text":"thee as long"},{"index":3,"strongs":"H05973","text":"as"},{"index":4,"strongs":0,"text":"the"},{"index":5,"strongs":"H08121","text":"sun"},{"index":6,"strongs":0,"text":"and"},{"index":7,"strongs":"H03394","text":"moon"},{"index":8,"strongs":"H06440","text":"endure,"},{"index":9,"strongs":0,"text":"throughout"},{"index":10,"strongs":"H01755","text":"all"},{"index":11,"strongs":"H01755","text":"generations."}]},{"chapter":72,"text":"He shall come down like rain upon the mown grass: as showers       [that] water the earth.","verse":6,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He shall come "},{"index":1,"strongs":"H03381","text":"down"},{"index":2,"strongs":0,"text":"like"},{"index":3,"strongs":"H04306","text":"rain"},{"index":4,"strongs":0,"text":"upon the"},{"index":5,"strongs":"H01488","text":"mown"},{"index":6,"strongs":0,"text":"grass: as"},{"index":7,"strongs":"H07241","text":"showers"},{"index":8,"strongs":0,"text":"that"},{"index":9,"strongs":"H02222","text":"water"},{"index":10,"strongs":0,"text":"the"},{"index":11,"strongs":"H0776","text":"earth."}]},{"chapter":72,"text":"In his days shall the righteous flourish; and abundance of peace       so long as the moon endureth.","verse":7,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"In his "},{"index":1,"strongs":"H03117","text":"days"},{"index":2,"strongs":0,"text":"shall the"},{"index":3,"strongs":"H06662","text":"righteous"},{"index":4,"strongs":"H06524","text":"flourish;"},{"index":5,"strongs":0,"text":"and"},{"index":6,"strongs":"H07230","text":"abundance"},{"index":7,"strongs":0,"text":"of"},{"index":8,"strongs":"H07965","text":"peace"},{"index":9,"strongs":0,"text":"so long as the"},{"index":10,"strongs":"H03394","text":"moon"},{"index":11,"strongs":0,"text":"endureth."}]},{"chapter":72,"text":"He shall have dominion also from sea to sea, and from the river       unto the ends of the earth.","verse":8,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He shall have "},{"index":1,"strongs":"H07287","text":"dominion"},{"index":2,"strongs":0,"text":"also from"},{"index":3,"strongs":"H03220","text":"sea"},{"index":4,"strongs":0,"text":"to"},{"index":5,"strongs":"H03220","text":"sea,"},{"index":6,"strongs":0,"text":"and from the"},{"index":7,"strongs":"H05104","text":"river"},{"index":8,"strongs":0,"text":"unto the"},{"index":9,"strongs":"H0657","text":"ends"},{"index":10,"strongs":0,"text":"of the"},{"index":11,"strongs":"H0776","text":"earth."}]},{"chapter":72,"text":"They that dwell in the wilderness shall bow before him; and his       enemies shall lick the dust.","verse":9,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"They that dwell in the "},{"index":1,"strongs":"H06728","text":"wilderness"},{"index":2,"strongs":0,"text":"shall"},{"index":3,"strongs":"H03766","text":"bow"},{"index":4,"strongs":"H06440","text":"before"},{"index":5,"strongs":0,"text":"him; and his"},{"index":6,"strongs":"H0341","text":"enemies"},{"index":7,"strongs":0,"text":"shall"},{"index":8,"strongs":"H03897","text":"lick"},{"index":9,"strongs":0,"text":"the"},{"index":10,"strongs":"H06083","text":"dust."}]},{"chapter":72,"text":"The kings of Tarshish and of the isles shall bring presents:       the kings of Sheba and Seba shall offer gifts.","verse":10,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H04428","text":"kings"},{"index":2,"strongs":0,"text":"of"},{"index":3,"strongs":"H08659","text":"Tarshish"},{"index":4,"strongs":0,"text":"and of the"},{"index":5,"strongs":"H0339","text":"isles"},{"index":6,"strongs":0,"text":"shall"},{"index":7,"strongs":"H07725","text":"bring"},{"index":8,"strongs":"H04503","text":"presents:"},{"index":9,"strongs":0,"text":"the"},{"index":10,"strongs":"H04428","text":"kings"},{"index":11,"strongs":0,"text":"of"},{"index":12,"strongs":"H07614","text":"Sheba"},{"index":13,"strongs":0,"text":"and"},{"index":14,"strongs":"H05434","text":"Seba"},{"index":15,"strongs":0,"text":"shall"},{"index":16,"strongs":"H07126","text":"offer"},{"index":17,"strongs":"H0814","text":"gifts."}]},{"chapter":72,"text":"Yea, all kings shall fall down before him: all nations shall       serve him.","verse":11,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Yea, all "},{"index":1,"strongs":"H04428","text":"kings"},{"index":2,"strongs":0,"text":"shall fall"},{"index":3,"strongs":"H07812","text":"down"},{"index":4,"strongs":0,"text":"before him: all"},{"index":5,"strongs":"H01471","text":"nations"},{"index":6,"strongs":0,"text":"shall"},{"index":7,"strongs":"H05647","text":"serve"},{"index":8,"strongs":0,"text":"him."}]},{"chapter":72,"text":"For he shall deliver the needy when he crieth; the poor also,       and [him] that hath no helper.","verse":12,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"For he shall "},{"index":1,"strongs":"H05337","text":"deliver"},{"index":2,"strongs":0,"text":"the"},{"index":3,"s