[{"chapter":35,"text":"Plead [my cause,] O LORD, with them that strive with me:       fight against them that fight against me.","verse":1,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Plead my cause, "},{"index":1,"strongs":"H01732","text":"O"},{"index":2,"strongs":"H07378","text":"LORD,"},{"index":3,"strongs":0,"text":"with them that"},{"index":4,"strongs":"H03068","text":"strive"},{"index":5,"strongs":0,"text":"with me: fight"},{"index":6,"strongs":"H03401","text":"against"},{"index":7,"strongs":0,"text":"them that"},{"index":8,"strongs":"H03898","text":"fight"},{"index":9,"strongs":0,"text":"against me."}]},{"chapter":35,"text":"Take hold of shield and buckler, and stand up for mine help.","verse":2,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Take "},{"index":1,"strongs":"H02388","text":"hold"},{"index":2,"strongs":0,"text":"of"},{"index":3,"strongs":"H04043","text":"shield"},{"index":4,"strongs":0,"text":"and"},{"index":5,"strongs":"H06793","text":"buckler,"},{"index":6,"strongs":0,"text":"and stand"},{"index":7,"strongs":"H06965","text":"up"},{"index":8,"strongs":0,"text":"for mine"},{"index":9,"strongs":"H05833","text":"help."}]},{"chapter":35,"text":"Draw out also the spear, and stop [the way] against them that       persecute me: say unto my soul, I [am] thy salvation.","verse":3,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Draw "},{"index":1,"strongs":"H07324","text":"out"},{"index":2,"strongs":0,"text":"also the"},{"index":3,"strongs":"H02595","text":"spear,"},{"index":4,"strongs":0,"text":"and"},{"index":5,"strongs":"H05462","text":"stop"},{"index":6,"strongs":0,"text":"the way"},{"index":7,"strongs":"H07125","text":"against"},{"index":8,"strongs":0,"text":"them that"},{"index":9,"strongs":"H07291","text":"persecute"},{"index":10,"strongs":0,"text":"me:"},{"index":11,"strongs":"H0559","text":"say"},{"index":12,"strongs":0,"text":"unto my"},{"index":13,"strongs":"H05315","text":"soul,"},{"index":14,"strongs":0,"text":"I am thy"},{"index":15,"strongs":"H03444","text":"salvation."}]},{"chapter":35,"text":"Let them be confounded and put to shame that seek after my soul:       let them be turned back and brought to confusion that devise my       hurt.","verse":4,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Let them be "},{"index":1,"strongs":"H0954","text":"confounded"},{"index":2,"strongs":0,"text":"and put to"},{"index":3,"strongs":"H03637","text":"shame"},{"index":4,"strongs":0,"text":"that"},{"index":5,"strongs":"H01245","text":"seek"},{"index":6,"strongs":0,"text":"after my"},{"index":7,"strongs":"H05315","text":"soul:"},{"index":8,"strongs":0,"text":"let them be"},{"index":9,"strongs":"H05472","text":"turned"},{"index":10,"strongs":"H0268","text":"back"},{"index":11,"strongs":0,"text":"and brought to"},{"index":12,"strongs":"H02659","text":"confusion"},{"index":13,"strongs":0,"text":"that"},{"index":14,"strongs":"H02803","text":"devise"},{"index":15,"strongs":0,"text":"my"},{"index":16,"strongs":"H07451","text":"hurt."}]},{"chapter":35,"text":"Let them be as chaff before the wind: and let the angel of the       LORD chase [them.]","verse":5,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Let them be as "},{"index":1,"strongs":"H04671","text":"chaff"},{"index":2,"strongs":"H06440","text":"before"},{"index":3,"strongs":0,"text":"the"},{"index":4,"strongs":"H07307","text":"wind:"},{"index":5,"strongs":0,"text":"and let the"},{"index":6,"strongs":"H04397","text":"angel"},{"index":7,"strongs":0,"text":"of the"},{"index":8,"strongs":"H03068","text":"LORD"},{"index":9,"strongs":"H01760","text":"chase"},{"index":10,"strongs":0,"text":"them."}]},{"chapter":35,"text":"Let their way be dark and slippery: and let the angel of the       LORD persecute them.","verse":6,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Let their "},{"index":1,"strongs":"H01870","text":"way"},{"index":2,"strongs":0,"text":"be"},{"index":3,"strongs":"H02822","text":"dark"},{"index":4,"strongs":0,"text":"and"},{"index":5,"strongs":"H02519","text":"slippery:"},{"index":6,"strongs":0,"text":"and let the"},{"index":7,"strongs":"H04397","text":"angel"},{"index":8,"strongs":0,"text":"of the"},{"index":9,"strongs":"H03068","text":"LORD"},{"index":10,"strongs":"H07291","text":"persecute"},{"index":11,"strongs":0,"text":"them."}]},{"chapter":35,"text":"For without cause have they hid for me their net [in] a pit,       [which] without cause they have digged for my soul.","verse":7,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"For without "},{"index":1,"strongs":"H02600","text":"cause"},{"index":2,"strongs":0,"text":"have they"},{"index":3,"strongs":"H02934","text":"hid"},{"index":4,"strongs":0,"text":"for me their"},{"index":5,"strongs":"H07568","text":"net"},{"index":6,"strongs":0,"text":"in a"},{"index":7,"strongs":"H07845","text":"pit,"},{"index":8,"strongs":0,"text":"which without"},{"index":9,"strongs":"H02600","text":"cause"},{"index":10,"strongs":0,"text":"they have"},{"index":11,"strongs":"H02658","text":"digged"},{"index":12,"strongs":0,"text":"for my"},{"index":13,"strongs":"H05315","text":"soul."}]},{"chapter":35,"text":"Let destruction come upon him at unawares; and let his net that       he hath hid catch himself: into that very destruction let him       fall.","verse":8,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Let "},{"index":1,"strongs":"H07722","text":"destruction"},{"index":2,"strongs":"H0935","text":"come"},{"index":3,"strongs":0,"text":"upon him at"},{"index":4,"strongs":"H03045","text":"unawares;"},{"index":5,"strongs":"H03808","text":"and"},{"index":6,"strongs":0,"text":"let his net"},{"index":7,"strongs":"H07568","text":"that"},{"index":8,"strongs":0,"text":"he hath hid"},{"index":9,"strongs":"H02934","text":"catch"},{"index":10,"strongs":"H03920","text":"himself:"},{"index":11,"strongs":0,"text":"into that very destruction"},{"index":12,"strongs":"H07722","text":"let"},{"index":13,"strongs":0,"text":"him fall."}]},{"chapter":35,"text":"And my soul shall be joyful in the LORD: it shall rejoice in his       salvation.","verse":9,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"And my "},{"index":1,"strongs":"H05315","text":"soul"},{"index":2,"strongs":0,"text":"shall be"},{"index":3,"strongs":"H01523","text":"joyful"},{"index":4,"strongs":0,"text":"in the"},{"index":5,"strongs":"H03068","text":"LORD:"},{"index":6,"strongs":0,"text":"it shall"},{"index":7,"strongs":"H07797","text":"rejoice"},{"index":8,"strongs":0,"text":"in his"},{"index":9,"strongs":"H03444","text":"salvation."}]},{"chapter":35,"text":"All my bones shall say, LORD, who [is] like unto thee, which       deliverest the poor from him that is too strong for him, yea, the       poor and the needy from him that spoileth him?","verse":10,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"All my "},{"index":1,"strongs":"H06106","text":"bones"},{"index":2,"strongs":0,"text":"shall"},{"index":3,"strongs":"H0559","text":"say,"},{"index":4,"strongs":"H03068","text":"LORD,"},{"index":5,"strongs":0,"text":"who is like unto thee, which"},{"index":6,"strongs":"H05337","text":"deliverest"},{"index":7,"strongs":0,"text":"the"},{"index":8,"strongs":"H06041","text":"poor"},{"index":9,"strongs":0,"text":"from him that is too"},{"index":10,"strongs":"H02389","text":"strong"},{"index":11,"strongs":0,"text":"for him, yea, the"},{"index":12,"strongs":"H06041","text":"poor"},{"index":13,"strongs":0,"text":"and the"},{"index":14,"strongs":"H0034","text":"needy"},{"index":15,"strongs":0,"text":"from him that"},{"index":16,"strongs":"H01497","text":"spoileth"},{"index":17,"strongs":0,"text":"him?"}]},{"chapter":35,"text":"False witnesses did rise up; they laid to my charge [things]       that I knew not.","verse":11,"book":"Psalms","reference":[{"index":0,"strongs":"H02555","text":"False"},{"index":1,"strongs":"H05707","text":"witnesses"},{"index":2,"strongs":0,"text":"did rise"},{"index":3,"strongs":"H06965","text":"up;"},{"index":4,"strongs":0,"text":"they laid to my"},{"index":5,"strongs":"H07592","text":"charge"},{"index":6,"strongs":0,"text":"things that I"},{"index":7,"strongs":"H03045","text":"knew"},{"index":8,"strongs":0,"text":"not."}]},{"chapter":35,"text":"They rewarded me evil for good [to] the spoiling of my soul.","verse":12,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"They "},{"index":1,"strongs":"H07999","text":"rewarded"},{"index":2,"strongs":0,"text":"me"},{"index":3,"strongs":"H07451","text":"evil"},{"index":4,"strongs":0,"text":"for"},{"index":5,"strongs":"H02896","text":"good"},{"index":6,"strongs":0,"text":"to the"},{"index":7,"strongs":"H07908","text":"spoiling"},{"index":8,"strongs":0,"text":"of my"},{"index":9,"strongs":"H05315","text":"soul."}]},{"chapter":35,"text":"But as for me, when they were sick, my clothing [was]       sackcloth: I humbled my soul with fasting; and my prayer returned       into mine own bosom.","verse":13,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"But as for me, when they were "},{"index":1,"strongs":"H02470","text":"sick,"},{"index":2,"strongs":0,"text":"my"},{"index":3,"strongs":"H03830","text":"clothing"},{"index":4,"strongs":0,"text":"was"},{"index":5,"strongs":"H08242","text":"sackcloth:"},{"index":6,"strongs":0,"text":"I"},{"index":7,"strongs":"H06031","text":"humbled"},{"index":8,"strongs":0,"text":"my"},{"index":9,"strongs":"H05315","text":"soul"},{"index":10,"strongs":0,"text":"with"},{"index":11,"strongs":"H06685","text":"fasting;"},{"index":12,"strongs":0,"text":"and my"},{"index":13,"strongs":"H08605","text":"prayer"},{"index":14,"strongs":"H07725","text":"returned"},{"index":15,"strongs":0,"text":"into mine own"},{"index":16,"strongs":"H02436","text":"bosom."}]},{"chapter":35,"text":"I behaved myself as though [he had been] my friend [or]       brother: I bowed down heavily, as one that mourneth [for his]       mother.","verse":14,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"I "},{"index":1,"strongs":"H01980","text":"behaved"},{"index":2,"strongs":0,"text":"myself as though he had been my"},{"index":3,"strongs":"H07453","text":"friend"},{"index":4,"strongs":0,"text":"or"},{"index":5,"strongs":"H0251","text":"brother:"},{"index":6,"strongs":0,"text":"I bowed"},{"index":7,"strongs":"H07817","text":"down"},{"index":8,"strongs":"H06937","text":"heavily,"},{"index":9,"strongs":0,"text":"as one that"},{"index":10,"strongs":"H0057","text":"mourneth"},{"index":11,"strongs":0,"text":"for his"},{"index":12,"strongs":"H0517","text":"mother."}]},{"chapter":35,"text":"But in mine adversity they rejoiced, and gathered themselves       together: [yea,] the abjects gathered themselves together against       me, and I knew [it] not; they did tear [me,] and ceased not:","verse":15,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"But in mine "},{"index":1,"strongs":"H06761","text":"adversity"},{"index":2,"strongs":0,"text":"they"},{"index":3,"strongs":"H08055","text":"rejoiced,"},{"index":4,"strongs":0,"text":"and gathered themselves"},{"index":5,"strongs":"H0622","text":"together:"},{"index":6,"strongs":0,"text":"yea, the"},{"index":7,"strongs":"H05222","text":"abjects"},{"index":8,"strongs":0,"text":"gathered themselves"},{"index":9,"strongs":"H0622","text":"together"},{"index":10,"strongs":0,"text":"against me, and I"},{"index":11,"strongs":"H03045","text":"knew"},{"index":12,"strongs":0,"text":"it not; they did"},{"index":13,"strongs":"H07167","text":"tear"},{"index":14,"strongs":0,"text":"me, and"},{"index":15,"strongs":"H01826","text":"ceased"},{"index":16,"strongs":0,"text":"not:"}]},{"chapter":35,"text":"With hypocritical mockers in feasts, they gnashed upon me with       their teeth.","verse":16,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"With "},{"index":1,"strongs":"H02611","text":"hypocritical"},{"index":2,"strongs":"H03934","text":"mockers"},{"index":3,"strongs":0,"text":"in"},{"index":4,"strongs":"H04580","text":"feasts,"},{"index":5,"strongs":0,"text":"they"},{"index":6,"strongs":"H02786","text":"gnashed"},{"index":7,"strongs":0,"text":"upon me with their"},{"index":8,"strongs":"H08127","text":"teeth."}]},{"chapter":35,"text":"Lord, how long wilt thou look on? rescue my soul from their       destructions, my darling from the lions.","verse":17,"book":"Psalms","reference":[{"index":0,"strongs":"H0136","text":"Lord,"},{"index":1,"strongs":0,"text":"how long wilt thou look"},{"index":2,"strongs":"H07200","text":"on?"},{"index":3,"strongs":"H07725","text":"rescue"},{"index":4,"strongs":0,"text":"my"},{"index":5,"strongs":"H05315","text":"soul"},{"index":6,"strongs":0,"text":"from their"},{"index":7,"strongs":"H07722","text":"destructions,"},{"index":8,"strongs":0,"text":"my"},{"index":9,"strongs":"H03173","text":"darling"},{"index":10,"strongs":0,"text":"from the"},{"index":11,"strongs":"H03715","text":"lions."}]},{"chapter":35,"text":"I will give thee thanks in the great congregation: I will       praise thee among much people.","verse":18,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"I will give thee "},{"index":1,"strongs":"H03034","text":"thanks"},{"index":2,"strongs":0,"text":"in the"},{"index":3,"strongs":"H07227","text":"great"},{"index":4,"strongs":"H06951","text":"congregation:"},{"index":5,"strongs":0,"text":"I will"},{"index":6,"strongs":"H01984","text":"praise"},{"index":7,"strongs":0,"text":"thee among"},{"index":8,"strongs":"H06099","text":"much"},{"index":9,"strongs":"H05971","text":"people."}]},{"chapter":35,"text":"Let not them that are mine enemies wrongfully rejoice over me:       [neither] let them wink with the eye that hate me without a cause.","verse":19,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Let not them that are mine "},{"index":1,"strongs":"H0341","text":"enemies"},{"index":2,"strongs":"H08267","text":"wrongfully"},{"index":3,"strongs":"H08055","text":"rejoice"},{"index":4,"strongs":0,"text":"over me: neither let them"},{"index":5,"strongs":"H07169","text":"wink"},{"index":6,"strongs":0,"text":"with the"},{"index":7,"strongs":"H05869","text":"eye"},{"index":8,"strongs":0,"text":"that"},{"index":9,"strongs":"H08130","text":"hate"},{"index":10,"strongs":0,"text":"me without a"},{"index":11,"strongs":"H02600","text":"cause."}]},{"chapter":35,"text":"For they speak not peace: but they devise deceitful matters       against [them that are] quiet in the land.","verse":20,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"For they "},{"index":1,"strongs":"H01696","text":"speak"},{"index":2,"strongs":0,"text":"not"},{"index":3,"strongs":"H07965","text":"peace:"},{"index":4,"strongs":0,"text":"but they"},{"index":5,"strongs":"H02803","text":"devise"},{"index":6,"strongs":"H04820","text":"deceitful"},{"index":7,"strongs":"H01697","text":"matters"},{"index":8,"strongs":0,"text":"against them that are"},{"index":9,"strongs":"H07282","text":"quiet"},{"index":10,"strongs":0,"text":"in the"},{"index":11,"strongs":"H0776","text":"land."}]},{"chapter":35,"text":"Yea, they opened their mouth wide against me, [and] said, Aha,       aha, our eye hath seen [it.]","verse":21,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Yea, they opened their "},{"index":1,"strongs":"H06310","text":"mouth"},{"index":2,"strongs":"H07337","text":"wide"},{"index":3,"strongs":0,"text":"against me, and"},{"index":4,"strongs":"H0559","text":"said,"},{"index":5,"strongs":"H01889","text":"Aha,"},{"index":6,"strongs":"H01889","text":"aha,"},{"index":7,"strongs":0,"text":"our"},{"index":8,"strongs":"H05869","text":"eye"},{"index":9,"strongs":0,"text":"hath"},{"index":10,"strongs":"H07200","text":"seen"},{"index":11,"strongs":0,"text":"it."}]},{"chapter":35,"text":"[This] thou hast seen, O LORD: keep not silence: O Lord, be not       far from me.","verse":22,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"This thou hast "},{"index":1,"strongs":"H07200","text