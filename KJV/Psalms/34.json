[{"chapter":34,"text":"I will bless the LORD at all times: his praise [shall]       continually [be] in my mouth.","verse":1,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"I will bless "},{"index":1,"strongs":"H01732","text":"the"},{"index":2,"strongs":0,"text":"LORD at"},{"index":3,"strongs":"H08138","text":"all"},{"index":4,"strongs":0,"text":"times:"},{"index":5,"strongs":"H02940","text":"his"},{"index":6,"strongs":"H06440","text":"praise"},{"index":7,"strongs":"H0040","text":"shall"},{"index":8,"strongs":0,"text":"continually be in"},{"index":9,"strongs":"H01644","text":"my"},{"index":10,"strongs":0,"text":"mouth."}]},{"chapter":34,"text":"My soul shall make her boast in the LORD: the humble shall hear       [thereof,] and be glad.","verse":2,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"My "},{"index":1,"strongs":"H05315","text":"soul"},{"index":2,"strongs":0,"text":"shall make her"},{"index":3,"strongs":"H01984","text":"boast"},{"index":4,"strongs":0,"text":"in the"},{"index":5,"strongs":"H03068","text":"LORD:"},{"index":6,"strongs":0,"text":"the"},{"index":7,"strongs":"H06035","text":"humble"},{"index":8,"strongs":0,"text":"shall"},{"index":9,"strongs":"H08085","text":"hear"},{"index":10,"strongs":0,"text":"thereof, and be"},{"index":11,"strongs":"H08055","text":"glad."}]},{"chapter":34,"text":"O magnify the LORD with me, and let us exalt his name together.","verse":3,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"O "},{"index":1,"strongs":"H01431","text":"magnify"},{"index":2,"strongs":0,"text":"the"},{"index":3,"strongs":"H03068","text":"LORD"},{"index":4,"strongs":0,"text":"with me, and let us"},{"index":5,"strongs":"H07311","text":"exalt"},{"index":6,"strongs":0,"text":"his"},{"index":7,"strongs":"H08034","text":"name"},{"index":8,"strongs":"H03162","text":"together."}]},{"chapter":34,"text":"I sought the LORD, and he heard me, and delivered me from all my       fears.","verse":4,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"I "},{"index":1,"strongs":"H01875","text":"sought"},{"index":2,"strongs":0,"text":"the"},{"index":3,"strongs":"H03068","text":"LORD,"},{"index":4,"strongs":0,"text":"and he"},{"index":5,"strongs":"H06030","text":"heard"},{"index":6,"strongs":0,"text":"me, and"},{"index":7,"strongs":"H05337","text":"delivered"},{"index":8,"strongs":0,"text":"me from all my"},{"index":9,"strongs":"H04035","text":"fears."}]},{"chapter":34,"text":"They looked unto him, and were lightened: and their faces were       not ashamed.","verse":5,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"They "},{"index":1,"strongs":"H05027","text":"looked"},{"index":2,"strongs":0,"text":"unto him, and were"},{"index":3,"strongs":"H05102","text":"lightened:"},{"index":4,"strongs":0,"text":"and their"},{"index":5,"strongs":"H06440","text":"faces"},{"index":6,"strongs":0,"text":"were not"},{"index":7,"strongs":"H02659","text":"ashamed."}]},{"chapter":34,"text":"This poor man cried, and the LORD heard [him,] and saved him out       of all his troubles.","verse":6,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"This poor "},{"index":1,"strongs":"H06041","text":"man"},{"index":2,"strongs":"H07121","text":"cried,"},{"index":3,"strongs":0,"text":"and the"},{"index":4,"strongs":"H03068","text":"LORD"},{"index":5,"strongs":"H08085","text":"heard"},{"index":6,"strongs":0,"text":"him, and"},{"index":7,"strongs":"H03467","text":"saved"},{"index":8,"strongs":0,"text":"him out of all his"},{"index":9,"strongs":"H06869","text":"troubles."}]},{"chapter":34,"text":"The angel of the LORD encampeth round about them that fear him,       and delivereth them.","verse":7,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H04397","text":"angel"},{"index":2,"strongs":0,"text":"of the"},{"index":3,"strongs":"H03068","text":"LORD"},{"index":4,"strongs":"H02583","text":"encampeth"},{"index":5,"strongs":0,"text":"round"},{"index":6,"strongs":"H05439","text":"about"},{"index":7,"strongs":0,"text":"them that"},{"index":8,"strongs":"H03373","text":"fear"},{"index":9,"strongs":0,"text":"him, and"},{"index":10,"strongs":"H02502","text":"delivereth"},{"index":11,"strongs":0,"text":"them."}]},{"chapter":34,"text":"O taste and see that the LORD [is] good: blessed [is] the man       [that] trusteth in him.","verse":8,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"O "},{"index":1,"strongs":"H02938","text":"taste"},{"index":2,"strongs":0,"text":"and"},{"index":3,"strongs":"H07200","text":"see"},{"index":4,"strongs":0,"text":"that the"},{"index":5,"strongs":"H03068","text":"LORD"},{"index":6,"strongs":0,"text":"is"},{"index":7,"strongs":"H02896","text":"good:"},{"index":8,"strongs":"H0835","text":"blessed"},{"index":9,"strongs":0,"text":"is the"},{"index":10,"strongs":"H01397","text":"man"},{"index":11,"strongs":0,"text":"that"},{"index":12,"strongs":"H02620","text":"trusteth"},{"index":13,"strongs":0,"text":"in him."}]},{"chapter":34,"text":"O fear the LORD, ye his saints: for [there is] no want to them       that fear him.","verse":9,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"O "},{"index":1,"strongs":"H03372","text":"fear"},{"index":2,"strongs":0,"text":"the"},{"index":3,"strongs":"H03068","text":"LORD,"},{"index":4,"strongs":0,"text":"ye his"},{"index":5,"strongs":"H06918","text":"saints:"},{"index":6,"strongs":0,"text":"for there is no"},{"index":7,"strongs":"H04270","text":"want"},{"index":8,"strongs":0,"text":"to them that"},{"index":9,"strongs":"H03373","text":"fear"},{"index":10,"strongs":0,"text":"him."}]},{"chapter":34,"text":"The young lions do lack, and suffer hunger: but they that seek       the LORD shall not want any good [thing.]","verse":10,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The young "},{"index":1,"strongs":"H03715","text":"lions"},{"index":2,"strongs":0,"text":"do"},{"index":3,"strongs":"H07326","text":"lack,"},{"index":4,"strongs":0,"text":"and suffer"},{"index":5,"strongs":"H07456","text":"hunger:"},{"index":6,"strongs":0,"text":"but they that"},{"index":7,"strongs":"H01875","text":"seek"},{"index":8,"strongs":0,"text":"the"},{"index":9,"strongs":"H03068","text":"LORD"},{"index":10,"strongs":0,"text":"shall not"},{"index":11,"strongs":"H02637","text":"want"},{"index":12,"strongs":0,"text":"any"},{"index":13,"strongs":"H02896","text":"good"},{"index":14,"strongs":0,"text":"thing."}]},{"chapter":34,"text":"Come, ye children, hearken unto me: I will teach you the fear       of the LORD.","verse":11,"book":"Psalms","reference":[{"index":0,"strongs":"H03212","text":"Come,"},{"index":1,"strongs":0,"text":"ye"},{"index":2,"strongs":"H01121","text":"children,"},{"index":3,"strongs":"H08085","text":"hearken"},{"index":4,"strongs":0,"text":"unto me: I will"},{"index":5,"strongs":"H03925","text":"teach"},{"index":6,"strongs":0,"text":"you the"},{"index":7,"strongs":"H03374","text":"fear"},{"index":8,"strongs":0,"text":"of the"},{"index":9,"strongs":"H03068","text":"LORD."}]},{"chapter":34,"text":"What man [is he that] desireth life, [and] loveth [many] days,       that he may see good?","verse":12,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"What "},{"index":1,"strongs":"H0376","text":"man"},{"index":2,"strongs":0,"text":"is he that"},{"index":3,"strongs":"H02655","text":"desireth"},{"index":4,"strongs":"H02416","text":"life,"},{"index":5,"strongs":0,"text":"and"},{"index":6,"strongs":"H0157","text":"loveth"},{"index":7,"strongs":0,"text":"many"},{"index":8,"strongs":"H03117","text":"days,"},{"index":9,"strongs":0,"text":"that he may"},{"index":10,"strongs":"H07200","text":"see"},{"index":11,"strongs":"H02896","text":"good?"}]},{"chapter":34,"text":"Keep thy tongue from evil, and thy lips from speaking guile.","verse":13,"book":"Psalms","reference":[{"index":0,"strongs":"H05341","text":"Keep"},{"index":1,"strongs":0,"text":"thy"},{"index":2,"strongs":"H03956","text":"tongue"},{"index":3,"strongs":0,"text":"from"},{"index":4,"strongs":"H07451","text":"evil,"},{"index":5,"strongs":0,"text":"and thy"},{"index":6,"strongs":"H08193","text":"lips"},{"index":7,"strongs":0,"text":"from"},{"index":8,"strongs":"H01696","text":"speaking"},{"index":9,"strongs":"H04820","text":"guile."}]},{"chapter":34,"text":"Depart from evil, and do good; seek peace, and pursue it.","verse":14,"book":"Psalms","reference":[{"index":0,"strongs":"H05493","text":"Depart"},{"index":1,"strongs":0,"text":"from"},{"index":2,"strongs":"H07451","text":"evil,"},{"index":3,"strongs":0,"text":"and"},{"index":4,"strongs":"H06213","text":"do"},{"index":5,"strongs":"H02896","text":"good;"},{"index":6,"strongs":"H01245","text":"seek"},{"index":7,"strongs":"H07965","text":"peace,"},{"index":8,"strongs":0,"text":"and"},{"index":9,"strongs":"H07291","text":"pursue"},{"index":10,"strongs":0,"text":"it."}]},{"chapter":34,"text":"The eyes of the LORD [are] upon the righteous, and his ears       [are open] unto their cry.","verse":15,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H05869","text":"eyes"},{"index":2,"strongs":0,"text":"of the"},{"index":3,"strongs":"H03068","text":"LORD"},{"index":4,"strongs":0,"text":"are upon the"},{"index":5,"strongs":"H06662","text":"righteous,"},{"index":6,"strongs":0,"text":"and his"},{"index":7,"strongs":"H0241","text":"ears"},{"index":8,"strongs":0,"text":"are open unto their"},{"index":9,"strongs":"H07775","text":"cry."}]},{"chapter":34,"text":"The face of the LORD [is] against them that do evil, to cut off       the remembrance of them from the earth.","verse":16,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H06440","text":"face"},{"index":2,"strongs":0,"text":"of the"},{"index":3,"strongs":"H03068","text":"LORD"},{"index":4,"strongs":0,"text":"is against them that"},{"index":5,"strongs":"H06213","text":"do"},{"index":6,"strongs":"H07451","text":"evil,"},{"index":7,"strongs":0,"text":"to cut"},{"index":8,"strongs":"H03772","text":"off"},{"index":9,"strongs":0,"text":"the"},{"index":10,"strongs":"H02143","text":"remembrance"},{"index":11,"strongs":0,"text":"of them from the"},{"index":12,"strongs":"H0776","text":"earth."}]},{"chapter":34,"text":"[The righteous] cry, and the LORD heareth, and delivereth them       out of all their troubles.","verse":17,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The righteous "},{"index":1,"strongs":"H06817","text":"cry,"},{"index":2,"strongs":0,"text":"and the"},{"index":3,"strongs":"H03068","text":"LORD"},{"index":4,"strongs":"H08085","text":"heareth,"},{"index":5,"strongs":0,"text":"and"},{"index":6,"strongs":"H05337","text":"delivereth"},{"index":7,"strongs":0,"text":"them out of all their"},{"index":8,"strongs":"H06869","text":"troubles."}]},{"chapter":34,"text":"The LORD [is] nigh unto them that are of a broken heart; and       saveth such as be of a contrite spirit.","verse":18,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H03068","text":"LORD"},{"index":2,"strongs":0,"text":"is"},{"index":3,"strongs":"H07138","text":"nigh"},{"index":4,"strongs":0,"text":"unto them that are of a"},{"index":5,"strongs":"H07665","text":"broken"},{"index":6,"strongs":"H03820","text":"heart;"},{"index":7,"strongs":0,"text":"and"},{"index":8,"strongs":"H03467","text":"saveth"},{"index":9,"strongs":0,"text":"such as be of a"},{"index":10,"strongs":"H01793","text":"contrite"},{"index":11,"strongs":"H07307","text":"spirit."}]},{"chapter":34,"text":"Many [are] the afflictions of the righteous: but the LORD       delivereth him out of them all.","verse":19,"book":"Psalms","reference":[{"index":0,"strongs":"H07227","text":"Many"},{"index":1,"strongs":0,"text":"are the"},{"index":2,"strongs":"H07451","text":"afflictions"},{"index":3,"strongs":0,"text":"of the"},{"index":4,"strongs":"H06662","text":"righteous:"},{"index":5,"strongs":0,"text":"but the"},{"index":6,"strongs":"H03068","text":"LORD"},{"index":7,"strongs":"H05337","text":"delivereth"},{"index":8,"strongs":0,"text":"him out of them all."}]},{"chapter":34,"text":"He keepeth all his bones: not one of them is broken.","verse":20,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He "},{"index":1,"strongs":"H08104","text":"keepeth"},{"index":2,"strongs":0,"text":"all his"},{"index":3,"strongs":"H06106","text":"bones:"},{"index":4,"strongs":0,"text":"not"},{"index":5,"strongs":"H0259","text":"one"},{"index":6,"strongs":0,"text":"of"},{"index":7,"strongs":"H02007","text":"them"},{"index":8,"strongs":0,"text":"is"},{"index":9,"strongs":"H07665","text":"broken."}]},{"chapter":34,"text":"Evil shall slay the wicked: and they that hate the righteous       shall be desolate.","verse":21,"book":"Psalms","reference":[{"index":0,"strongs":"H07451","text":"Evil"},{"index":1,"strongs":0,"text":"shall"},{"index":2,"strongs":"H04191","text":"slay"},{"index":3,"strongs":0,"text":"the"},{"index":4,"strongs":"H07563","text":"wicked:"},{"index":5,"strongs":0,"text":"and they that"},{"index":6,"strongs":"H08130","text":"hate"},{"index":7,"strongs":0,"text":"the"},{"index":8,"strongs":"H06662","text":"righteous"},{"index":9,"strongs":0,"text":"shall be"},{"index":10,"strongs":"H0816","text":"desolate."}]},{"chapter":34,"text":"The LORD redeemeth the soul of his servants: and none of them       that trust in him shall be desolate.   Psalm 35  A [Psalm] of David.","verse":22,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H03068","text":"LORD"},{"index":2,"strongs":"H06299","text":"redeemeth"},{"index":3,"strongs":0,"text":"the"},{"index":4,"strongs":"H05315","text":"soul"},{"index":5,"strongs":0,"text":"of his"},{"index":6,"strongs":"H05650","text":"servants:"},{"index":7,"strongs":0,"text":"and none of them that"},{"index":8,"strongs":"H02620","text":"trust"},{"index":9,"strongs":0,"text":"in him shall be"},{"index":10,"strongs":"H0816","text":"desolate."}]}]