[{"chapter":62,"text":"Truly my soul waiteth upon God: from him [cometh] my       salvation.","verse":1,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Truly my soul "},{"index":1,"strongs":"H05329","text":"waiteth"},{"index":2,"strongs":0,"text":"upon"},{"index":3,"strongs":"H03038","text":"God:"},{"index":4,"strongs":0,"text":"from"},{"index":5,"strongs":"H04210","text":"him"},{"index":6,"strongs":0,"text":"cometh"},{"index":7,"strongs":"H01732","text":"my"},{"index":8,"strongs":0,"text":"salvation."}]},{"chapter":62,"text":"He only [is] my rock and my salvation; [he is] my defence; I       shall not be greatly moved.","verse":2,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He only is my "},{"index":1,"strongs":"H06697","text":"rock"},{"index":2,"strongs":0,"text":"and my"},{"index":3,"strongs":"H03444","text":"salvation;"},{"index":4,"strongs":0,"text":"he is my"},{"index":5,"strongs":"H04869","text":"defence;"},{"index":6,"strongs":0,"text":"I shall not be"},{"index":7,"strongs":"H07227","text":"greatly"},{"index":8,"strongs":"H04131","text":"moved."}]},{"chapter":62,"text":"How long will ye imagine mischief against a man? ye shall be       slain all of you: as a bowing wall [shall ye be, and as] a       tottering fence.","verse":3,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"How long will ye imagine "},{"index":1,"strongs":"H02050","text":"mischief"},{"index":2,"strongs":0,"text":"against a"},{"index":3,"strongs":"H0376","text":"man?"},{"index":4,"strongs":0,"text":"ye shall be"},{"index":5,"strongs":"H07523","text":"slain"},{"index":6,"strongs":0,"text":"all of you: as a"},{"index":7,"strongs":"H05186","text":"bowing"},{"index":8,"strongs":"H07023","text":"wall"},{"index":9,"strongs":0,"text":"shall ye be, and as a"},{"index":10,"strongs":"H01760","text":"tottering"},{"index":11,"strongs":"H01447","text":"fence."}]},{"chapter":62,"text":"They only consult to cast [him] down from his excellency: they       delight in lies: they bless with their mouth, but they curse       inwardly. Selah.","verse":4,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"They only "},{"index":1,"strongs":"H03289","text":"consult"},{"index":2,"strongs":0,"text":"to cast him"},{"index":3,"strongs":"H05080","text":"down"},{"index":4,"strongs":0,"text":"from his"},{"index":5,"strongs":"H07613","text":"excellency:"},{"index":6,"strongs":0,"text":"they"},{"index":7,"strongs":"H07521","text":"delight"},{"index":8,"strongs":0,"text":"in"},{"index":9,"strongs":"H03577","text":"lies:"},{"index":10,"strongs":0,"text":"they"},{"index":11,"strongs":"H01288","text":"bless"},{"index":12,"strongs":0,"text":"with their"},{"index":13,"strongs":"H06310","text":"mouth,"},{"index":14,"strongs":0,"text":"but they"},{"index":15,"strongs":"H07043","text":"curse"},{"index":16,"strongs":"H07130","text":"inwardly."},{"index":17,"strongs":"H05542","text":"Selah."}]},{"chapter":62,"text":"My soul, wait thou only upon God; for my expectation [is] from       him.","verse":5,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"My "},{"index":1,"strongs":"H05315","text":"soul,"},{"index":2,"strongs":"H01826","text":"wait"},{"index":3,"strongs":0,"text":"thou only upon"},{"index":4,"strongs":"H0430","text":"God;"},{"index":5,"strongs":0,"text":"for my"},{"index":6,"strongs":"H08615","text":"expectation"},{"index":7,"strongs":0,"text":"is from him."}]},{"chapter":62,"text":"He only [is] my rock and my salvation: [he is] my defence; I       shall not be moved.","verse":6,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He only is my "},{"index":1,"strongs":"H06697","text":"rock"},{"index":2,"strongs":0,"text":"and my"},{"index":3,"strongs":"H03444","text":"salvation:"},{"index":4,"strongs":0,"text":"he is my"},{"index":5,"strongs":"H04869","text":"defence;"},{"index":6,"strongs":0,"text":"I shall not be"},{"index":7,"strongs":"H04131","text":"moved."}]},{"chapter":62,"text":"In God [is] my salvation and my glory: the rock of my strength,       [and] my refuge, [is] in God.","verse":7,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"In "},{"index":1,"strongs":"H0430","text":"God"},{"index":2,"strongs":0,"text":"is my"},{"index":3,"strongs":"H03468","text":"salvation"},{"index":4,"strongs":0,"text":"and my"},{"index":5,"strongs":"H03519","text":"glory:"},{"index":6,"strongs":0,"text":"the"},{"index":7,"strongs":"H06697","text":"rock"},{"index":8,"strongs":0,"text":"of my"},{"index":9,"strongs":"H05797","text":"strength,"},{"index":10,"strongs":0,"text":"and my"},{"index":11,"strongs":"H04268","text":"refuge,"},{"index":12,"strongs":0,"text":"is in"},{"index":13,"strongs":"H0430","text":"God."}]},{"chapter":62,"text":"Trust in him at all times; [ye] people, pour out your heart       before him: God [is] a refuge for us. Selah.","verse":8,"book":"Psalms","reference":[{"index":0,"strongs":"H0982","text":"Trust"},{"index":1,"strongs":0,"text":"in him at all"},{"index":2,"strongs":"H06256","text":"times;"},{"index":3,"strongs":0,"text":"ye"},{"index":4,"strongs":"H05971","text":"people,"},{"index":5,"strongs":0,"text":"pour"},{"index":6,"strongs":"H08210","text":"out"},{"index":7,"strongs":0,"text":"your"},{"index":8,"strongs":"H03824","text":"heart"},{"index":9,"strongs":"H06440","text":"before"},{"index":10,"strongs":0,"text":"him:"},{"index":11,"strongs":"H0430","text":"God"},{"index":12,"strongs":0,"text":"is a"},{"index":13,"strongs":"H04268","text":"refuge"},{"index":14,"strongs":0,"text":"for us."},{"index":15,"strongs":"H05542","text":"Selah."}]},{"chapter":62,"text":"Surely men of low degree [are] vanity, [and] men of high degree       [are] a lie: to be laid in the balance, they [are] altogether       [lighter] than vanity.","verse":9,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Surely "},{"index":1,"strongs":"H01121","text":"men"},{"index":2,"strongs":"H0120","text":"of"},{"index":3,"strongs":0,"text":"low degree are"},{"index":4,"strongs":"H01892","text":"vanity,"},{"index":5,"strongs":0,"text":"and"},{"index":6,"strongs":"H0376","text":"men"},{"index":7,"strongs":0,"text":"of high degree are a"},{"index":8,"strongs":"H03577","text":"lie:"},{"index":9,"strongs":0,"text":"to be"},{"index":10,"strongs":"H05927","text":"laid"},{"index":11,"strongs":0,"text":"in the"},{"index":12,"strongs":"H03976","text":"balance,"},{"index":13,"strongs":0,"text":"they are"},{"index":14,"strongs":"H03162","text":"altogether"},{"index":15,"strongs":0,"text":"lighter than"},{"index":16,"strongs":"H01892","text":"vanity."}]},{"chapter":62,"text":"Trust not in oppression, and become not vain in robbery: if       riches increase, set not your heart [upon them.]","verse":10,"book":"Psalms","reference":[{"index":0,"strongs":"H0982","text":"Trust"},{"index":1,"strongs":0,"text":"not in"},{"index":2,"strongs":"H06233","text":"oppression,"},{"index":3,"strongs":0,"text":"and become not"},{"index":4,"strongs":"H01891","text":"vain"},{"index":5,"strongs":0,"text":"in"},{"index":6,"strongs":"H01498","text":"robbery:"},{"index":7,"strongs":0,"text":"if"},{"index":8,"strongs":"H02428","text":"riches"},{"index":9,"strongs":"H05107","text":"increase,"},{"index":10,"strongs":"H07896","text":"set"},{"index":11,"strongs":0,"text":"not your"},{"index":12,"strongs":"H03820","text":"heart"},{"index":13,"strongs":0,"text":"upon them."}]},{"chapter":62,"text":"God hath spoken once; twice have I heard this; that power       [belongeth] unto God.","verse":11,"book":"Psalms","reference":[{"index":0,"strongs":"H0430","text":"God"},{"index":1,"strongs":0,"text":"hath"},{"index":2,"strongs":"H01696","text":"spoken"},{"index":3,"strongs":"H0259","text":"once;"},{"index":4,"strongs":"H08147","text":"twice"},{"index":5,"strongs":0,"text":"have I"},{"index":6,"strongs":"H08085","text":"heard"},{"index":7,"strongs":"H02098","text":"this;"},{"index":8,"strongs":0,"text":"that"},{"index":9,"strongs":"H05797","text":"power"},{"index":10,"strongs":0,"text":"belongeth unto"},{"index":11,"strongs":"H0430","text":"God."}]},{"chapter":62,"text":"Also unto thee, O Lord, [belongeth] mercy: for thou renderest       to every man according to his work.   Psalm 63  A Psalm of David, when he was in the wilderness of Judah.","verse":12,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Also unto thee, O "},{"index":1,"strongs":"H0136","text":"Lord,"},{"index":2,"strongs":0,"text":"belongeth"},{"index":3,"strongs":"H02617","text":"mercy:"},{"index":4,"strongs":0,"text":"for thou"},{"index":5,"strongs":"H07999","text":"renderest"},{"index":6,"strongs":0,"text":"to every"},{"index":7,"strongs":"H0376","text":"man"},{"index":8,"strongs":0,"text":"according to his"},{"index":9,"strongs":"H04639","text":"work."}]}]