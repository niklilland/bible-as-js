strongs":"H06094","text":"wounds."}]},{"chapter":147,"text":"He telleth the number of the stars; he calleth them all by       [their] names.","verse":4,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He "},{"index":1,"strongs":"H04487","text":"telleth"},{"index":2,"strongs":0,"text":"the"},{"index":3,"strongs":"H04557","text":"number"},{"index":4,"strongs":0,"text":"of the"},{"index":5,"strongs":"H03556","text":"stars;"},{"index":6,"strongs":0,"text":"he"},{"index":7,"strongs":"H07121","text":"calleth"},{"index":8,"strongs":0,"text":"them all by their"},{"index":9,"strongs":"H08034","text":"names."}]},{"chapter":147,"text":"Great [is] our Lord, and of great power: his understanding [is]       infinite.","verse":5,"book":"Psalms","reference":[{"index":0,"strongs":"H01419","text":"Great"},{"index":1,"strongs":0,"text":"is our"},{"index":2,"strongs":"H0113","text":"Lord,"},{"index":3,"strongs":0,"text":"and of"},{"index":4,"strongs":"H07227","text":"great"},{"index":5,"strongs":"H03581","text":"power:"},{"index":6,"strongs":0,"text":"his"},{"index":7,"strongs":"H08394","text":"understanding"},{"index":8,"strongs":0,"text":"is"},{"index":9,"strongs":"H04557","text":"infinite."}]},{"chapter":147,"text":"The LORD lifteth up the meek: he casteth the wicked down to the       ground.","verse":6,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H03068","text":"LORD"},{"index":2,"strongs":0,"text":"lifteth"},{"index":3,"strongs":"H05749","text":"up"},{"index":4,"strongs":0,"text":"the"},{"index":5,"strongs":"H06035","text":"meek:"},{"index":6,"strongs":0,"text":"he"},{"index":7,"strongs":"H08213","text":"casteth"},{"index":8,"strongs":0,"text":"the"},{"index":9,"strongs":"H07563","text":"wicked"},{"index":10,"strongs":"H08213","text":"down"},{"index":11,"strongs":0,"text":"to the"},{"index":12,"strongs":"H0776","text":"ground."}]},{"chapter":147,"text":"Sing unto the LORD with thanksgiving; sing praise upon the harp       unto our God:","verse":7,"book":"Psalms","reference":[{"index":0,"strongs":"H06030","text":"Sing"},{"index":1,"strongs":0,"text":"unto the"},{"index":2,"strongs":"H03068","text":"LORD"},{"index":3,"strongs":0,"text":"with"},{"index":4,"strongs":"H08426","text":"thanksgiving;"},{"index":5,"strongs":0,"text":"sing"},{"index":6,"strongs":"H02167","text":"praise"},{"index":7,"strongs":0,"text":"upon the"},{"index":8,"strongs":"H03658","text":"harp"},{"index":9,"strongs":0,"text":"unto our"},{"index":10,"strongs":"H0430","text":"God:"}]},{"chapter":147,"text":"Who covereth the heaven with clouds, who prepareth rain for the       earth, who maketh grass to grow upon the mountains.","verse":8,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Who "},{"index":1,"strongs":"H03680","text":"covereth"},{"index":2,"strongs":0,"text":"the"},{"index":3,"strongs":"H08064","text":"heaven"},{"index":4,"strongs":0,"text":"with"},{"index":5,"strongs":"H05645","text":"clouds,"},{"index":6,"strongs":0,"text":"who"},{"index":7,"strongs":"H03559","text":"prepareth"},{"index":8,"strongs":"H04306","text":"rain"},{"index":9,"strongs":0,"text":"for the"},{"index":10,"strongs":"H0776","text":"earth,"},{"index":11,"strongs":0,"text":"who maketh"},{"index":12,"strongs":"H02682","text":"grass"},{"index":13,"strongs":0,"text":"to"},{"index":14,"strongs":"H06779","text":"grow"},{"index":15,"strongs":0,"text":"upon the"},{"index":16,"strongs":"H02022","text":"mountains."}]},{"chapter":147,"text":"He giveth to the beast his food, [and] to the young ravens       which cry.","verse":9,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He "},{"index":1,"strongs":"H05414","text":"giveth"},{"index":2,"strongs":0,"text":"to the"},{"index":3,"strongs":"H0929","text":"beast"},{"index":4,"strongs":0,"text":"his"},{"index":5,"strongs":"H03899","text":"food,"},{"index":6,"strongs":0,"text":"and to the"},{"index":7,"strongs":"H01121","text":"young"},{"index":8,"strongs":"H06158","text":"ravens"},{"index":9,"strongs":0,"text":"which"},{"index":10,"strongs":"H07121","text":"cry."}]},{"chapter":147,"text":"He delighteth not in the strength of the horse: he taketh not       pleasure in the legs of a man.","verse":10,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He "},{"index":1,"strongs":"H02654","text":"delighteth"},{"index":2,"strongs":0,"text":"not in the"},{"index":3,"strongs":"H01369","text":"strength"},{"index":4,"strongs":0,"text":"of the"},{"index":5,"strongs":"H05483","text":"horse:"},{"index":6,"strongs":0,"text":"he taketh not"},{"index":7,"strongs":"H07521","text":"pleasure"},{"index":8,"strongs":0,"text":"in the"},{"index":9,"strongs":"H07785","text":"legs"},{"index":10,"strongs":0,"text":"of a"},{"index":11,"strongs":"H0376","text":"man."}]},{"chapter":147,"text":"The LORD taketh pleasure in them that fear him, in those that       hope in his mercy.","verse":11,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H03068","text":"LORD"},{"index":2,"strongs":0,"text":"taketh"},{"index":3,"strongs":"H07521","text":"pleasure"},{"index":4,"strongs":0,"text":"in them that"},{"index":5,"strongs":"H03373","text":"fear"},{"index":6,"strongs":0,"text":"him, in those that"},{"index":7,"strongs":"H03176","text":"hope"},{"index":8,"strongs":0,"text":"in his"},{"index":9,"strongs":"H02617","text":"mercy."}]},{"chapter":147,"text":"Praise the LORD, O Jerusalem; praise thy God, O Zion.","verse":12,"book":"Psalms","reference":[{"index":0,"strongs":"H07623","text":"Praise"},{"index":1,"strongs":0,"text":"the"},{"index":2,"strongs":"H03068","text":"LORD,"},{"index":3,"strongs":0,"text":"O"},{"index":4,"strongs":"H03389","text":"Jerusalem;"},{"index":5,"strongs":"H01984","text":"praise"},{"index":6,"strongs":0,"text":"thy"},{"index":7,"strongs":"H0430","text":"God,"},{"index":8,"strongs":0,"text":"O"},{"index":9,"strongs":"H06726","text":"Zion."}]},{"chapter":147,"text":"For he hath strengthened the bars of thy gates; he hath       blessed thy children within thee.","verse":13,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"For he hath "},{"index":1,"strongs":"H02388","text":"strengthened"},{"index":2,"strongs":0,"text":"the"},{"index":3,"strongs":"H01280","text":"bars"},{"index":4,"strongs":0,"text":"of thy"},{"index":5,"strongs":"H08179","text":"gates;"},{"index":6,"strongs":0,"text":"he hath"},{"index":7,"strongs":"H01288","text":"blessed"},{"index":8,"strongs":0,"text":"thy"},{"index":9,"strongs":"H01121","text":"children"},{"index":10,"strongs":"H07130","text":"within"},{"index":11,"strongs":0,"text":"thee."}]},{"chapter":147,"text":"He maketh peace [in] thy borders, [and] filleth thee with the       finest of the wheat.","verse":14,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He "},{"index":1,"strongs":"H07760","text":"maketh"},{"index":2,"strongs":"H07965","text":"peace"},{"index":3,"strongs":0,"text":"in thy"},{"index":4,"strongs":"H01366","text":"borders,"},{"index":5,"strongs":0,"text":"and"},{"index":6,"strongs":"H07646","text":"filleth"},{"index":7,"strongs":0,"text":"thee with the"},{"index":8,"strongs":"H02459","text":"finest"},{"index":9,"strongs":0,"text":"of the"},{"index":10,"strongs":"H02406","text":"wheat."}]},{"chapter":147,"text":"He sendeth forth his commandment [upon] earth: his word       runneth very swiftly.","verse":15,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He sendeth "},{"index":1,"strongs":"H07971","text":"forth"},{"index":2,"strongs":0,"text":"his"},{"index":3,"strongs":"H0565","text":"commandment"},{"index":4,"strongs":0,"text":"upon"},{"index":5,"strongs":"H0776","text":"earth:"},{"index":6,"strongs":0,"text":"his"},{"index":7,"strongs":"H01697","text":"word"},{"index":8,"strongs":"H07323","text":"runneth"},{"index":9,"strongs":0,"text":"very"},{"index":10,"strongs":"H04120","text":"swiftly."}]},{"chapter":147,"text":"He giveth snow like wool: he scattereth the hoar frost like       ashes.","verse":16,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He "},{"index":1,"strongs":"H05414","text":"giveth"},{"index":2,"strongs":"H07950","text":"snow"},{"index":3,"strongs":0,"text":"like"},{"index":4,"strongs":"H06785","text":"wool:"},{"index":5,"strongs":0,"text":"he"},{"index":6,"strongs":"H06340","text":"scattereth"},{"index":7,"strongs":0,"text":"the"},{"index":8,"strongs":"H03713","text":"hoarfrost"},{"index":9,"strongs":0,"text":"like"},{"index":10,"strongs":"H0665","text":"ashes."}]},{"chapter":147,"text":"He casteth forth his ice like morsels: who can stand before       his cold?","verse":17,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He casteth "},{"index":1,"strongs":"H07993","text":"forth"},{"index":2,"strongs":0,"text":"his"},{"index":3,"strongs":"H07140","text":"ice"},{"index":4,"strongs":0,"text":"like"},{"index":5,"strongs":"H06595","text":"morsels:"},{"index":6,"strongs":0,"text":"who can"},{"index":7,"strongs":"H05975","text":"stand"},{"index":8,"strongs":"H06440","text":"before"},{"index":9,"strongs":0,"text":"his"},{"index":10,"strongs":"H07135","text":"cold?"}]},{"chapter":147,"text":"He sendeth out his word, and melteth them: he causeth his wind       to blow, [and] the waters flow.","verse":18,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He sendeth "},{"index":1,"strongs":"H07971","text":"out"},{"index":2,"strongs":0,"text":"his"},{"index":3,"strongs":"H01697","text":"word,"},{"index":4,"strongs":0,"text":"and"},{"index":5,"strongs":"H04529","text":"melteth"},{"index":6,"strongs":0,"text":"them: he causeth his"},{"index":7,"strongs":"H07307","text":"wind"},{"index":8,"strongs":0,"text":"to"},{"index":9,"strongs":"H05380","text":"blow,"},{"index":10,"strongs":0,"text":"and the"},{"index":11,"strongs":"H04325","text":"waters"},{"index":12,"strongs":"H05140","text":"flow."}]},{"chapter":147,"text":"He sheweth his word unto Jacob, his statutes and his judgments       unto Israel.","verse":19,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He "},{"index":1,"strongs":"H05046","text":"sheweth"},{"index":2,"strongs":0,"text":"his"},{"index":3,"strongs":"H01697","text":"word"},{"index":4,"strongs":"H01697","text":"unto"},{"index":5,"strongs":0,"text":"Jacob,"},{"index":6,"strongs":"H03290","text":"his"},{"index":7,"strongs":0,"text":"statutes"},{"index":8,"strongs":"H02706","text":"and"},{"index":9,"strongs":0,"text":"his judgments"},{"index":10,"strongs":"H04941","text":"unto"},{"index":11,"strongs":0,"text":"Israel."}]},{"chapter":147,"text":"He hath not dealt so with any nation: and [as for his]       judgments, they have not known them. Praise ye the LORD.   Psalm 148","verse":20,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He hath not "},{"index":1,"strongs":"H06213","text":"dealt"},{"index":2,"strongs":0,"text":"so with any"},{"index":3,"strongs":"H01471","text":"nation:"},{"index":4,"strongs":0,"text":"and as for his"},{"index":5,"strongs":"H04941","text":"judgments,"},{"index":6,"strongs":0,"text":"they have not"},{"index":7,"strongs":"H03045","text":"known"},{"index":8,"strongs":0,"text":"them."},{"index":9,"strongs":"H01984","text":"Praise"},{"index":10,"strongs":0,"text":"ye the"},{"index":11,"strongs":"H03050","text":"LORD."}]}]