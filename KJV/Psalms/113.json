[{"chapter":113,"text":"Praise ye the LORD. Praise, O ye servants of the LORD, praise       the name of the LORD.","verse":1,"book":"Psalms","reference":[{"index":0,"strongs":"H01984","text":"Praise"},{"index":1,"strongs":0,"text":"ye the"},{"index":2,"strongs":"H03050","text":"LORD."},{"index":3,"strongs":"H01984","text":"Praise,"},{"index":4,"strongs":0,"text":"O ye"},{"index":5,"strongs":"H05650","text":"servants"},{"index":6,"strongs":0,"text":"of the"},{"index":7,"strongs":"H03068","text":"LORD,"},{"index":8,"strongs":"H01984","text":"praise"},{"index":9,"strongs":0,"text":"the"},{"index":10,"strongs":"H08034","text":"name"},{"index":11,"strongs":0,"text":"of the"},{"index":12,"strongs":"H03068","text":"LORD."}]},{"chapter":113,"text":"Blessed be the name of the LORD from this time forth and for       evermore.","verse":2,"book":"Psalms","reference":[{"index":0,"strongs":"H01288","text":"Blessed"},{"index":1,"strongs":0,"text":"be the"},{"index":2,"strongs":"H08034","text":"name"},{"index":3,"strongs":0,"text":"of the"},{"index":4,"strongs":"H03068","text":"LORD"},{"index":5,"strongs":0,"text":"from this time"},{"index":6,"strongs":"H06258","text":"forth"},{"index":7,"strongs":0,"text":"and"},{"index":8,"strongs":"H05704","text":"for"},{"index":9,"strongs":"H05769","text":"evermore."}]},{"chapter":113,"text":"From the rising of the sun unto the going down of the same the       LORD'S name [is] to be praised.","verse":3,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"From the "},{"index":1,"strongs":"H04217","text":"rising"},{"index":2,"strongs":0,"text":"of the"},{"index":3,"strongs":"H08121","text":"sun"},{"index":4,"strongs":0,"text":"unto the going"},{"index":5,"strongs":"H03996","text":"down"},{"index":6,"strongs":0,"text":"of the same the"},{"index":7,"strongs":"H03068","text":"LORD's"},{"index":8,"strongs":"H08034","text":"name"},{"index":9,"strongs":0,"text":"is to be"},{"index":10,"strongs":"H01984","text":"praised."}]},{"chapter":113,"text":"The LORD [is] high above all nations, [and] his glory above the       heavens.","verse":4,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H03068","text":"LORD"},{"index":2,"strongs":0,"text":"is"},{"index":3,"strongs":"H07311","text":"high"},{"index":4,"strongs":0,"text":"above all"},{"index":5,"strongs":"H01471","text":"nations,"},{"index":6,"strongs":0,"text":"and his"},{"index":7,"strongs":"H03519","text":"glory"},{"index":8,"strongs":0,"text":"above the"},{"index":9,"strongs":"H08064","text":"heavens."}]},{"chapter":113,"text":"Who [is] like unto the LORD our God, who dwelleth on high,","verse":5,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Who is like unto the "},{"index":1,"strongs":"H03068","text":"LORD"},{"index":2,"strongs":0,"text":"our"},{"index":3,"strongs":"H0430","text":"God,"},{"index":4,"strongs":0,"text":"who"},{"index":5,"strongs":"H03427","text":"dwelleth"},{"index":6,"strongs":0,"text":"on"},{"index":7,"strongs":"H01361","text":"high,"}]},{"chapter":113,"text":"Who humbleth [himself] to behold [the things that are] in       heaven, and in the earth!","verse":6,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Who "},{"index":1,"strongs":"H08213","text":"humbleth"},{"index":2,"strongs":0,"text":"himself to"},{"index":3,"strongs":"H07200","text":"behold"},{"index":4,"strongs":0,"text":"the things that are in"},{"index":5,"strongs":"H08064","text":"heaven,"},{"index":6,"strongs":0,"text":"and in the"},{"index":7,"strongs":"H0776","text":"earth!"}]},{"chapter":113,"text":"He raiseth up the poor out of the dust, [and] lifteth the needy       out of the dunghill;","verse":7,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He raiseth "},{"index":1,"strongs":"H06965","text":"up"},{"index":2,"strongs":0,"text":"the"},{"index":3,"strongs":"H01800","text":"poor"},{"index":4,"strongs":0,"text":"out of the"},{"index":5,"strongs":"H06083","text":"dust,"},{"index":6,"strongs":0,"text":"and"},{"index":7,"strongs":"H07311","text":"lifteth"},{"index":8,"strongs":0,"text":"the"},{"index":9,"strongs":"H0034","text":"needy"},{"index":10,"strongs":0,"text":"out of the"},{"index":11,"strongs":"H0830","text":"dunghill;"}]},{"chapter":113,"text":"That he may set [him] with princes, [even] with the princes of       his people.","verse":8,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"That he may "},{"index":1,"strongs":"H03427","text":"set"},{"index":2,"strongs":0,"text":"him with"},{"index":3,"strongs":"H05081","text":"princes,"},{"index":4,"strongs":0,"text":"even with the"},{"index":5,"strongs":"H05081","text":"princes"},{"index":6,"strongs":0,"text":"of his"},{"index":7,"strongs":"H05971","text":"people."}]},{"chapter":113,"text":"He maketh the barren woman to keep house, [and to be] a joyful       mother of children. Praise ye the LORD.   Psalm 114","verse":9,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He maketh the barren "},{"index":1,"strongs":"H06135","text":"woman"},{"index":2,"strongs":0,"text":"to"},{"index":3,"strongs":"H03427","text":"keep"},{"index":4,"strongs":"H01004","text":"house,"},{"index":5,"strongs":0,"text":"and to be a"},{"index":6,"strongs":"H08056","text":"joyful"},{"index":7,"strongs":"H0517","text":"mother"},{"index":8,"strongs":0,"text":"of"},{"index":9,"strongs":"H01121","text":"children."},{"index":10,"strongs":"H01984","text":"Praise"},{"index":11,"strongs":0,"text":"ye the"},{"index":12,"strongs":"H03050","text":"LORD."}]}]