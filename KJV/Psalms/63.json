trongs":"H02896","text":"better"},{"index":4,"strongs":0,"text":"than"},{"index":5,"strongs":"H02416","text":"life,"},{"index":6,"strongs":0,"text":"my"},{"index":7,"strongs":"H08193","text":"lips"},{"index":8,"strongs":0,"text":"shall"},{"index":9,"strongs":"H07623","text":"praise"},{"index":10,"strongs":0,"text":"thee."}]},{"chapter":63,"text":"Thus will I bless thee while I live: I will lift up my hands in       thy name.","verse":4,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Thus will I "},{"index":1,"strongs":"H01288","text":"bless"},{"index":2,"strongs":0,"text":"thee while I"},{"index":3,"strongs":"H02416","text":"live:"},{"index":4,"strongs":0,"text":"I will lift"},{"index":5,"strongs":"H05375","text":"up"},{"index":6,"strongs":0,"text":"my"},{"index":7,"strongs":"H03709","text":"hands"},{"index":8,"strongs":0,"text":"in thy"},{"index":9,"strongs":"H08034","text":"name."}]},{"chapter":63,"text":"My soul shall be satisfied as [with] marrow and fatness; and my       mouth shall praise [thee] with joyful lips:","verse":5,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"My "},{"index":1,"strongs":"H05315","text":"soul"},{"index":2,"strongs":0,"text":"shall be"},{"index":3,"strongs":"H07646","text":"satisfied"},{"index":4,"strongs":0,"text":"as with"},{"index":5,"strongs":"H02459","text":"marrow"},{"index":6,"strongs":0,"text":"and"},{"index":7,"strongs":"H01880","text":"fatness;"},{"index":8,"strongs":0,"text":"and my"},{"index":9,"strongs":"H06310","text":"mouth"},{"index":10,"strongs":0,"text":"shall"},{"index":11,"strongs":"H01984","text":"praise"},{"index":12,"strongs":0,"text":"thee with"},{"index":13,"strongs":"H07445","text":"joyful"},{"index":14,"strongs":"H08193","text":"lips:"}]},{"chapter":63,"text":"When I remember thee upon my bed, [and] meditate on thee in the       [night] watches.","verse":6,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"When I "},{"index":1,"strongs":"H02142","text":"remember"},{"index":2,"strongs":0,"text":"thee upon my"},{"index":3,"strongs":"H03326","text":"bed,"},{"index":4,"strongs":0,"text":"and"},{"index":5,"strongs":"H01897","text":"meditate"},{"index":6,"strongs":0,"text":"on thee in the night"},{"index":7,"strongs":"H0821","text":"watches."}]},{"chapter":63,"text":"Because thou hast been my help, therefore in the shadow of thy       wings will I rejoice.","verse":7,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Because thou hast been my "},{"index":1,"strongs":"H05833","text":"help,"},{"index":2,"strongs":0,"text":"therefore in the"},{"index":3,"strongs":"H06738","text":"shadow"},{"index":4,"strongs":0,"text":"of thy"},{"index":5,"strongs":"H03671","text":"wings"},{"index":6,"strongs":0,"text":"will I"},{"index":7,"strongs":"H07442","text":"rejoice."}]},{"chapter":63,"text":"My soul followeth hard after thee: thy right hand upholdeth me.","verse":8,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"My "},{"index":1,"strongs":"H05315","text":"soul"},{"index":2,"strongs":0,"text":"followeth"},{"index":3,"strongs":"H01692","text":"hard"},{"index":4,"strongs":"H0310","text":"after"},{"index":5,"strongs":0,"text":"thee: thy right"},{"index":6,"strongs":"H03225","text":"hand"},{"index":7,"strongs":"H08551","text":"upholdeth"},{"index":8,"strongs":0,"text":"me."}]},{"chapter":63,"text":"But those [that] seek my soul, to destroy [it,] shall go into       the lower parts of the earth.","verse":9,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"But those that "},{"index":1,"strongs":"H01245","text":"seek"},{"index":2,"strongs":0,"text":"my"},{"index":3,"strongs":"H05315","text":"soul,"},{"index":4,"strongs":0,"text":"to"},{"index":5,"strongs":"H07722","text":"destroy"},{"index":6,"strongs":0,"text":"it, shall"},{"index":7,"strongs":"H0935","text":"go"},{"index":8,"strongs":0,"text":"into the lower"},{"index":9,"strongs":"H08482","text":"parts"},{"index":10,"strongs":0,"text":"of the"},{"index":11,"strongs":"H0776","text":"earth."}]},{"chapter":63,"text":"They shall fall by the sword: they shall be a portion for foxes.","verse":10,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"They shall "},{"index":1,"strongs":"H05064","text":"fall"},{"index":2,"strongs":0,"text":"by the"},{"index":3,"strongs":"H02719","text":"sword:"},{"index":4,"strongs":"H03027","text":"they"},{"index":5,"strongs":0,"text":"shall be a portion"},{"index":6,"strongs":"H04521","text":"for"},{"index":7,"strongs":0,"text":"foxes."}]},{"chapter":63,"text":"But the king shall rejoice in God; every one that sweareth by       him shall glory: but the mouth of them that speak lies shall be       stopped.   Psalm 64  To the chief Musician, A Psalm of David.","verse":11,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"But the "},{"index":1,"strongs":"H04428","text":"king"},{"index":2,"strongs":0,"text":"shall"},{"index":3,"strongs":"H08055","text":"rejoice"},{"index":4,"strongs":0,"text":"in"},{"index":5,"strongs":"H0430","text":"God;"},{"index":6,"strongs":0,"text":"every one that"},{"index":7,"strongs":"H07650","text":"sweareth"},{"index":8,"strongs":0,"text":"by him shall"},{"index":9,"strongs":"H01984","text":"glory:"},{"index":10,"strongs":0,"text":"but the"},{"index":11,"strongs":"H06310","text":"mouth"},{"index":12,"strongs":0,"text":"of them that"},{"index":13,"strongs":"H01696","text":"speak"},{"index":14,"strongs":"H08267","text":"lies"},{"index":15,"strongs":0,"text":"shall be"},{"index":16,"strongs":"H05534","text":"stopped."}]}]