ies.","verse":7,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Mine "},{"index":1,"strongs":"H05869","text":"eye"},{"index":2,"strongs":0,"text":"is"},{"index":3,"strongs":"H06244","text":"consumed"},{"index":4,"strongs":0,"text":"because of"},{"index":5,"strongs":"H03708","text":"grief;"},{"index":6,"strongs":0,"text":"it waxeth"},{"index":7,"strongs":"H06275","text":"old"},{"index":8,"strongs":0,"text":"because of all mine"},{"index":9,"strongs":"H06887","text":"enemies."}]},{"chapter":6,"text":"Depart from me, all ye workers of iniquity; for the LORD hath       heard the voice of my weeping.","verse":8,"book":"Psalms","reference":[{"index":0,"strongs":"H05493","text":"Depart"},{"index":1,"strongs":0,"text":"from me, all ye"},{"index":2,"strongs":"H06466","text":"workers"},{"index":3,"strongs":0,"text":"of"},{"index":4,"strongs":"H0205","text":"iniquity;"},{"index":5,"strongs":0,"text":"for the"},{"index":6,"strongs":"H03068","text":"LORD"},{"index":7,"strongs":0,"text":"hath"},{"index":8,"strongs":"H08085","text":"heard"},{"index":9,"strongs":0,"text":"the"},{"index":10,"strongs":"H06963","text":"voice"},{"index":11,"strongs":0,"text":"of my"},{"index":12,"strongs":"H01065","text":"weeping."}]},{"chapter":6,"text":"The LORD hath heard my supplication; the LORD will receive my       prayer.","verse":9,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H03068","text":"LORD"},{"index":2,"strongs":0,"text":"hath"},{"index":3,"strongs":"H08085","text":"heard"},{"index":4,"strongs":0,"text":"my"},{"index":5,"strongs":"H08467","text":"supplication;"},{"index":6,"strongs":0,"text":"the"},{"index":7,"strongs":"H03068","text":"LORD"},{"index":8,"strongs":0,"text":"will"},{"index":9,"strongs":"H03947","text":"receive"},{"index":10,"strongs":0,"text":"my"},{"index":11,"strongs":"H08605","text":"prayer."}]},{"chapter":6,"text":"Let all mine enemies be ashamed and sore vexed: let them return       [and] be ashamed suddenly.   