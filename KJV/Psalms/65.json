[{"chapter":65,"text":"Praise waiteth for thee, O God, in Sion: and unto thee shall       the vow be performed.","verse":1,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Praise waiteth for "},{"index":1,"strongs":"H05329","text":"thee,"},{"index":2,"strongs":0,"text":"O"},{"index":3,"strongs":"H04210","text":"God,"},{"index":4,"strongs":0,"text":"in"},{"index":5,"strongs":"H07892","text":"Sion:"},{"index":6,"strongs":0,"text":"and"},{"index":7,"strongs":"H01732","text":"unto"},{"index":8,"strongs":"H08416","text":"thee"},{"index":9,"strongs":"H01747","text":"shall"},{"index":10,"strongs":0,"text":"the vow be"},{"index":11,"strongs":"H0430","text":"performed."}]},{"chapter":65,"text":"O thou that hearest prayer, unto thee shall all flesh come.","verse":2,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"O thou that "},{"index":1,"strongs":"H08085","text":"hearest"},{"index":2,"strongs":"H08605","text":"prayer,"},{"index":3,"strongs":0,"text":"unto thee shall all"},{"index":4,"strongs":"H01320","text":"flesh"},{"index":5,"strongs":"H0935","text":"come."}]},{"chapter":65,"text":"Iniquities prevail against me: [as for] our transgressions, thou       shalt purge them away.","verse":3,"book":"Psalms","reference":[{"index":0,"strongs":"H05771","text":"Iniquities"},{"index":1,"strongs":"H01697","text":"prevail"},{"index":2,"strongs":"H01396","text":"against"},{"index":3,"strongs":0,"text":"me: as for our transgressions,"},{"index":4,"strongs":"H06588","text":"thou"},{"index":5,"strongs":0,"text":"shalt purge them away."}]},{"chapter":65,"text":"Blessed [is the man whom] thou choosest, and causest to approach       [unto thee, that] he may dwell in thy courts: we shall be       satisfied with the goodness of thy house, [even] of thy holy       temple.","verse":4,"book":"Psalms","reference":[{"index":0,"strongs":"H0835","text":"Blessed"},{"index":1,"strongs":0,"text":"is the man whom thou"},{"index":2,"strongs":"H0977","text":"choosest,"},{"index":3,"st