 Song [or] Psalm.","verse":13,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H03733","text":"pastures"},{"index":2,"strongs":0,"text":"are"},{"index":3,"strongs":"H03847","text":"clothed"},{"index":4,"strongs":0,"text":"with"},{"index":5,"strongs":"H06629","text":"flocks;"},{"index":6,"strongs":0,"text":"the"},{"index":7,"strongs":"H06010","text":"valleys"},{"index":8,"strongs":0,"text":"also are covered"},{"index":9,"strongs":"H05848","text":"over"},{"index":10,"strongs":0,"text":"with"},{"index":11,"strongs":"H01250","text":"corn;"},{"index":12,"strongs":0,"text":"they shout for"},{"index":13,"strongs":"H07321","text":"joy,"},{"index":14,"strongs":0,"text":"they also"},{"index":15,"strongs":"H07891","text":"sing."}]}]