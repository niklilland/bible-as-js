[{"chapter":143,"text":"Hear my prayer, O LORD, give ear to my supplications: in thy       faithfulness answer me, [and] in thy righteousness.","verse":1,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Hear "},{"index":1,"strongs":"H04210","text":"my"},{"index":2,"strongs":0,"text":"prayer,"},{"index":3,"strongs":"H01732","text":"O"},{"index":4,"strongs":"H08085","text":"LORD,"},{"index":5,"strongs":0,"text":"give"},{"index":6,"strongs":"H08605","text":"ear"},{"index":7,"strongs":0,"text":"to"},{"index":8,"strongs":"H03068","text":"my"},{"index":9,"strongs":0,"text":"supplications:"},{"index":10,"strongs":"H0238","text":"in"},{"index":11,"strongs":0,"text":"thy faithfulness"},{"index":12,"strongs":"H08469","text":"answer"},{"index":13,"strongs":0,"text":"me, and"},{"index":14,"strongs":"H0530","text":"in"},{"index":15,"strongs":"H06030","text":"thy"},{"index":16,"strongs":0,"text":"righteousness."}]},{"chapter":143,"text":"And enter not into judgment with thy servant: for in thy sight       shall no man living be justified.","verse":2,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"And "},{"index":1,"strongs":"H0935","text":"enter"},{"index":2,"strongs":0,"text":"not into"},{"index":3,"strongs":"H04941","text":"judgment"},{"index":4,"strongs":0,"text":"with thy"},{"index":5,"strongs":"H05650","text":"servant:"},{"index":6,"strongs":0,"text":"for in thy"},{"index":7,"strongs":"H06440","text":"sight"},{"index":8,"strongs":0,"text":"shall no man"},{"index":9,"strongs":"H02416","text":"living"},{"index":10,"strongs":0,"text":"be"},{"index":11,"strongs":"H06663","text":"justified."}]},{"chapter":143,"text":"For the enemy hath persecuted my soul; he hath smitten my life       down to the ground; he hath made me to dwell in darkness, as       those that have been long dead.","verse":3,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"For the "},{"index":1,"strongs":"H0341","text":"enemy"},{"index":2,"strongs":0,"text":"hath"},{"index":3,"s