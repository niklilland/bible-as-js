index":13,"strongs":"H04639","text":"work"},{"index":14,"strongs":0,"text":"of thy"},{"index":15,"strongs":"H03027","text":"hands."}]},{"chapter":143,"text":"I stretch forth my hands unto thee: my soul [thirsteth] after       thee, as a thirsty land. Selah.","verse":6,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"I stretch "},{"index":1,"strongs":"H06566","text":"forth"},{"index":2,"strongs":0,"text":"my"},{"index":3,"strongs":"H03027","text":"hands"},{"index":4,"strongs":0,"text":"unto thee: my"},{"index":5,"strongs":"H05315","text":"soul"},{"index":6,"strongs":0,"text":"thirsteth after thee, as a"},{"index":7,"strongs":"H05889","text":"thirsty"},{"index":8,"strongs":"H0776","text":"land."},{"index":9,"strongs":"H05542","text":"Selah."}]},{"chapter":143,"text":"Hear me speedily, O LORD: my spirit faileth: hide not thy face       from me, lest I be like unto them that go down into the pit.","verse":7,"book":"Psalms","reference":[{"index":0,"strongs":"H06030","text":"Hear"},{"index":1,"strongs":0,"text":"me"},{"index":2,"strongs":"H04118","text":"speedily,"},{"index":3,"strongs":0,"text":"O"},{"index":4,"strongs":"H03068","text":"LORD:"},{"index":5,"strongs":0,"text":"my"},{"index":6,"strongs":"H07307","text":"spirit"},{"index":7,"strongs":"H03615","text":"faileth:"},{"index":8,"strongs":"H05641","text":"hide"},{"index":9,"strongs":0,"text":"not thy"},{"index":10,"strongs":"H06440","text":"face"},{"index":11,"strongs":0,"text":"from me, lest I be"},{"index":12,"strongs":"H04911","text":"like"},{"index":13,"strongs":0,"text":"unto them that go"},{"index":14,"strongs":"H03381","text":"down"},{"index":15,"strongs":0,"text":"into the"},{"index":16,"strongs":"H0953","text":"pit."}]},{"chapter":143,"text":"Cause me to hear thy lovingkindness in the morning; for in thee       do I trust: cause me to know the way wherein I should walk; for I       lift up my soul unto thee.","verse":8,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Cause me to "},{"index":1,"strongs":"H08085","text":"hear"},{"index":2,"strongs":0,"text":"thy"},{"index":3,"strongs":"H02617","text":"lovingkindness"},{"index":4,"strongs":0,"text":"in the"},{"index":5,"strongs":"H01242","text":"morning;"},{"index":6,"strongs":0,"text":"for in thee do I"},{"index":7,"strongs":"H0982","text":"trust:"},{"index":8,"strongs":0,"text":"cause me to"},{"index":9,"strongs":"H03045","text":"know"},{"index":10,"strongs":0,"text":"the"},{"index":11,"strongs":"H01870","text":"way"},{"index":12,"strongs":"H02098","text":"wherein"},{"index":13,"strongs":0,"text":"I should"},{"index":14,"strongs":"H03212","text":"walk;"},{"index":15,"strongs":0,"text":"for I lift"},{"index":16,"strongs":"H05375","text":"up"},{"index":17,"strongs":0,"text":"my"},{"index":18,"strongs":"H05315","text":"soul"},{"index":19,"strongs":0,"text":"unto thee."}]},{"chapter":143,"text":"Deliver me, O LORD, from mine enemies: I flee unto thee to hide       me.","verse":9,"book":"Psalms","reference":[{"index":0,"strongs":"H05337","text":"Deliver"},{"index":1,"strongs":0,"text":"me, O"},{"index":2,"strongs":"H03068","text":"LORD,"},{"index":3,"strongs":0,"text":"from mine"},{"index":4,"strongs":"H0341","text":"enemies:"},{"index":5,"strongs":0,"text":"I flee unto thee to"},{"index":6,"strongs":"H03680","text":"hide"},{"index":7,"strongs":0,"text":"me."}]},{"chapter":143,"text":"Teach me to do thy will; for thou [art] my God: thy spirit       [is] good; lead me into the land of uprightness.","verse":10,"book":"Psalms","reference":[{"index":0,"strongs":"H03925","text":"Teach"},{"index":1,"strongs":0,"text":"me to"},{"index":2,"strongs":"H06213","text":"do"},{"index":3,"strongs":0,"text":"thy"},{"index":4,"strongs":"H07522","text":"will;"},{"index":5,"strongs":0,"text":"for thou art my"},{"index":6,"strongs":"H0430","text":"God:"},{"index":7,"strongs":0,"text":"thy"},{"index":8,"strongs":"H07307","text":"spirit"},{"index":9,"strongs":0,"text":"is"},{"index":10,"strongs":"H02896","text":"good;"},{"