[{"chapter":90,"text":"LORD, thou hast been our dwelling place in all generations.","verse":1,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Lord, "},{"index":1,"strongs":"H08605","text":"thou"},{"index":2,"strongs":0,"text":"hast"},{"index":3,"strongs":"H04872","text":"been"},{"index":4,"strongs":0,"text":"our"},{"index":5,"strongs":"H0376","text":"dwelling"},{"index":6,"strongs":0,"text":"place"},{"index":7,"strongs":"H0430","text":"in"},{"index":8,"strongs":"H0136","text":"all"},{"index":9,"strongs":0,"text":"generations."}]},{"chapter":90,"text":"Before the mountains were brought forth, or ever thou hadst       formed the earth and the world, even from everlasting to       everlasting, thou [art] God.","verse":2,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Before the "},{"index":1,"strongs":"H02022","text":"mountains"},{"index":2,"strongs":0,"text":"were brought"},{"index":3,"strongs":"H03205","text":"forth,"},{"index":4,"strongs":0,"text":"or ever thou hadst"},{"index":5,"strongs":"H02342","text":"formed"},{"index":6,"strongs":0,"text":"the"},{"index":7,"strongs":"H0776","text":"earth"},{"index":8,"strongs":0,"text":"and the"},{"index":9,"strongs":"H08398","text":"world,"},{"index":10,"strongs":0,"text":"even from"},{"index":11,"strongs":"H05769","text":"everlasting"},{"index":12,"strongs":"H05704","text":"to"},{"index":13,"strongs":"H05769","text":"everlasting,"},{"index":14,"strongs":0,"text":"thou art"},{"index":15,"strongs":"H0410","text":"God."}]},{"chapter":90,"text":"Thou turnest man to destruction; and sayest, Return, ye children       of men.","verse":3,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Thou "},{"index":1,"strongs":"H07725","text":"turnest"},{"index":2,"strongs":"H0582","text":"man"},{"index":3,"strongs":0,"text":"to"},{"index":4,"strongs":"H01793","text":"destruction;"},{"index":5,"strongs":0,"text":"and"},{"index":6,"strongs":"H0559","text":"sayest,"},{"index":7,"strongs":"H07725","text":"Return,"},{"index":8,"strongs":0,"text":"ye"},{"index":9,"strongs":"H01121","text":"children"},{"index":10,"strongs":0,"text":"of"},{"index":11,"strongs":"H0120","text":"men."}]},{"chapter":90,"text":"For a thousand years in thy sight [are but] as yesterday when it       is past, and [as] a watch in the night.","verse":4,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"For a "},{"index":1,"strongs":"H0505","text":"thousand"},{"index":2,"strongs":"H08141","text":"years"},{"index":3,"strongs":0,"text":"in thy"},{"index":4,"strongs":"H05869","text":"sight"},{"index":5,"strongs":0,"text":"are but"},{"index":6,"strongs":"H03117","text":"as"},{"index":7,"strongs":"H0865","text":"yesterday"},{"index":8,"strongs":0,"text":"when it is"},{"index":9,"strongs":"H05674","text":"past,"},{"index":10,"strongs":0,"text":"and as a"},{"index":11,"strongs":"H0821","text":"watch"},{"index":12,"strongs":0,"text":"in the"},{"index":13,"strongs":"H03915","text":"night."}]},{"chapter":90,"text":"Thou carriest them away as with a flood; they are [as] a sleep:       in the morning [they are] like grass [which] groweth up.","verse":5,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Thou carriest them away as with a "},{"index":1,"strongs":"H02229","text":"flood;"},{"index":2,"strongs":0,"text":"they are as a"},{"index":3,"strongs":"H08142","text":"sleep:"},{"index":4,"strongs":0,"text":"in the"},{"index":5,"strongs":"H01242","text":"morning"},{"index":6,"strongs":0,"text":"they are like"},{"index":7,"strongs":"H02682","text":"grass"},{"index":8,"strongs":0,"text":"which groweth"},{"index":9,"strongs":"H02498","text":"up."}]},{"chapter":90,"text":"In the morning it flourisheth, and groweth up; in the evening it       is cut down, and withereth.","verse":6,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"In the "},{"index":1,"strongs":"H01242","text":"morning"},{"index":2,"strongs":0,"text":"it"},{"index":3,"strongs":"H06692","text":"flourisheth,"},{"index":4,"strongs":0,"text":"and groweth"},{"index":5,"strongs":"H02498","text":"up;"},{"index":6,"strongs":0,"text":"in the"},{"index":7,"strongs":"H06153","text":"evening"},{"index":8,"strongs":0,"text":"it is cut"},{"index":9,"strongs":"H04135","text":"down,"},{"index":10,"strongs":0,"text":"and"},{"index":11,"strongs":"H03001","text":"withereth."}]},{"chapter":90,"text":"For we are consumed by thine anger, and by thy wrath are we       troubled.","verse":7,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"For we are "},{"index":1,"strongs":"H03615","text":"consumed"},{"index":2,"strongs":0,"text":"by thine"},{"index":3,"strongs":"H0639","text":"anger,"},{"index":4,"strongs":0,"text":"and by thy"},{"index":5,"strongs":"H02534","text":"wrath"},{"index":6,"strongs":0,"text":"are we"},{"index":7,"strongs":"H0926","text":"troubled."}]},{"chapter":90,"text":"Thou hast set our iniquities before thee, our secret [sins] in       the light of thy countenance.","verse":8,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Thou hast "},{"index":1,"strongs":"H07896","text":"set"},{"index":2,"strongs":0,"text":"our"},{"index":3,"strongs":"H05771","text":"iniquities"},{"index":4,"strongs":0,"text":"before thee, our"},{"index":5,"strongs":"H05956","text":"secret"},{"index":6,"strongs":0,"text":"sins in the"},{"index":7,"strongs":"H03974","text":"light"},{"index":8,"strongs":0,"text":"of thy"},{"index":9,"strongs":"H06440","text":"countenance."}]},{"chapter":90,"text":"For all our days are passed away in thy wrath: we spend our       years as a tale [that is told.]","verse":9,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"For all our "},{"index":1,"strongs":"H03117","text":"days"},{"index":2,"strongs":0,"text":"are passed"},{"index":3,"strongs":"H06437","text":"away"},{"index":4,"strongs":0,"text":"in thy"},{"index":5,"strongs":"H05678","text":"wrath:"},{"index":6,"strongs":0,"text":"we"},{"index":7,"strongs":"H03615","text":"spend"},{"index":8,"strongs":0,"text":"our"},{"index":9,"strongs":"H08141","text":"years"},{"index":10,"strongs":0,"text":"as a"},{"index":11,"strongs":"H01899","text":"tale"},{"index":12,"strongs":0,"text":"that is told."}]},{"chapter":90,"text":"The days of our years [are] threescore years and ten; and if by       reason of strength [they be] fourscore years, yet [is] their       strength labour and sorrow; for it is soon cut off, and we fly       away.","verse":10,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H03117","text":"days"},{"index":2,"strongs":0,"text":"of our"},{"index":3,"strongs":"H08141","text":"years"},{"index":4,"strongs":0,"text":"are"},{"index":5,"strongs":"H07657","text":"threescore"},{"index":6,"strongs":"H08141","text":"years"},{"index":7,"strongs":0,"text":"and"},{"index":8,"strongs":"H07657","text":"ten;"},{"index":9,"strongs":0,"text":"and if by reason of"},{"index":10,"strongs":"H01369","text":"strength"},{"index":11,"strongs":0,"text":"they be"},{"index":12,"strongs":"H08084","text":"fourscore"},{"index":13,"strongs":"H08141","text":"years,"},{"index":14,"strongs":0,"text":"yet is their"},{"index":15,"strongs":"H07296","text":"strength"},{"index":16,"strongs":"H05999","text":"labour"},{"index":17,"strongs":0,"text":"and"},{"index":18,"strongs":"H0205","text":"sorrow;"},{"index":19,"strongs":0,"text":"for it is"},{"index":20,"strongs":"H02440","text":"soon"},{"index":21,"strongs":0,"text":"cut"},{"index":22,"strongs":"H01468","text":"off,"},{"index":23,"strongs":0,"text":"and we fly"},{"index":24,"strongs":"H05774","text":"away."}]},{"chapter":90,"text":"Who knoweth the power of thine anger? even according to thy       fear, [so is] thy wrath.","verse":11,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Who "},{"index":1,"strongs":"H03045","text":"knoweth"},{"index":2,"strongs":0,"text":"the"},{"index":3,"strongs":"H05797","text":"power"},{"index":4,"strongs":0,"text":"of thine"},{"index":5,"strongs":"H0639","text":"anger?"},{"index":6,"strongs":0,"text":"even according to thy"},{"index":7,"strongs":"H03374","text":"fear,"},{"index":8,"strongs":0,"text":"so is thy"},{"index":9,"strongs":"H05678","text":"wrath."}]},{"chapter":90,"text":"So teach [us] to number our days, that we may apply [our]       hearts unto wisdom.","verse":12,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"So "},{"index":1,"strongs":"H03045","text":"teach"},{"index":2,"strongs":0,"text":"us to"},{"index":3,"strongs":"H04487","text":"number"},{"index":4,"strongs":0,"text":"our"},{"index":5,"strongs":"H03117","text":"days,"},{"index":6,"strongs":0,"text":"that we may"},{"index":7,"strongs":"H0935","text":"apply"},{"index":8,"strongs":0,"text":"our"},{"index":9,"strongs":"H03824","text":"hearts"},{"index":10,"strongs":0,"text":"unto"},{"index":11,"strongs":"H02451","text":"wisdom."}]},{"chapter":90,"text":"Return, O LORD, how long? and let it repent thee concerning thy       servants.","verse":13,"book":"Psalms","reference":[{"index":0,"strongs":"H07725","text":"Return,"},{"index":1,"strongs":0,"text":"O"},{"index":2,"strongs":"H03068","text":"LORD,"},{"index":3,"strongs":0,"text":"how long? and let it"},{"index":4,"strongs":"H05162","text":"repent"},{"index":5,"strongs":0,"text":"thee concerning thy"},{"index":6,"strongs":"H05650","text":"servants."}]},{"chapter":90,"text":"O satisfy us early with thy mercy; that we may rejoice and be       glad all our days.","verse":14,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"O "},{"index":1,"strongs":"H07646","text":"satisfy"},{"index":2,"strongs":0,"text":"us"},{"index":3,"strongs":"H01242","text":"early"},{"index":4,"strongs":0,"text":"with thy"},{"index":5,"strongs":"H02617","text":"mercy;"},{"index":6,"strongs":0,"text":"that we may"},{"index":7,"strongs":"H07442","text":"rejoice"},{"index":8,"strongs":0,"text":"and be"},{"index":9,"strongs":"H08055","text":"glad"},{"index":10,"strongs":0,"text":"all our"},{"index":11,"strongs":"H03117","text":"days."}]},{"chapter":90,"text":"Make us glad according to the days [wherein] thou hast       afflicted us, [and] the years [wherein] we have seen evil.","verse":15,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Make us "},{"index":1,"strongs":"H08055","text":"glad"},{"index":2,"strongs":0,"text":"according to the"},{"index":3,"strongs":"H03117","text":"days"},{"index":4,"strongs":0,"text":"wherein thou hast"},{"index":5,"strongs":"H06031","text":"afflicted"},{"index":6,"strongs":0,"text":"us, and the"},{"index":7,"strongs":"H08141","text":"years"},{"index":8,"strongs":0,"text":"wherein we have"},{"index":9,"strongs":"H07200","text":"seen"},{"index":10,"strongs":"H07451","text":"evil."}]},{"chapter":90,"text":"Let thy work appear unto thy servants, and thy glory unto their       children.","verse":16,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Let thy "},{"index":1,"strongs":"H06467","text":"work"},{"index":2,"strongs":"H07200","text":"appear"},{"index":3,"strongs":0,"text":"unto thy"},{"index":4,"strongs":"H05650","text":"servants,"},{"index":5,"strongs":0,"text":"and thy"},{"index":6,"strongs":"H01926","text":"glory"},{"index":7,"strongs":0,"text":"unto their"},{"index":8,"strongs":"H01121","text":"children."}]},{"chapter":90,"text":"And let the beauty of the LORD our God be upon us: and       establish thou the work of our hands upon us; yea, the work of       our hands establish thou it.   Psalm 91","verse":17,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"And let the "},{"index":1,"strongs":"H05278","text":"beauty"},{"index":2,"strongs":0,"text":"of the"},{"index":3,"strongs":"H03068","text":"LORD"},{"index":4,"strongs":0,"text":"our"},{"index":5,"strongs":"H0430","text":"God"},{"index":6,"strongs":0,"text":"be upon us: and"},{"index":7,"strongs":"H03559","text":"establish"},{"index":8,"strongs":0,"text":"thou the"},{"index":9,"strongs":"H04639","text":"work"},{"index":10,"strongs":0,"text":"of our"},{"index":11,"strongs":"H03027","text":"hands"},{"index":12,"strongs":0,"text":"upon us; yea, the"},{"index":13,"strongs":"H04639","text":"work"},{"index":14,"strongs":0,"text":"of our"},{"index":15,"strongs":"H03027","text":"hands"},{"index":16,"strongs":"H03559","text":"establish"},{"index":17,"strongs":0,"text":"thou it."}]}]