[{"chapter":77,"text":"I cried unto God with my voice, [even] unto God with my       voice; and he gave ear unto me.","verse":1,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"I cried unto "},{"index":1,"strongs":"H05329","text":"God"},{"index":2,"strongs":0,"text":"with"},{"index":3,"strongs":"H03038","text":"my"},{"index":4,"strongs":0,"text":"voice,"},{"index":5,"strongs":"H04210","text":"even"},{"index":6,"strongs":0,"text":"unto"},{"index":7,"strongs":"H0623","text":"God"},{"index":8,"strongs":0,"text":"with"},{"index":9,"strongs":"H06817","text":"my"},{"index":10,"strongs":0,"text":"voice;"},{"index":11,"strongs":"H0430","text":"and"},{"index":12,"strongs":0,"text":"he gave"},{"index":13,"strongs":"H06963","text":"ear"},{"index":14,"strongs":0,"text":"unto me."}]},{"chapter":77,"text":"In the day of my trouble I sought the Lord: my sore ran in the       night, and ceased not: my soul refused to be comforted.","verse":2,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"In the "},{"index":1,"strongs":"H03117","text":"day"},{"index":2,"strongs":0,"text":"of my"},{"index":3,"strongs":"H06869","text":"trouble"},{"index":4,"strongs":0,"text":"I"},{"index":5,"strongs":"H01875","text":"sought"},{"index":6,"strongs":0,"text":"the"},{"index":7,"strongs":"H0136","text":"Lord:"},{"index":8,"strongs":0,"text":"my"},{"index":9,"strongs":"H03027","text":"sore"},{"index":10,"strongs":"H05064","text":"ran"},{"index":11,"strongs":0,"text":"in the"},{"index":12,"strongs":"H03915","text":"night,"},{"index":13,"strongs":0,"text":"and"},{"index":14,"strongs":"H06313","text":"ceased"},{"index":15,"strongs":0,"text":"not: my"},{"index":16,"strongs":"H05315","text":"soul"},{"index":17,"strongs":"H03985","text":"refused"},{"index":18,"strongs":0,"text":"to be"},{"index":19,"strongs":"H05162","text":"comforted."}]},{"chapter":77,"text":"I remembered God, and was troubled: I complained, and my spirit       was overwhelmed. Selah.","verse":3,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"I "},{"index":1,"strongs":"H02142","text":"remembered"},{"index":2,"strongs":"H0430","text":"God,"},{"index":3,"strongs":0,"text":"and was"},{"index":4,"strongs":"H01993","text":"troubled:"},{"index":5,"strongs":0,"text":"I"},{"index":6,"strongs":"H07878","text":"complained,"},{"index":7,"strongs":0,"text":"and my"},{"index":8,"strongs":"H07307","text":"spirit"},{"index":9,"strongs":0,"text":"was"},{"index":10,"strongs":"H05848","text":"overwhelmed."},{"index":11,"strongs":"H05542","text":"Selah."}]},{"chapter":77,"text":"Thou holdest mine eyes waking: I am so troubled that I cannot       speak.","verse":4,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Thou "},{"index":1,"strongs":"H0270","text":"holdest"},{"index":2,"strongs":0,"text":"mine"},{"index":3,"strongs":"H05869","text":"eyes"},{"index":4,"strongs":"H08109","text":"waking:"},{"index":5,"strongs":0,"text":"I am so"},{"index":6,"strongs":"H06470","text":"troubled"},{"index":7,"strongs":0,"text":"that I cannot"},{"index":8,"strongs":"H01696","text":"speak."}]},{"chapter":77,"text":"I have considered the days of old, the years of ancient times.","verse":5,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"I have "},{"index":1,"strongs":"H02803","text":"considered"},{"index":2,"strongs":0,"text":"the"},{"index":3,"strongs":"H03117","text":"days"},{"index":4,"strongs":0,"text":"of"},{"index":5,"strongs":"H06924","text":"old,"},{"index":6,"strongs":0,"text":"the"},{"index":7,"strongs":"H08141","text":"years"},{"index":8,"strongs":0,"text":"of ancient"},{"index":9,"strongs":"H05769","text":"times."}]},{"chapter":77,"text":"I call to remembrance my song in the night: I commune with mine       own heart: and my spirit made diligent search.","verse":6,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"I call to "},{"index":1,"strongs":"H02142","text":"remembrance"},{"index":2,"strongs":0,"text":"my"},{"index":3,"strongs":"H05058","text":"song"},{"index":4,"strongs":0,"text":"in the"},{"index":5,"strongs":"H03915","text":"night:"},{"index":6,"strongs":0,"text":"I"},{"index":7,"strongs":"H07878","text":"commune"},{"index":8,"strongs":0,"text":"with mine own"},{"index":9,"strongs":"H03824","text":"heart:"},{"index":10,"strongs":0,"text":"and my"},{"index":11,"strongs":"H07307","text":"spirit"},{"index":12,"strongs":0,"text":"made diligent"},{"index":13,"strongs":"H02664","text":"search."}]},{"chapter":77,"text":"Will the Lord cast off for ever? and will he be favourable no       more?","verse":7,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Will the "},{"index":1,"strongs":"H0136","text":"Lord"},{"index":2,"strongs":0,"text":"cast"},{"index":3,"strongs":"H02186","text":"off"},{"index":4,"strongs":0,"text":"for"},{"index":5,"strongs":"H05769","text":"ever?"},{"index":6,"strongs":0,"text":"and"},{"index":7,"strongs":"H03254","text":"will"},{"index":8,"strongs":0,"text":"he be"},{"index":9,"strongs":"H07521","text":"favourable"},{"index":10,"strongs":0,"text":"no more?"}]},{"chapter":77,"text":"Is his mercy clean gone for ever? doth [his] promise fail for       evermore?","verse":8,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Is his "},{"index":1,"strongs":"H02617","text":"mercy"},{"index":2,"strongs":0,"text":"clean"},{"index":3,"strongs":"H0656","text":"gone"},{"index":4,"strongs":0,"text":"for"},{"index":5,"strongs":"H05331","text":"ever?"},{"index":6,"strongs":0,"text":"doth his"},{"index":7,"strongs":"H0562","text":"promise"},{"index":8,"strongs":"H01584","text":"fail"},{"index":9,"strongs":0,"text":"for"},{"index":10,"strongs":"H01755","text":"evermore?"}]},{"chapter":77,"text":"Hath God forgotten to be gracious? hath he in anger shut up his       tender mercies? Selah.","verse":9,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Hath "},{"index":1,"strongs":"H0410","text":"God"},{"index":2,"strongs":"H07911","text":"forgotten"},{"index":3,"strongs":0,"text":"to be"},{"index":4,"strongs":"H02589","text":"gracious?"},{"index":5,"strongs":0,"text":"hath he in"},{"index":6,"strongs":"H0639","text":"anger"},{"index":7,"strongs":0,"text":"shut"},{"index":8,"strongs":"H07092","text":"up"},{"index":9,"strongs":0,"text":"his tender"},{"index":10,"strongs":"H07356","text":"mercies?"},{"index":11,"strongs":"H05542","text":"Selah."}]},{"chapter":77,"text":"And I said, This [is] my infirmity: [but I will remember] the       years of the right hand of the most High.","verse":10,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"And I "},{"index":1,"strongs":"H0559","text":"said,"},{"index":2,"strongs":0,"text":"This is my"},{"index":3,"strongs":"H02470","text":"infirmity:"},{"index":4,"strongs":0,"text":"but I will remember the"},{"index":5,"strongs":"H08141","text":"years"},{"index":6,"strongs":0,"text":"of the right"},{"index":7,"strongs":"H03225","text":"hand"},{"index":8,"strongs":0,"text":"of the most"},{"index":9,"strongs":"H05945","text":"High."}]},{"chapter":77,"text":"I will remember the works of the LORD: surely I will remember       thy wonders of old.","verse":11,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"I will "},{"index":1,"strongs":"H02142","text":"remember"},{"index":2,"strongs":0,"text":"the"},{"index":3,"strongs":"H04611","text":"works"},{"index":4,"strongs":0,"text":"of the"},{"index":5,"strongs":"H03050","text":"LORD:"},{"index":6,"strongs":0,"text":"surely I will"},{"index":7,"strongs":"H02142","text":"remember"},{"index":8,"strongs":0,"text":"thy"},{"index":9,"strongs":"H06382","text":"wonders"},{"index":10,"strongs":0,"text":"of"},{"index":11,"strongs":"H06924","text":"old."}]},{"chapter":77,"text":"I will meditate also of all thy work, and talk of thy doings.","verse":12,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"I will "},{"index":1,"strongs":"H01897","text":"meditate"},{"index":2,"strongs":0,"text":"also of all thy"},{"index":3,"strongs":"H06467","text":"work,"},{"index":4,"strongs":0,"text":"and"},{"index":5,"strongs":"H07878","text":"talk"},{"index":6,"strongs":0,"text":"of thy"},{"index":7,"strongs":"H05949","text":"doings."}]},{"chapter":77,"text":"Thy way, O God, [is] in the sanctuary: who [is so] great a God       as [our] God?","verse":13,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Thy "},{"index":1,"strongs":"H01870","text":"way,"},{"index":2,"strongs":0,"text":"O"},{"index":3,"strongs":"H0430","text":"God,"},{"index":4,"strongs":0,"text":"is in the"},{"index":5,"strongs":"H06944","text":"sanctuary:"},{"index":6,"strongs":0,"text":"who is so"},{"index":7,"strongs":"H01419","text":"great"},{"index":8,"strongs":0,"text":"a"},{"index":9,"strongs":"H0410","text":"God"},{"index":10,"strongs":0,"text":"as our"},{"index":11,"strongs":"H0430","text":"God?"}]},{"chapter":77,"text":"Thou [art] the God that doest wonders: thou hast declared thy       strength among the people.","verse":14,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Thou art the "},{"index":1,"strongs":"H0410","text":"God"},{"index":2,"strongs":0,"text":"that"},{"index":3,"strongs":"H06213","text":"doest"},{"index":4,"strongs":"H06382","text":"wonders:"},{"index":5,"strongs":0,"text":"thou hast"},{"index":6,"strongs":"H03045","text":"declared"},{"index":7,"strongs":0,"text":"thy"},{"index":8,"strongs":"H05797","text":"strength"},{"index":9,"strongs":0,"text":"among the"},{"index":10,"strongs":"H05971","text":"people."}]},{"chapter":77,"text":"Thou hast with [thine] arm redeemed thy people, the sons of       Jacob and Joseph. Selah.","verse":15,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Thou hast with thine "},{"index":1,"strongs":"H02220","text":"arm"},{"index":2,"strongs":"H01350","text":"redeemed"},{"index":3,"strongs":0,"text":"thy"},{"index":4,"strongs":"H05971","text":"people,"},{"index":5,"strongs":0,"text":"the"},{"index":6,"strongs":"H01121","text":"sons"},{"index":7,"strongs":0,"text":"of"},{"index":8,"strongs":"H03290","text":"Jacob"},{"index":9,"strongs":0,"text":"and"},{"index":10,"strongs":"H03130","text":"Joseph."},{"index":11,"strongs":"H05542","text":"Selah."}]},{"chapter":77,"text":"The waters saw thee, O God, the waters saw thee; they were       afraid: the depths also were troubled.","verse":16,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H04325","text":"waters"},{"index":2,"strongs":"H07200","text":"saw"},{"index":3,"strongs":0,"text":"thee, O"},{"index":4,"strongs":"H0430","text":"God,"},{"index":5,"strongs":0,"text":"the"},{"index":6,"strongs":"H04325","text":"waters"},{"index":7,"strongs":"H07200","text":"saw"},{"index":8,"strongs":0,"text":"thee; they were"},{"index":9,"strongs":"H02342","text":"afraid:"},{"index":10,"strongs":0,"text":"the"},{"index":11,"strongs":"H08415","text":"depths"},{"index":12,"strongs":0,"text":"also were"},{"index":13,"strongs":"H07264","text":"troubled."}]},{"chapter":77,"text":"The clouds poured out water: the skies sent out a sound: thine       arrows also went abroad.","verse":17,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H05645","text":"clouds"},{"index":2,"strongs":0,"text":"poured"},{"index":3,"strongs":"H02229","text":"out"},{"index":4,"strongs":"H04325","text":"water:"},{"index":5,"strongs":0,"text":"the"},{"index":6,"strongs":"H07834","text":"skies"},{"index":7,"strongs":0,"text":"sent"},{"index":8,"strongs":"H05414","text":"out"},{"index":9,"strongs":0,"text":"a"},{"index":10,"strongs":"H06963","text":"sound:"},{"index":11,"strongs":0,"text":"thine"},{"index":12,"strongs":"H02687","text":"arrows"},{"index":13,"strongs":0,"text":"also went"},{"index":14,"strongs":"H01980","text":"abroad."}]},{"chapter":77,"text":"The voice of thy thunder [was] in the heaven: the lightnings       lightened the world: the earth trembled and shook.","verse":18,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H06963","text":"voice"},{"index":2,"strongs":0,"text":"of thy"},{"index":3,"strongs":"H07482","text":"thunder"},{"index":4,"strongs":0,"text":"was in the"},{"index":5,"strongs":"H01534","text":"heaven:"},{"index":6,"strongs":0,"text":"the"},{"index":7,"strongs":"H01300","text":"lightnings"},{"index":8,"strongs":"H0215","text":"lightened"},{"index":9,"strongs":0,"text":"the"},{"index":10,"strongs":"H08398","text":"world:"},{"index":11,"strongs":0,"text":"the"},{"index":12,"strongs":"H0776","text":"earth"},{"index":13,"strongs":"H07264","text":"trembled"},{"index":14,"strongs":0,"text":"and"},{"index":15,"strongs":"H07493","text":"shook."}]},{"chapter":77,"text":"Thy way [is] in the sea, and thy path in the great waters, and       thy footsteps are not known.","verse":19,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Thy "},{"index":1,"strongs":"H01870","text":"way"},{"index":2,"strongs":0,"text":"is in the"},{"index":3,"strongs":"H03220","text":"sea,"},{"index":4,"strongs":0,"text":"and thy"},{"index":5,"strongs":"H07635","text":"path"},{"index":6,"strongs":0,"text":"in the"},{"index":7,"strongs":"H07227","text":"great"},{"index":8,"strongs":"H04325","text":"waters,"},{"index":9,"strongs":0,"text":"and thy"},{"index":10,"strongs":"H06119","text":"footsteps"},{"index":11,"strongs":0,"text":"are not"},{"index":12,"strongs":"H03045","text":"known."}]},{"chapter":77,"text":"Thou leddest thy people like a flock by the hand of Moses and       Aaron.   Psalm 78  Maschil of Asaph.","verse":20,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Thou "},{"index":1,"strongs":"H05148","text":"leddest"},{"index":2,"strongs":0,"text":"thy"},{"index":3,"strongs":"H05971","text":"people"},{"index":4,"strongs":0,"text":"like a"},{"index":5,"strongs":"H06629","text":"flock"},{"index":6,"strongs":0,"text":"by the"},{"index":7,"strongs":"H03027","text":"hand"},{"index":8,"strongs":0,"text":"of"},{"index":9,"strongs":"H04872","text":"Moses"},{"index":10,"strongs":0,"text":"and"},{"index":11,"strongs":"H0175","text":"Aaron."}]}]