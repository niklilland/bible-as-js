ext":"everlasting"},{"index":10,"strongs":"H01285","text":"covenant:"}]},{"chapter":105,"text":"Saying, Unto thee will I give the land of Canaan, the lot of       your inheritance:","verse":11,"book":"Psalms","reference":[{"index":0,"strongs":"H0559","text":"Saying,"},{"index":1,"strongs":0,"text":"Unto thee will I"},{"index":2,"strongs":"H05414","text":"give"},{"index":3,"strongs":0,"text":"the"},{"index":4,"strongs":"H0776","text":"land"},{"index":5,"strongs":0,"text":"of"},{"index":6,"strongs":"H03667","text":"Canaan,"},{"index":7,"strongs":0,"text":"the"},{"index":8,"strongs":"H02256","text":"lot"},{"index":9,"strongs":0,"text":"of your"},{"index":10,"strongs":"H05159","text":"inheritance:"}]},{"chapter":105,"text":"When they were [but] a few men in number; yea, very few, and       strangers in it.","verse":12,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"When they were but a few "},{"index":1,"strongs":"H04962","text":"men"},{"index":2,"strongs":0,"text":"in"},{"index":3,"strongs":"H04557","text":"number;"},{"index":4,"strongs":0,"text":"yea, very"},{"index":5,"strongs":"H04592","text":"few,"},{"index":6,"strongs":0,"text":"and"},{"index":7,"strongs":"H01481","text":"strangers"},{"index":8,"strongs":0,"text":"in it."}]},{"chapter":105,"text":"When they went from one nation to another, from [one] kingdom       to another people;","verse":13,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"When they "},{"index":1,"strongs":"H01980","text":"went"},{"index":2,"strongs":0,"text":"from one"},{"index":3,"strongs":"H01471","text":"nation"},{"index":4,"strongs":0,"text":"to another, from one"},{"index":5,"strongs":"H04467","text":"kingdom"},{"index":6,"strongs":0,"text":"to"},{"index":7,"strongs":"H0312","text":"another"},{"index":8,"strongs":"H05971","text":"people;"}]},{"chapter":105,"text":"He suffered no man to do them wrong: yea, he reproved kings       for their sakes;","verse":14,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He "},{"index":1,"strongs":"H03240","text":"suffered"},{"index":2,"strongs":0,"text":"no"},{"index":3,"strongs":"H0120","text":"man"},{"index":4,"strongs":0,"text":"to do them"},{"index":5,"strongs":"H06231","text":"wrong:"},{"index":6,"strongs":0,"text":"yea, he"},{"index":7,"strongs":"H03198","text":"reproved"},{"index":8,"strongs":"H04428","text":"kings"},{"index":9,"strongs":0,"text":"for their sakes;"}]},{"chapter":105,"text":"[Saying,] Touch not mine anointed, and do my prophets no harm.","verse":15,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Saying, "},{"index":1,"strongs":"H05060","text":"Touch"},{"index":2,"strongs":0,"text":"not mine"},{"index":3,"strongs":"H04899","text":"anointed,"},{"index":4,"strongs":0,"text":"and do my"},{"index":5,"strongs":"H05030","text":"prophets"},{"index":6,"strongs":0,"text":"no"},{"index":7,"strongs":"H07489","text":"harm."}]},{"chapter":105,"text":"Moreover he called for a famine upon the land: he brake the       whole staff of bread.","verse":16,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Moreover he "},{"index":1,"strongs":"H07121","text":"called"},{"index":2,"strongs":0,"text":"for a"},{"index":3,"strongs":"H07458","text":"famine"},{"index":4,"strongs":0,"text":"upon the"},{"index":5,"strongs":"H0776","text":"land:"},{"index":6,"strongs":0,"text":"he"},{"index":7,"strongs":"H07665","text":"brake"},{"index":8,"strongs":0,"text":"the whole"},{"index":9,"strongs":"H04294","text":"staff"},{"index":10,"strongs":0,"text":"of"},{"index":11,"strongs":"H03899","text":"bread."}]},{"chapter":105,"text":"He sent a man before them, [even] Joseph, [who] was sold for a       servant:","verse":17,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He "},{"index":1,"strongs":"H07971","text":"sent"},{"index":2,"strongs":0,"text":"a"},{"index":3,"strongs":"H0376","text":"man"},{"index":4,"strongs":"H06440","text":"before"},{"index":5,"strongs":0,"text":"them, even"},{"index":6,"strongs":"H03130","tex