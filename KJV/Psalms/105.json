":"H06605","text":"free."}]},{"chapter":105,"text":"He made him lord of his house, and ruler of all his substance:","verse":21,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He "},{"index":1,"strongs":"H07760","text":"made"},{"index":2,"strongs":0,"text":"him"},{"index":3,"strongs":"H0113","text":"lord"},{"index":4,"strongs":0,"text":"of his"},{"index":5,"strongs":"H01004","text":"house,"},{"index":6,"strongs":0,"text":"and"},{"index":7,"strongs":"H04910","text":"ruler"},{"index":8,"strongs":0,"text":"of all his"},{"index":9,"strongs":"H07075","text":"substance:"}]},{"chapter":105,"text":"To bind his princes at his pleasure; and teach his senators       wisdom.","verse":22,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"To "},{"index":1,"strongs":"H0631","text":"bind"},{"index":2,"strongs":0,"text":"his"},{"index":3,"strongs":"H08269","text":"princes"},{"index":4,"strongs":0,"text":"at his"},{"index":5,"strongs":"H05315","text":"pleasure;"},{"index":6,"strongs":0,"text":"and teach his"},{"index":7,"strongs":"H02205","text":"senators"},{"index":8,"strongs":"H02449","text":"wisdom."}]},{"chapter":105,"text":"Israel also came into Egypt; and Jacob sojourned in the land       of Ham.","verse":23,"book":"Psalms","reference":[{"index":0,"strongs":"H03478","text":"Israel"},{"index":1,"strongs":0,"text":"also"},{"index":2,"strongs":"H0935","text":"came"},{"index":3,"strongs":0,"text":"into"},{"index":4,"strongs":"H04714","text":"Egypt;"},{"index":5,"strongs":0,"text":"and"},{"index":6,"strongs":"H03290","text":"Jacob"},{"index":7,"strongs":"H01481","text":"sojourned"},{"index":8,"strongs":0,"text":"in the"},{"index":9,"strongs":"H0776","text":"land"},{"index":10,"strongs":0,"text":"of"},{"index":11,"strongs":"H02526","text":"Ham."}]},{"chapter":105,"text":"And he increased his people greatly; and made them stronger       than their enemies.","verse":24,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"And he "},{"index":1,"strongs":"H06509","text":"increased"},{"index":2,"strongs":0,"text":"his"},{"index":3,"strongs":"H05971","text":"people"},{"index":4,"strongs":"H03966","text":"greatly;"},{"index":5,"strongs":0,"text":"and made them"},{"index":6,"strongs":"H06105","text":"stronger"},{"index":7,"strongs":0,"text":"than their"},{"index":8,"strongs":"H06862","text":"enemies."}]},{"chapter":105,"text":"He turned their heart to hate his people, to deal subtilly       with his servants.","verse":25,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He "},{"index":1,"strongs":"H02015","text":"turned"},{"index":2,"strongs":0,"text":"their"},{"index":3,"strongs":"H03820","text":"heart"},{"index":4,"strongs":0,"text":"to"},{"index":5,"strongs":"H08130","text":"hate"},{"index":6,"strongs":0,"text":"his"},{"index":7,"strongs":"H05971","text":"people,"},{"index":8,"strongs":0,"text":"to deal"},{"index":9,"strongs":"H05230","text":"subtilly"},{"index":10,"strongs":0,"text":"with his"},{"index":11,"strongs":"H05650","text":"servants."}]},{"chapter":105,"text":"He sent Moses his servant; [and] Aaron whom he had chosen.","verse":26,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He "},{"index":1,"strongs":"H07971","text":"sent"},{"index":2,"strongs":"H04872","text":"Moses"},{"index":3,"strongs":0,"text":"his"},{"index":4,"strongs":"H05650","text":"servant;"},{"index":5,"strongs":0,"text":"and"},{"index":6,"strongs":"H0175","text":"Aaron"},{"index":7,"strongs":0,"text":"whom he had"},{"index":8,"strongs":"H0977","text":"chosen."}]},{"chapter":105,"text":"They shewed his signs among them, and wonders in the land of       Ham.","verse":27,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"They "},{"index":1,"strongs":"H07760","text":"shewed"},{"index":2,"strongs":0,"text":"his"},{"index":3,"strongs":"H01697","text":"signs"},{"index":4,"strongs":"H0226","text":"among"},{"index":5,"strongs":0,"text":"them, and"},{"index":6,"strongs":"H04159","text":"wonders"},{"index":7,"strongs":0,"text":"in the"},{"index":8,"strongs":"H0776","text":"land"},{"index":9,"strongs":0,"text":"of"},{"index":10,"strongs":"H02526","text":"Ham."}]},{"chapter":105,"text":"He sent darkness, and made it dark; and they rebelled not       against his word.","verse":28,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He "},{"index":1,"strongs":"H07971","text":"sent"},{"index":2,"strongs":"H02822","text":"darkness,"},{"index":3,"strongs":0,"text":"and made it"},{"index":4,"strongs":"H02821","text":"dark;"},{"index":5,"strongs":0,"text":"and they"},{"index":6,"strongs":"H04784","text":"rebelled"},{"index":7,"strongs":0,"text":"not against his"},{"index":8,"strongs":"H01697","text":"word."}]},{"chapter":105,"text":"He turned their waters into blood, and slew their fish.","verse":29,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He "},{"index":1,"strongs":"H02015","text":"turned"},{"index":2,"strongs":0,"text":"their"},{"index":3,"strongs":"H04325","text":"waters"},{"index":4,"strongs":0,"text":"into"},{"index":5,"strongs":"H01818","text":"blood,"},{"index":6,"strongs":0,"text":"and"},{"index":7,"strongs":"H04191","text":"slew"},{"index":8,"strongs":0,"text":"their"},{"index":9,"strongs":"H01710","text":"fish."}]},{"chapter":105,"text":"Their land brought forth frogs in abundance, in the chambers       of their kings.","verse":30,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Their "},{"index":1,"strongs":"H0776","text":"land"},{"index":2,"strongs":0,"text":"brought"},{"index":3,"strongs":"H08317","text":"forth"},{"index":4,"strongs":"H06854","text":"frogs"},{"index":5,"strongs":0,"text":"in"},{"index":6,"strongs":"H08317","text":"abundance,"},{"index":7,"strongs":0,"text":"in the"},{"index":8,"strongs":"H02315","text":"chambers"},{"index":9,"strongs":0,"text":"of their"},{"index":10,"strongs":"H04428","text":"kings."}]},{"chapter":105,"text":"He spake, and there came divers sorts of flies, [and] lice in       all their coasts.","verse":31,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He "},{"index":1,"strongs":"H0559","text":"spake,"},{"index":2,"strongs":0,"text":"and there"},{"index":3,"strongs":"H0935","text":"came"},{"index":4,"strongs":0,"text":"divers sorts of"},{"index":5,"strongs":"H06157","text":"flies,"},{"index":6,"strongs":0,"text":"and"},{"index":7,"strongs":"H03654","text":"lice"},{"index":8,"strongs":0,"text":"in all their"},{"index":9,"strongs":"H01366","text":"coasts."}]},{"chapter":105,"text":"He gave them hail for rain, [and] flaming fire in their land.","verse":32,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He "},{"index":1,"strongs":"H05414","text":"gave"},{"index":2,"strongs":0,"text":"them"},{"index":3,"strongs":"H01259","text":"hail"},{"index":4,"strongs":0,"text":"for"},{"index":5,"strongs":"H01653","text":"rain,"},{"index":6,"strongs":0,"text":"and"},{"index":7,"strongs":"H03852","text":"flaming"},{"index":8,"strongs":"H0784","text":"fire"},{"index":9,"strongs":0,"text":"in their"},{"index":10,"strongs":"H0776","text":"land."}]},{"chapter":105,"text":"He smote their vines also and their fig trees; and brake the       trees of their coasts.","verse":33,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He "},{"index":1,"strongs":"H05221","text":"smote"},{"index":2,"strongs":0,"text":"their"},{"index":3,"strongs":"H01612","text":"vines"},{"index":4,"strongs":0,"text":"also and their fig"},{"index":5,"strongs":"H08384","text":"trees;"},{"index":6,"strongs":0,"text":"and"},{"index":7,"strongs":"H07665","text":"brake"},{"index":8,"strongs":0,"text":"the"},{"index":9,"strongs":"H06086","text":"trees"},{"index":10,"strongs":0,"text":"of their"},{"index":11,"strongs":"H01366","text":"coasts."}]},{"chapter":105,"text":"He spake, and the locusts came, and caterpillers, and that       without number,","verse":34,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He "},{"index":1,"strongs":"H0559","text":"spake,"},{"index":2,"strongs":0,"text":"and