[{"chapter":111,"text":"Praise ye the LORD. I will praise the LORD with [my] whole       heart, in the assembly of the upright, and [in] the congregation.","verse":1,"book":"Psalms","reference":[{"index":0,"strongs":"H01984","text":"Praise"},{"index":1,"strongs":0,"text":"ye the"},{"index":2,"strongs":"H03050","text":"LORD."},{"index":3,"strongs":0,"text":"I will"},{"index":4,"strongs":"H03034","text":"praise"},{"index":5,"strongs":0,"text":"the"},{"index":6,"strongs":"H03068","text":"LORD"},{"index":7,"strongs":0,"text":"with my whole"},{"index":8,"strongs":"H03824","text":"heart,"},{"index":9,"strongs":0,"text":"in the"},{"index":10,"strongs":"H05475","text":"assembly"},{"index":11,"strongs":0,"text":"of the"},{"index":12,"strongs":"H03477","text":"upright,"},{"index":13,"strongs":0,"text":"and in the"},{"index":14,"strongs":"H05712","text":"congregation."}]},{"chapter":111,"text":"The works of the LORD [are] great, sought out of all them that       have pleasure therein.","verse":2,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H04639","text":"works"},{"index":2,"strongs":0,"text":"of the"},{"index":3,"strongs":"H03068","text":"LORD"},{"index":4,"strongs":0,"text":"are"},{"index":5,"strongs":"H01419","text":"great,"},{"index":6,"strongs":0,"text":"sought"},{"index":7,"strongs":"H01875","text":"out"},{"index":8,"strongs":0,"text":"of all them that have"},{"index":9,"strongs":"H02656","text":"pleasure"},{"index":10,"strongs":0,"text":"therein."}]},{"chapter":111,"text":"His work [is] honourable and glorious: and his righteousness       endureth for ever.","verse":3,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"His "},{"index":1,"strongs":"H06467","text":"work"},{"index":2,"strongs":0,"text":"is"},{"index":3,"strongs":"H01935","text":"honourable"},{"index":4,"strongs":0,"text":"and"},{"index":5,"strongs":"H01926","text":"glorious:"},{"index":6,"strongs":0,"text":"and his"},{"index":7,"strongs":"H06666","text":"righteousness"},{"index":8,"strongs":"H05975","text":"endureth"},{"index":9,"strongs":0,"text":"for"},{"index":10,"strongs":"H05703","text":"ever."}]},{"chapter":111,"text":"He hath made his wonderful works to be remembered: the LORD       [is] gracious and full of compassion.","verse":4,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He hath "},{"index":1,"strongs":"H06213","text":"made"},{"index":2,"strongs":0,"text":"his wonderful"},{"index":3,"strongs":"H06381","text":"works"},{"index":4,"strongs":0,"text":"to be"},{"index":5,"strongs":"H02143","text":"remembered:"},{"index":6,"strongs":0,"text":"the"},{"index":7,"strongs":"H03068","text":"LORD"},{"index":8,"strongs":0,"text":"is"},{"index":9,"strongs":"H02587","text":"gracious"},{"index":10,"strongs":0,"text":"and full of"},{"index":11,"strongs":"H07349","text":"compassion."}]},{"chapter":111,"text":"He hath given meat unto them that fear him: he will ever be       mindful of his covenant.","verse":5,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He hath "},{"index":1,"strongs":"H05414","text":"given"},{"index":2,"strongs":"H02964","text":"meat"},{"index":3,"strongs":0,"text":"unto them that"},{"index":4,"strongs":"H03373","text":"fear"},{"index":5,"strongs":0,"text":"him: he will"},{"index":6,"strongs":"H05769","text":"ever"},{"index":7,"strongs":0,"text":"be"},{"index":8,"strongs":"H02142","text":"mindful"},{"index":9,"strongs":0,"text":"of his"},{"index":10,"strongs":"H01285","text":"covenant."}]},{"chapter":111,"text":"He hath shewed his people the power of his works, that he may       give them the heritage of the heathen.","verse":6,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He hath "},{"index":1,"strongs":"H05046","text":"shewed"},{"index":2,"strongs":0,"text":"his"},{"index":3,"strongs":"H05971","text":"people"},{"index":4,"strongs":0,"text":"the"},{"index":5,"strongs":"H03581","text":"power"},{"index":6,"strongs":0,"text":"of his"},{"index":7,"strongs":"H04639","text":"works,"},{"index":8,"strongs":0,"text":"that he may"},{"index":9,"strongs":"H05414","text":"give"},{"index":10,"strongs":0,"text":"them the"},{"index":11,"strongs":"H05159","text":"heritage"},{"index":12,"strongs":0,"text":"of the"},{"index":13,"strongs":"H01471","text":"heathen."}]},{"chapter":111,"text":"The works of his hands [are] verity and judgment; all his       commandments [are] sure.","verse":7,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H04639","text":"works"},{"index":2,"strongs":0,"text":"of his"},{"index":3,"strongs":"H03027","text":"hands"},{"index":4,"strongs":0,"text":"are"},{"index":5,"strongs":"H0571","text":"verity"},{"index":6,"strongs":0,"text":"and"},{"index":7,"strongs":"H04941","text":"judgment;"},{"index":8,"strongs":0,"text":"all his"},{"index":9,"strongs":"H06490","text":"commandments"},{"index":10,"strongs":0,"text":"are"},{"index":11,"strongs":"H0539","text":"sure."}]},{"chapter":111,"text":"They stand fast for ever and ever, [and are] done in truth and       uprightness.","verse":8,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"They stand "},{"index":1,"strongs":"H05564","text":"fast"},{"index":2,"strongs":0,"text":"for"},{"index":3,"strongs":"H05703","text":"ever"},{"index":4,"strongs":0,"text":"and"},{"index":5,"strongs":"H05769","text":"ever,"},{"index":6,"strongs":0,"text":"and are"},{"index":7,"strongs":"H06213","text":"done"},{"index":8,"strongs":0,"text":"in"},{"index":9,"strongs":"H0571","text":"truth"},{"index":10,"strongs":0,"text":"and"},{"index":11,"strongs":"H03477","text":"uprightness."}]},{"chapter":111,"text":"He sent redemption unto his people: he hath commanded his       covenant for ever: holy and reverend [is] his name.","verse":9,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He "},{"index":1,"strongs":"H07971","text":"sent"},{"index":2,"strongs":"H06304","text":"redemption"},{"index":3,"strongs":0,"text":"unto his"},{"index":4,"strongs":"H05971","text":"people:"},{"index":5,"strongs":0,"text":"he hath"},{"index":6,"strongs":"H06680","text":"commanded"},{"index":7,"strongs":0,"text":"his"},{"index":8,"strongs":"H01285","text":"covenant"},{"index":9,"strongs":0,"text":"for"},{"index":10,"strongs":"H05769","text":"ever:"},{"index":11,"strongs":"H06918","text":"holy"},{"index":12,"strongs":0,"text":"and"},{"index":13,"strongs":"H03372","text":"reverend"},{"index":14,"strongs":0,"text":"is his"},{"index":15,"strongs":"H08034","text":"name."}]},{"chapter":111,"text":"The fear of the LORD [is] the beginning of wisdom: a good       understanding have all they that do [his commandments:] his       praise endureth for ever.   Psalm 112","verse":10,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H03374","text":"fear"},{"index":2,"strongs":0,"text":"of the"},{"index":3,"strongs":"H03068","text":"LORD"},{"index":4,"strongs":0,"text":"is the"},{"index":5,"strongs":"H07225","text":"beginning"},{"index":6,"strongs":0,"text":"of"},{"index":7,"strongs":"H02451","text":"wisdom:"},{"index":8,"strongs":0,"text":"a"},{"index":9,"strongs":"H02896","text":"good"},{"index":10,"strongs":"H07922","text":"understanding"},{"index":11,"strongs":0,"text":"have all they that"},{"index":12,"strongs":"H06213","text":"do"},{"index":13,"strongs":0,"text":"his commandments: his"},{"index":14,"strongs":"H08416","text":"praise"},{"index":15,"strongs":"H05975","text":"endureth"},{"index":16,"strongs":0,"text":"for"},{"index":17,"strongs":"H05703","text":"ever."}]}]