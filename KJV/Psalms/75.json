[{"chapter":75,"text":"Unto thee, O God, do we give thanks, [unto thee] do we give       thanks: for [that] thy name is near thy wondrous works declare.","verse":1,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Unto thee, O "},{"index":1,"strongs":"H05329","text":"God,"},{"index":2,"strongs":"H0516","text":"do"},{"index":3,"strongs":0,"text":"we"},{"index":4,"strongs":"H04210","text":"give"},{"index":5,"strongs":0,"text":"thanks,"},{"index":6,"strongs":"H07892","text":"unto"},{"index":7,"strongs":0,"text":"thee"},{"index":8,"strongs":"H0623","text":"do"},{"index":9,"strongs":0,"text":"we give thanks:"},{"index":10,"strongs":"H0430","text":"for"},{"index":11,"strongs":0,"text":"that thy name"},{"index":12,"strongs":"H03034","text":"is"},{"index":13,"strongs":0,"text":"near thy wondrous works declare."}]},{"chapter":75,"text":"When I shall receive the congregation I will judge uprightly.","verse":2,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"When I shall "},{"index":1,"strongs":"H03947","text":"receive"},{"index":2,"strongs":0,"text":"the"},{"index":3,"strongs":"H04150","text":"congregation"},{"index":4,"strongs":0,"text":"I will"},{"index":5,"strongs":"H08199","text":"judge"},{"index":6,"strongs":"H04339","text":"uprightly."}]},{"chapter":75,"text":"The earth and all the inhabitants thereof are dissolved: I bear       up the pillars of it. Selah.","verse":3,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H0776","text":"earth"},{"index":2,"strongs":0,"text":"and all the"},{"index":3,"strongs":"H03427","text":"inhabitants"},{"index":4,"strongs":0,"text":"thereof are"},{"index":5,"strongs":"H04127","text":"dissolved:"},{"index":6,"strongs":0,"text":"I bear"},{"index":7,"strongs":"H08505","text":"up"},{"index":8,"strongs":0,"text":"the"},{"index":9,"strongs":"H05982","text":"pillars"},{"index":10,"strongs":0,"text":"of it."},{"index":11,"strongs":"H05542","text":"Selah."}]},{"chapter":75,"text":"I said unto the fools, Deal not foolishly: and to the wicked,       Lift not up the horn:","verse":4,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"I "},{"index":1,"strongs":"H0559","text":"said"},{"index":2,"strongs":0,"text":"unto the"},{"index":3,"strongs":"H01984","text":"fools,"},{"index":4,"strongs":0,"text":"Deal not"},{"index":5,"strongs":"H01984","text":"foolishly:"},{"index":6,"strongs":0,"text":"and to the"},{"index":7,"strongs":"H07563","text":"wicked,"},{"index":8,"strongs":0,"text":"Lift not"},{"index":9,"strongs":"H07311","text":"up"},{"index":10,"strongs":0,"text":"the"},{"index":11,"strongs":"H07161","text":"horn:"}]},{"chapter":75,"text":"Lift not up your horn on high: speak [not with] a stiff neck.","verse":5,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Lift not "},{"index":1,"strongs":"H07311","text":"up"},{"index":2,"strongs":0,"text":"your"},{"index":3,"strongs":"H07161","text":"horn"},{"index":4,"strongs":0,"text":"on"},{"index":5,"strongs":"H04791","text":"high:"},{"index":6,"strongs":"H01696","text":"speak"},{"index":7,"strongs":0,"text":"not with a"},{"index":8,"strongs":"H06277","text":"stiff"},{"index":9,"strongs":"H06677","text":"neck."}]},{"chapter":75,"text":"For promotion [cometh] neither from the east, nor from the west,       nor from the south.","verse":6,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"For "},{"index":1,"strongs":"H07311","text":"promotion"},{"index":2,"strongs":0,"text":"cometh neither from the"},{"index":3,"strongs":"H04161","text":"east,"},{"index":4,"strongs":0,"text":"nor from the"},{"index":5,"strongs":"H04628","text":"west,"},{"index":6,"strongs":0,"text":"nor from the"},{"index":7,"strongs":"H04057","text":"south."}]},{"chapter":75,"text":"But God [is] the judge: he putteth down one, and setteth up       another.","verse":7,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"But "},{"index":1,"strongs":"H0430","text":"God"},{"index":2,"strongs":0,"text":"is the"},{"index":3,"strongs":"H08199","text":"judge:"},{"index":4,"strongs":0,"text":"he putteth"},{"index":5,"strongs":"H08213","text":"down"},{"index":6,"strongs":0,"text":"one, and setteth"},{"index":7,"strongs":"H07311","text":"up"},{"index":8,"strongs":0,"text":"another."}]},{"chapter":75,"text":"For in the hand of the LORD [there is] a cup, and the wine is       red; it is full of mixture; and he poureth out of the same: but       the dregs thereof, all the wicked of the earth shall wring [them]       out, [and] drink [them.]","verse":8,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"For in the "},{"index":1,"strongs":"H03027","text":"hand"},{"index":2,"strongs":0,"text":"of the"},{"index":3,"strongs":"H03068","text":"LORD"},{"index":4,"strongs":0,"text":"there is a"},{"index":5,"strongs":"H03563","text":"cup,"},{"index":6,"strongs":0,"text":"and the"},{"index":7,"strongs":"H03196","text":"wine"},{"index":8,"strongs":0,"text":"is"},{"index":9,"strongs":"H02560","text":"red;"},{"index":10,"strongs":0,"text":"it is"},{"index":11,"strongs":"H04392","text":"full"},{"index":12,"strongs":0,"text":"of"},{"index":13,"strongs":"H04538","text":"mixture;"},{"index":14,"strongs":0,"text":"and he poureth"},{"index":15,"strongs":"H05064","text":"out"},{"index":16,"strongs":0,"text":"of the"},{"index":17,"strongs":"H02088","text":"same:"},{"index":18,"strongs":0,"text":"but the"},{"index":19,"strongs":"H08105","text":"dregs"},{"index":20,"strongs":0,"text":"thereof, all the"},{"index":21,"strongs":"H07563","text":"wicked"},{"index":22,"strongs":0,"text":"of the"},{"index":23,"strongs":"H0776","text":"earth"},{"index":24,"strongs":0,"text":"shall wring them"},{"index":25,"strongs":"H04680","text":"out,"},{"index":26,"strongs":0,"text":"and"},{"index":27,"strongs":"H08354","text":"drink"},{"index":28,"strongs":0,"text":"them."}]},{"chapter":75,"text":"But I will declare for ever; I will sing praises to the God of       Jacob.","verse":9,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"But I will "},{"index":1,"strongs":"H05046","text":"declare"},{"index":2,"strongs":0,"text":"for"},{"index":3,"strongs":"H05769","text":"ever;"},{"index":4,"strongs":0,"text":"I will sing"},{"index":5,"strongs":"H02167","text":"praises"},{"index":6,"strongs":0,"text":"to the"},{"index":7,"strongs":"H0430","text":"God"},{"index":8,"strongs":0,"text":"of"},{"index":9,"strongs":"H03290","text":"Jacob."}]},{"chapter":75,"text":"All the horns of the wicked also will I cut off; [but] the       horns of the righteous shall be exalted.   Psalm 76  To the chief Musician on Neginoth, A Psalm [or] Song of Asaph.","verse":10,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"All the "},{"index":1,"strongs":"H07161","text":"horns"},{"index":2,"strongs":0,"text":"of the"},{"index":3,"strongs":"H07563","text":"wicked"},{"index":4,"strongs":0,"text":"also will I cut"},{"index":5,"strongs":"H01438","text":"off;"},{"index":6,"strongs":0,"text":"but the"},{"index":7,"strongs":"H07161","text":"horns"},{"index":8,"strongs":0,"text":"of the"},{"index":9,"strongs":"H06662","text":"righteous"},{"index":10,"strongs":0,"text":"shall be"},{"index":11,"strongs":"H07311","text":"exalted."}]}]