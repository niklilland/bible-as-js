[{"chapter":87,"text":"His foundation [is] in the holy mountains.","verse":1,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"His "},{"index":1,"strongs":"H04210","text":"foundation"},{"index":2,"strongs":0,"text":"is"},{"index":3,"strongs":"H07892","text":"in"},{"index":4,"strongs":0,"text":"the holy"},{"index":5,"strongs":"H01121","text":"mountains."}]},{"chapter":87,"text":"The LORD loveth the gates of Zion more than all the dwellings of       Jacob.","verse":2,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H03068","text":"LORD"},{"index":2,"strongs":"H0157","text":"loveth"},{"index":3,"strongs":0,"text":"the"},{"index":4,"strongs":"H08179","text":"gates"},{"index":5,"strongs":0,"text":"of"},{"index":6,"strongs":"H06726","text":"Zion"},{"index":7,"strongs":0,"text":"more than all the"},{"index":8,"strongs":"H04908","text":"dwellings"},{"index":9,"strongs":0,"text":"of"},{"index":10,"strongs":"H03290","text":"Jacob."}]},{"chapter":87,"text":"Glorious things are spoken of thee, O city of God. Selah.","verse":3,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Glorious "},{"index":1,"strongs":"H03513","text":"things"},{"index":2,"strongs":0,"text":"are"},{"index":3,"strongs":"H01696","text":"spoken"},{"index":4,"strongs":0,"text":"of thee, O"},{"index":5,"strongs":"H05892","text":"city"},{"index":6,"strongs":0,"text":"of"},{"index":7,"strongs":"H0430","text":"God."},{"index":8,"strongs":"H05542","text":"Selah."}]},{"chapter":87,"text":"I will make mention of Rahab and Babylon to them that know me:       behold Philistia, and Tyre, with Ethiopia; this [man] was born       there.","verse":4,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"I will make "},{"index":1,"strongs":"H02142","text":"mention"},{"index":2,"strongs":0,"text":"of"},{"index":3,"strongs":"H07294","text":"Rahab"},{"index":4,"strongs":0,"text":"and"},{"index":5,"strongs":"H0894","text":"Babylon"},{"index":6,"strongs":0,"text":"t