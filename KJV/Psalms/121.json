[{"chapter":121,"text":"I will lift up mine eyes unto the hills, from whence cometh       my help.","verse":1,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"I "},{"index":1,"strongs":"H07892","text":"will"},{"index":2,"strongs":0,"text":"lift"},{"index":3,"strongs":"H04609","text":"up"},{"index":4,"strongs":0,"text":"mine eyes unto"},{"index":5,"strongs":"H05375","text":"the"},{"index":6,"strongs":0,"text":"hills,"},{"index":7,"strongs":"H05869","text":"from"},{"index":8,"strongs":0,"text":"whence cometh"},{"index":9,"strongs":"H02022","text":"my"},{"index":10,"strongs":0,"text":"help."}]},{"chapter":121,"text":"My help [cometh] from the LORD, which made heaven and earth.","verse":2,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"My "},{"index":1,"strongs":"H05828","text":"help"},{"index":2,"strongs":0,"text":"cometh from the"},{"index":3,"strongs":"H03068","text":"LORD,"},{"index":4,"strongs":0,"text":"which"},{"index":5,"strongs":"H06213","text":"made"},{"index":6,"strongs":"H08064","text":"heaven"},{"index":7,"strongs":0,"text":"and"},{"index":8,"strongs":"H0776","text":"earth."}]},{"chapter":121,"text":"He will not suffer thy foot to be moved: he that keepeth thee       will not slumber.","verse":3,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"He will not "},{"index":1,"strongs":"H05414","text":"suffer"},{"index":2,"strongs":0,"text":"thy"},{"index":3,"strongs":"H07272","text":"foot"},{"index":4,"strongs":0,"text":"to be"},{"index":5,"strongs":"H04132","text":"moved:"},{"index":6,"strongs":0,"text":"he that"},{"index":7,"strongs":"H08104","text":"keepeth"},{"index":8,"strongs":0,"text":"thee will not"},{"index":9,"strongs":"H05123","text":"slumber."}]},{"chapter":121,"text":"Behold, he that keepeth Israel shall neither slumber nor sleep.","verse":4,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Behold, he that "},{"index":1,"strongs":"H08104","text":"keepeth"},{"index":2,"strongs":"H03478","text":"Israel"},{"index":3,"strongs":0,"text":"shall neither"},{"index":4,"strongs":"H05123","text":"slumber"},{"index":5,"strongs":0,"text":"nor"},{"index":6,"strongs":"H03462","text":"sleep."}]},{"chapter":121,"text":"The LORD [is] thy keeper: the LORD [is] thy shade upon thy       right hand.","verse":5,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H03068","text":"LORD"},{"index":2,"strongs":0,"text":"is thy"},{"index":3,"strongs":"H08104","text":"keeper:"},{"index":4,"strongs":0,"text":"the"},{"index":5,"strongs":"H03068","text":"LORD"},{"index":6,"strongs":0,"text":"is thy"},{"index":7,"strongs":"H06738","text":"shade"},{"index":8,"strongs":0,"text":"upon thy"},{"index":9,"strongs":"H03225","text":"right"},{"index":10,"strongs":"H03027","text":"hand."}]},{"chapter":121,"text":"The sun shall not smite thee by day, nor the moon by night.","verse":6,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H08121","text":"sun"},{"index":2,"strongs":0,"text":"shall not"},{"index":3,"strongs":"H05221","text":"smite"},{"index":4,"strongs":0,"text":"thee by"},{"index":5,"strongs":"H03119","text":"day,"},{"index":6,"strongs":0,"text":"nor the"},{"index":7,"strongs":"H03394","text":"moon"},{"index":8,"strongs":0,"text":"by"},{"index":9,"strongs":"H03915","text":"night."}]},{"chapter":121,"text":"The LORD shall preserve thee from all evil: he shall preserve       thy soul.","verse":7,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H03068","text":"LORD"},{"index":2,"strongs":0,"text":"shall"},{"index":3,"strongs":"H08104","text":"preserve"},{"index":4,"strongs":0,"text":"thee from all"},{"index":5,"strongs":"H07451","text":"evil:"},{"index":6,"strongs":0,"text":"he shall"},{"index":7,"strongs":"H08104","text":"preserve"},{"index":8,"strongs":0,"text":"thy"},{"index":9,"strongs":"H05315","text":"soul."}]},{"chapter":121,"text":"The LORD shall preserve thy going out and thy coming in from       this time forth, and even for evermore.   Psalm 122  A Song of degrees of David.","verse":8,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H03068","text":"LORD"},{"index":2,"strongs":0,"text":"shall"},{"index":3,"strongs":"H08104","text":"preserve"},{"index":4,"strongs":0,"text":"thy going"},{"index":5,"strongs":"H03318","text":"out"},{"index":6,"strongs":0,"text":"and thy coming"},{"index":7,"strongs":"H0935","text":"in"},{"index":8,"strongs":0,"text":"from this time forth, and even"},{"index":9,"strongs":"H05704","text":"for"},{"index":10,"strongs":"H05769","text":"evermore."}]}]