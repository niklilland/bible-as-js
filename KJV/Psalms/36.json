[{"chapter":36,"text":"The transgression of the wicked saith within my heart, [that       there is] no fear of God before his eyes.","verse":1,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The transgression of "},{"index":1,"strongs":"H05329","text":"the"},{"index":2,"strongs":0,"text":"wicked saith within"},{"index":3,"strongs":"H01732","text":"my"},{"index":4,"strongs":0,"text":"heart,"},{"index":5,"strongs":"H05650","text":"that"},{"index":6,"strongs":0,"text":"there is"},{"index":7,"strongs":"H03068","text":"no"},{"index":8,"strongs":0,"text":"fear"},{"index":9,"strongs":"H06588","text":"of"},{"index":10,"strongs":0,"text":"God before"},{"index":11,"strongs":"H07563","text":"his"},{"index":12,"strongs":"H05002","text":"eyes."}]},{"chapter":36,"text":"For he flattereth himself in his own eyes, until his iniquity be       found to be hateful.","verse":2,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"For he "},{"index":1,"strongs":"H02505","text":"flattereth"},{"index":2,"strongs":0,"text":"himself in his own"},{"index":3,"strongs":"H05869","text":"eyes,"},{"index":4,"strongs":0,"text":"until his"},{"index":5,"strongs":"H05771","text":"iniquity"},{"index":6,"strongs":0,"text":"be"},{"index":7,"strongs":"H04672","text":"found"},{"index":8,"strongs":0,"text":"to be"},{"index":9,"strongs":"H08130","text":"hateful."}]},{"chapter":36,"text":"The words of his mouth [are] iniquity and deceit: he hath left       off to be wise, [and] to do good.","verse":3,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"The "},{"index":1,"strongs":"H01697","text":"words"},{"index":2,"strongs":0,"text":"of his"},{"index":3,"strongs":"H06310","text":"mouth"},{"index":4,"strongs":0,"text":"are"},{"index":5,"strongs":"H0205","text":"iniquity"},{"index":6,"strongs":0,"text":"and"},{"index":7,"strongs":"H04820","text":"deceit:"},{"index":8,"strongs":0,"text":"he hath left"},{"index":9,"strongs":"H02308","text":"off"},{"index":10,"strongs":0,"text":"to be"},