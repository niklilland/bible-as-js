ntains;"},{"index":5,"strongs":0,"text":"thy"},{"index":6,"strongs":"H04941","text":"judgments"},{"index":7,"strongs":0,"text":"are a"},{"index":8,"strongs":"H07227","text":"great"},{"index":9,"strongs":"H08415","text":"deep:"},{"index":10,"strongs":0,"text":"O"},{"index":11,"strongs":"H03068","text":"LORD,"},{"index":12,"strongs":0,"text":"thou"},{"index":13,"strongs":"H03467","text":"preservest"},{"index":14,"strongs":"H0120","text":"man"},{"index":15,"strongs":0,"text":"and"},{"index":16,"strongs":"H0929","text":"beast."}]},{"chapter":36,"text":"How excellent [is] thy lovingkindness, O God! therefore the       children of men put their trust under the shadow of thy wings.","verse":7,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"How "},{"index":1,"strongs":"H03368","text":"excellent"},{"index":2,"strongs":0,"text":"is thy"},{"index":3,"strongs":"H02617","text":"lovingkindness,"},{"index":4,"strongs":0,"text":"O"},{"index":5,"strongs":"H0430","text":"God!"},{"index":6,"strongs":0,"text":"therefore the"},{"index":7,"strongs":"H01121","text":"children"},{"index":8,"strongs":0,"text":"of"},{"index":9,"strongs":"H0120","text":"men"},{"index":10,"strongs":0,"text":"put their"},{"index":11,"strongs":"H02620","text":"trust"},{"index":12,"strongs":0,"text":"under the"},{"index":13,"strongs":"H06738","text":"shadow"},{"index":14,"strongs":0,"text":"of thy"},{"index":15,"strongs":"H03671","text":"wings."}]},{"chapter":36,"text":"They shall be abundantly satisfied with the fatness of thy       house; and thou shalt make them drink of the river of thy       pleasures.","verse":8,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"They shall be abundantly "},{"index":1,"strongs":"H07301","text":"satisfied"},{"index":2,"strongs":0,"text":"with the"},{"index":3,"strongs":"H01880","text":"fatness"},{"index":4,"strongs":0,"text":"of thy"},{"index":5,"strongs":"H01004","text":"house;"},{"index":6,"strongs":0,"text":"and thou shalt make them"},{"index":7,"strong