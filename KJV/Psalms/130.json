[{"chapter":130,"text":"Out of the depths have I cried unto thee, O LORD.","verse":1,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Out "},{"index":1,"strongs":"H07892","text":"of"},{"index":2,"strongs":0,"text":"the"},{"index":3,"strongs":"H04609","text":"depths"},{"index":4,"strongs":0,"text":"have I cried"},{"index":5,"strongs":"H04615","text":"unto"},{"index":6,"strongs":0,"text":"thee, O"},{"index":7,"strongs":"H07121","text":"LORD."}]},{"chapter":130,"text":"Lord, hear my voice: let thine ears be attentive to the voice       of my supplications.","verse":2,"book":"Psalms","reference":[{"index":0,"strongs":"H0136","text":"Lord,"},{"index":1,"strongs":"H08085","text":"hear"},{"index":2,"strongs":0,"text":"my"},{"index":3,"strongs":"H06963","text":"voice:"},{"index":4,"strongs":0,"text":"let thine"},{"index":5,"strongs":"H0241","text":"ears"},{"index":6,"strongs":0,"text":"be"},{"index":7,"strongs":"H07183","text":"attentive"},{"index":8,"strongs":0,"text":"to the"},{"index":9,"strongs":"H06963","text":"voice"},{"index":10,"strongs":0,"text":"of my"},{"index":11,"strongs":"H08469","text":"supplications."}]},{"chapter":130,"text":"If thou, LORD, shouldest mark iniquities, O Lord, who shall       stand?","verse":3,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"If thou, "},{"index":1,"strongs":"H03050","text":"LORD,"},{"index":2,"strongs":0,"text":"shouldest"},{"index":3,"strongs":"H08104","text":"mark"},{"index":4,"strongs":"H05771","text":"iniquities,"},{"index":5,"strongs":0,"text":"O"},{"index":6,"strongs":"H0136","text":"Lord,"},{"index":7,"strongs":0,"text":"who shall"},{"index":8,"strongs":"H05975","text":"stand?"}]},{"chapter":130,"text":"But [there is] forgiveness with thee, that thou mayest be       feared.","verse":4,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"But there is "},{"index":1,"strongs":"H05547","text":"forgiveness"},{"index":2,"strongs":0,"text":"with thee, that thou mayest be"},{"index":3,"strongs":"H03372","text":"feared."}]},{"chapter":130,"text":"I wait for the LORD, my soul doth wait, and in his word do I       hope.","verse":5,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"I "},{"index":1,"strongs":"H06960","text":"wait"},{"index":2,"strongs":0,"text":"for the"},{"index":3,"strongs":"H03068","text":"LORD,"},{"index":4,"strongs":0,"text":"my"},{"index":5,"strongs":"H05315","text":"soul"},{"index":6,"strongs":0,"text":"doth"},{"index":7,"strongs":"H06960","text":"wait,"},{"index":8,"strongs":0,"text":"and in his"},{"index":9,"strongs":"H01697","text":"word"},{"index":10,"strongs":0,"text":"do I"},{"index":11,"strongs":"H03176","text":"hope."}]},{"chapter":130,"text":"My soul [waiteth] for the Lord more than they that watch for       the morning: [I say, more than] they that watch for the morning.","verse":6,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"My "},{"index":1,"strongs":"H05315","text":"soul"},{"index":2,"strongs":0,"text":"waiteth for the"},{"index":3,"strongs":"H0136","text":"Lord"},{"index":4,"strongs":0,"text":"more than they that"},{"index":5,"strongs":"H08104","text":"watch"},{"index":6,"strongs":0,"text":"for the"},{"index":7,"strongs":"H01242","text":"morning:"},{"index":8,"strongs":0,"text":"I say, more than they that"},{"index":9,"strongs":"H08104","text":"watch"},{"index":10,"strongs":0,"text":"for the"},{"index":11,"strongs":"H01242","text":"morning."}]},{"chapter":130,"text":"Let Israel hope in the LORD: for with the LORD [there is]       mercy, and with him [is] plenteous redemption.","verse":7,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Let "},{"index":1,"strongs":"H03478","text":"Israel"},{"index":2,"strongs":"H03176","text":"hope"},{"index":3,"strongs":0,"text":"in the"},{"index":4,"strongs":"H03068","text":"LORD:"},{"index":5,"strongs":0,"text":"for with the"},{"index":6,"strongs":"H03068","text":"LORD"},{"index":7,"strongs":0,"text":"there is"},{"index":8,"strongs":"H02617","text":"mercy,"},{"index":9,"strongs":0,"text":"and with him is"},{"index":10,"strongs":"H07235","text":"plenteous"},{"index":11,"strongs":"H06304","text":"redemption."}]},{"chapter":130,"text":"And he shall redeem Israel from all his iniquities.   Psalm 131  A Song of degrees of David.","verse":8,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"And he shall "},{"index":1,"strongs":"H06299","text":"redeem"},{"index":2,"strongs":"H03478","text":"Israel"},{"index":3,"strongs":0,"text":"from all his"},{"index":4,"strongs":"H05771","text":"iniquities."}]}]