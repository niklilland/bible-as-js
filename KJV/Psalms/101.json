[{"chapter":101,"text":"I will sing of mercy and judgment: unto thee, O LORD, will I       sing.","verse":1,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"I "},{"index":1,"strongs":"H04210","text":"will"},{"index":2,"strongs":0,"text":"sing"},{"index":3,"strongs":"H01732","text":"of"},{"index":4,"strongs":0,"text":"mercy and"},{"index":5,"strongs":"H07891","text":"judgment:"},{"index":6,"strongs":0,"text":"unto"},{"index":7,"strongs":"H02617","text":"thee,"},{"index":8,"strongs":0,"text":"O"},{"index":9,"strongs":"H04941","text":"LORD,"},{"index":10,"strongs":0,"text":"will I sing."}]},{"chapter":101,"text":"I will behave myself wisely in a perfect way. O when wilt thou       come unto me? I will walk within my house with a perfect heart.","verse":2,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"I will behave myself "},{"index":1,"strongs":"H07919","text":"wisely"},{"index":2,"strongs":0,"text":"in a"},{"index":3,"strongs":"H08549","text":"perfect"},{"index":4,"strongs":"H01870","text":"way."},{"index":5,"strongs":0,"text":"O when wilt thou"},{"index":6,"strongs":"H0935","text":"come"},{"index":7,"strongs":0,"text":"unto me? I will"},{"index":8,"strongs":"H01980","text":"walk"},{"index":9,"strongs":"H07130","text":"within"},{"index":10,"strongs":0,"text":"my"},{"index":11,"strongs":"H01004","text":"house"},{"index":12,"strongs":0,"text":"with a"},{"index":13,"strongs":"H08537","text":"perfect"},{"index":14,"strongs":"H03824","text":"heart."}]},{"chapter":101,"text":"I will set no wicked thing before mine eyes: I hate the work of       them that turn aside; [it] shall not cleave to me.","verse":3,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"I will "},{"index":1,"strongs":"H07896","text":"set"},{"index":2,"strongs":0,"text":"no"},{"index":3,"strongs":"H01100","text":"wicked"},{"index":4,"strongs":"H01697","text":"thing"},{"index":5,"strongs":0,"text":"before mine"},{"index":6,"strongs":"H05869","text":"eyes:"},{"index":7,"strongs":0,"text":"I"},{"index":8,"strongs":"H08130","text":"hate"},{"index":9,"strongs":0,"text":"the"},{"index":10,"strongs":"H06213","text":"work"},{"index":11,"strongs":0,"text":"of them that turn"},{"index":12,"strongs":"H07750","text":"aside;"},{"index":13,"strongs":0,"text":"it shall not"},{"index":14,"strongs":"H01692","text":"cleave"},{"index":15,"strongs":0,"text":"to me."}]},{"chapter":101,"text":"A froward heart shall depart from me: I will not know a wicked       [person.]","verse":4,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"A "},{"index":1,"strongs":"H06141","text":"froward"},{"index":2,"strongs":"H03824","text":"heart"},{"index":3,"strongs":0,"text":"shall"},{"index":4,"strongs":"H05493","text":"depart"},{"index":5,"strongs":0,"text":"from me: I will not"},{"index":6,"strongs":"H03045","text":"know"},{"index":7,"strongs":0,"text":"a"},{"index":8,"strongs":"H07451","text":"wicked"},{"index":9,"strongs":0,"text":"person."}]},{"chapter":101,"text":"Whoso privily slandereth his neighbour, him will I cut off: him       that hath an high look and a proud heart will not I suffer.","verse":5,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"Whoso "},{"index":1,"strongs":"H05643","text":"privily"},{"index":2,"strongs":"H03960","text":"slandereth"},{"index":3,"strongs":0,"text":"his"},{"index":4,"strongs":"H07453","text":"neighbour,"},{"index":5,"strongs":0,"text":"him will I cut"},{"index":6,"strongs":"H06789","text":"off:"},{"index":7,"strongs":0,"text":"him that hath an"},{"index":8,"strongs":"H01362","text":"high"},{"index":9,"strongs":"H05869","text":"look"},{"index":10,"strongs":0,"text":"and a"},{"index":11,"strongs":"H07342","text":"proud"},{"index":12,"strongs":"H03824","text":"heart"},{"index":13,"strongs":0,"text":"will not I"},{"index":14,"strongs":"H03201","text":"suffer."}]},{"chapter":101,"text":"Mine eyes [shall be] upon the faithful of the land, that they       may dwell with me: he that walketh in a perfect way, he shall