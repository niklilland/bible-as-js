[{"chapter":82,"text":"God standeth in the congregation of the mighty; he judgeth       among the gods.","verse":1,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"God "},{"index":1,"strongs":"H04210","text":"standeth"},{"index":2,"strongs":0,"text":"in"},{"index":3,"strongs":"H0623","text":"the"},{"index":4,"strongs":"H0430","text":"congregation"},{"index":5,"strongs":"H05324","text":"of"},{"index":6,"strongs":0,"text":"the mighty;"},{"index":7,"strongs":"H05712","text":"he"},{"index":8,"strongs":0,"text":"judgeth among"},{"index":9,"strongs":"H0410","text":"the"},{"index":10,"strongs":0,"text":"gods."}]},{"chapter":82,"text":"How long will ye judge unjustly, and accept the persons of the       wicked? Selah.","verse":2,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"How long will ye "},{"index":1,"strongs":"H08199","text":"judge"},{"index":2,"strongs":"H05766","text":"unjustly,"},{"index":3,"strongs":0,"text":"and"},{"index":4,"strongs":"H05375","text":"accept"},{"index":5,"strongs":0,"text":"the"},{"index":6,"strongs":"H06440","text":"persons"},{"index":7,"strongs":0,"text":"of the"},{"index":8,"strongs":"H07563","text":"wicked?"},{"index":9,"strongs":"H05542","text":"Selah."}]},{"chapter":82,"text":"Defend the poor and fatherless: do justice to the afflicted and       needy.","verse":3,"book":"Psalms","reference":[{"index":0,"strongs":"H08199","text":"Defend"},{"index":1,"strongs":0,"text":"the"},{"index":2,"strongs":"H01800","text":"poor"},{"index":3,"strongs":0,"text":"and"},{"index":4,"strongs":"H03490","text":"fatherless:"},{"index":5,"strongs":0,"text":"do"},{"index":6,"strongs":"H06663","text":"justice"},{"index":7,"strongs":0,"text":"to the"},{"index":8,"strongs":"H06041","text":"afflicted"},{"index":9,"strongs":0,"text":"and"},{"index":10,"strongs":"H07326","text":"needy."}]},{"chapter":82,"text":"Deliver the poor and needy: rid [them] out of the hand of the       wicked.","verse":4,"book":"Psalms","reference":[{"index":0,"strongs":"H06403","text":"Deliver"},{"index":1,"strongs":0,"text":"the"},{"index":2,"strongs":"H01800","text":"poor"},{"index":3,"strongs":0,"text":"and"},{"index":4,"strongs":"H0034","text":"needy:"},{"index":5,"strongs":"H05337","text":"rid"},{"index":6,"strongs":0,"text":"them out of the"},{"index":7,"strongs":"H03027","text":"hand"},{"index":8,"strongs":0,"text":"of the"},{"index":9,"strongs":"H07563","text":"wicked."}]},{"chapter":82,"text":"They know not, neither will they understand; they walk on in       darkness: all the foundations of the earth are out of course.","verse":5,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"They "},{"index":1,"strongs":"H03045","text":"know"},{"index":2,"strongs":0,"text":"not, neither will they"},{"index":3,"strongs":"H0995","text":"understand;"},{"index":4,"strongs":0,"text":"they"},{"index":5,"strongs":"H01980","text":"walk"},{"index":6,"strongs":0,"text":"on in"},{"index":7,"strongs":"H02825","text":"darkness:"},{"index":8,"strongs":0,"text":"all the"},{"index":9,"strongs":"H04144","text":"foundations"},{"index":10,"strongs":0,"text":"of the"},{"index":11,"strongs":"H0776","text":"earth"},{"index":12,"strongs":0,"text":"are out of"},{"index":13,"strongs":"H04131","text":"course."}]},{"chapter":82,"text":"I have said, Ye [are] gods; and all of you [are] children of the       most High.","verse":6,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"I have "},{"index":1,"strongs":"H0559","text":"said,"},{"index":2,"strongs":0,"text":"Ye are"},{"index":3,"strongs":"H0430","text":"gods;"},{"index":4,"strongs":0,"text":"and all of you are"},{"index":5,"strongs":"H01121","text":"children"},{"index":6,"strongs":0,"text":"of the most"},{"index":7,"strongs":"H05945","text":"High."}]},{"chapter":82,"text":"But ye shall die like men, and fall like one of the princes.","verse":7,"book":"Psalms","reference":[{"index":0,"strongs":"H0403","text":"But"},{"index":1,"strongs":0,"text":"ye shall"},{"index":2,"strongs":"H04191","text":"die"},{"index":3,"strongs":0,"text":"like"},{"index":4,"strongs":"H0120","text":"men,"},{"index":5,"strongs":0,"text":"and"},{"index":6,"strongs":"H05307","text":"fall"},{"index":7,"strongs":0,"text":"like"},{"index":8,"strongs":"H0259","text":"one"},{"index":9,"strongs":0,"text":"of the"},{"index":10,"strongs":"H08269","text":"princes."}]},{"chapter":82,"text":"Arise, O God, judge the earth: for thou shalt inherit all       nations.   Psalm 83  A Song [or] Psalm of Asaph.","verse":8,"book":"Psalms","reference":[{"index":0,"strongs":"H06965","text":"Arise,"},{"index":1,"strongs":0,"text":"O"},{"index":2,"strongs":"H0430","text":"God,"},{"index":3,"strongs":"H08199","text":"judge"},{"index":4,"strongs":0,"text":"the"},{"index":5,"strongs":"H0776","text":"earth:"},{"index":6,"strongs":0,"text":"for thou shalt"},{"index":7,"strongs":"H05157","text":"inherit"},{"index":8,"strongs":0,"text":"all"},{"index":9,"strongs":"H01471","text":"nations."}]}]