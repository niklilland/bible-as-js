uld not go"},{"index":11,"strongs":"H03381","text":"down"},{"index":12,"strongs":0,"text":"to the"},{"index":13,"strongs":"H0953","text":"pit."}]},{"chapter":30,"text":"Sing unto the LORD, O ye saints of his, and give thanks at the       remembrance of his holiness.","verse":4,"book":"Psalms","reference":[{"index":0,"strongs":"H02167","text":"Sing"},{"index":1,"strongs":0,"text":"unto the"},{"index":2,"strongs":"H03068","text":"LORD,"},{"index":3,"strongs":0,"text":"O ye"},{"index":4,"strongs":"H02623","text":"saints"},{"index":5,"strongs":0,"text":"of his, and give"},{"index":6,"strongs":"H03034","text":"thanks"},{"index":7,"strongs":0,"text":"at the"},{"index":8,"strongs":"H02143","text":"remembrance"},{"index":9,"strongs":0,"text":"of his"},{"index":10,"strongs":"H06944","text":"holiness."}]},{"chapter":30,"text":"For his anger [endureth but] a moment; in his favour is life:       weeping may endure for a night, but joy [cometh] in the morning.","verse":5,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"For his "},{"index":1,"strongs":"H0639","text":"anger"},{"index":2,"strongs":0,"text":"endureth but a"},{"index":3,"strongs":"H07281","text":"moment;"},{"index":4,"strongs":0,"text":"in his"},{"index":5,"strongs":"H07522","text":"favour"},{"index":6,"strongs":0,"text":"is"},{"index":7,"strongs":"H02416","text":"life:"},{"index":8,"strongs":"H01065","text":"weeping"},{"index":9,"strongs":0,"text":"may"},{"index":10,"strongs":"H03885","text":"endure"},{"index":11,"strongs":0,"text":"for a"},{"index":12,"strongs":"H06153","text":"night,"},{"index":13,"strongs":0,"text":"but"},{"index":14,"strongs":"H07440","text":"joy"},{"index":15,"strongs":0,"text":"cometh in the"},{"index":16,"strongs":"H01242","text":"morning."}]},{"chapter":30,"text":"And in my prosperity I said, I shall never be moved.","verse":6,"book":"Psalms","reference":[{"index":0,"strongs":0,"text":"And in my "},{"index":1,"strongs":"H07959","text":"prosperity"},{"index":2,"strongs":0,"text":"I